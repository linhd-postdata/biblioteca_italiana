[
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "1",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Queste a' bei nodi biondi,"
                },
                {
                    "verse": "Queste al cresp'oro fin della mia Fille"
                },
                {
                    "verse": "Io tesso erbette, e frondi,"
                },
                {
                    "verse": "E violette, e fior di color mille."
                },
                {
                    "verse": "Altri 'l foco, e' bei raggi, e le faville"
                },
                {
                    "verse": "Spiegher\u00e0 de' begli occhi pi\u00f9 che umani;"
                },
                {
                    "verse": "Altri le bianche mani,"
                },
                {
                    "verse": "O quei detti soavi,"
                },
                {
                    "verse": "Altrettante d'Amor catene, e chiavi."
                }
            ]
        ]
    },
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "2",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Dir\u00f2 del vago pi\u00e8, che l'erba infiora?"
                },
                {
                    "verse": "O della bianca mano,"
                },
                {
                    "verse": "Che gli arboscelli imperla, inostra, e 'ndora?"
                },
                {
                    "verse": "O del bel viso umano?"
                },
                {
                    "verse": "O del soave sguardo umile, e piano?"
                },
                {
                    "verse": "O delle dolci angeliche parole?"
                },
                {
                    "verse": "Questi l'Alba ne reca; e quegli il Sole;"
                },
                {
                    "verse": "Quelle gli eterni o Ciel concenti tuoi:"
                },
                {
                    "verse": "Ma quanto \u00e8 'l me' tacer, ch'io non l'annoi."
                }
            ]
        ]
    },
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "3",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Nuova io non so, se stella,"
                },
                {
                    "verse": "O Sol dal Cielo in terra"
                },
                {
                    "verse": "in forma di selvaggia Pastorella"
                },
                {
                    "verse": "Scese per farmi guerra;"
                },
                {
                    "verse": "E 'l dardo, e la facella"
                },
                {
                    "verse": "Fu 'l dolce riso, e 'l bel guardo soave:"
                },
                {
                    "verse": "Altre costei non have"
                },
                {
                    "verse": "Armi che queste; altr'io"
                },
                {
                    "verse": "Scudo non ho che 'l duolo, e 'l pianger mio."
                }
            ]
        ]
    },
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "4",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Vaga Angeletta d'amoroso grembo"
                },
                {
                    "verse": "Mosse, e scorse quaggi\u00f9 dal terzo coro"
                },
                {
                    "verse": "Di stella in guisa, che per l'aer vole:"
                },
                {
                    "verse": "Era il capo seren lucido nembo"
                },
                {
                    "verse": "Di foco, e fiamma, non pur d'ambra, o d'oro,"
                },
                {
                    "verse": "E duo' folgori gli occhi, e 'l viso un Sole:"
                },
                {
                    "verse": "Tal fui cenere io tosto: hor come, hor quando"
                },
                {
                    "verse": "Al Ciel n'and\u00f2 s\u00ec lieve alma volando?"
                }
            ]
        ]
    },
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "5",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Angeletta gentil cinta di mirto"
                },
                {
                    "verse": "Stese l'arabe piume, e quasi un vivo"
                },
                {
                    "verse": "Sol dal chiaro balcone in terra scese:"
                },
                {
                    "verse": "Fiammeggiava il crin vago ondoso, ed irto."
                },
                {
                    "verse": "Ardean le guance, folgorava il divo"
                },
                {
                    "verse": "Alto splendor delle sue luci accese:"
                },
                {
                    "verse": "Allor subito io corsi, e 'n Paradiso"
                },
                {
                    "verse": "Immantenente mi rividi assiso."
                }
            ]
        ]
    },
    {
        "author": "Giovan Battista, il Giovane Strozzi",
        "work": "Madrigali",
        "title": "6",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000672",
        "text": [
            [
                {
                    "verse": "Cara Angeletta s\u00f9 del terzo coro"
                },
                {
                    "verse": "Scese dal Cielo; e quasi un vivo Sole"
                },
                {
                    "verse": "Dolce s'assise a pi\u00e8 d'un verde alloro:"
                },
                {
                    "verse": "Era latte il bel sen; rose, e viole"
                },
                {
                    "verse": "Il volto; e gli occhi stelle; e' suoi crini oro;"
                },
                {
                    "verse": "E nettar le dolcissime parole:"
                },
                {
                    "verse": "Ond'io subito corsi; e pi\u00f9 non oso"
                },
                {
                    "verse": "Altrui ridir del mio stato gioioso."
                }
            ]
        ]
    }
]