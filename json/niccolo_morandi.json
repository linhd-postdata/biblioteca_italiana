[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000635",
        "author": "Niccolò Morandi",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quando più ghiaccio o più neve rinfresca"
                },
                {
                    "verse": "corso di stella, più scaldar mi fai,"
                },
                {
                    "verse": "Amor, che triegua a tutti gli altri dai,"
                },
                {
                    "verse": "meco ogni forza, ché di te non n'esca."
                }
            ],
            [
                {
                    "verse": "La carta di mie mano er'ancor fresca:"
                },
                {
                    "verse": "sempre fuggire e libertà giurai,"
                },
                {
                    "verse": "quando mi porse bellezze non mai"
                },
                {
                    "verse": "vedute al mondo, ond'io arsi com'esca."
                }
            ],
            [
                {
                    "verse": "Come agli atti celesti gli occhi porsi,"
                },
                {
                    "verse": "vidi più bel che 'l sole e di più stima"
                },
                {
                    "verse": "colei che lacci avea sotto le chiome."
                }
            ],
            [
                {
                    "verse": "Di dicembre il dì quinto all'ora prima"
                },
                {
                    "verse": "prigionier fui d'Amor, per ch'io trascorsi"
                },
                {
                    "verse": "nelle suo doppie ragne, e non so come."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000635",
        "author": "Niccolò Morandi",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Come amoroso augel, che, fuor di gente,"
                },
                {
                    "verse": "fronde coperte e solitar boschi ami,"
                },
                {
                    "verse": "onde compagno alcuna volta brami,"
                },
                {
                    "verse": "di cui s'allegra riveder sovente,"
                }
            ],
            [
                {
                    "verse": "la pasturella semplice, che 'l sente,"
                },
                {
                    "verse": "batte le fronde e par che questo chiami,"
                },
                {
                    "verse": "onde si parte dagli ombrosi rami,"
                },
                {
                    "verse": "po' vi ritorna paventosamente,"
                }
            ],
            [
                {
                    "verse": "così, lasso, torn'io tal ora appresso"
                },
                {
                    "verse": "dov'io fu' preso, e con Amor ragiono"
                },
                {
                    "verse": "del mie signor, che giorno e notte scrivo."
                }
            ],
            [
                {
                    "verse": "Amanti, il dico a voi ed a me stesso:"
                },
                {
                    "verse": "dal dì ch'i' scorsi insino a oggi sono"
                },
                {
                    "verse": "stato in fra due sì: «Sono o morto o vivo?»"
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000635",
        "author": "Niccolò Morandi",
        "collection": "Poesie",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Dolce mie lira, ov'è colei che spesso"
                },
                {
                    "verse": "nelle suo man essercitar si suole,"
                },
                {
                    "verse": "dove risplende il nostro vivo sole?"
                },
                {
                    "verse": "Forse che 'l cielo il vuol con seco apresso?"
                }
            ],
            [
                {
                    "verse": "O vedovetta camera sanz'esso"
                },
                {
                    "verse": "el non vederlo a me più ch'altri duole"
                },
                {
                    "verse": "e 'l non udir l'angeliche parole;"
                },
                {
                    "verse": "di voi m'incresce come di me stesso!"
                }
            ],
            [
                {
                    "verse": "O voi, a cui può dirsi ogni segreto,"
                },
                {
                    "verse": "quel che mi cinse di catena il collo"
                },
                {
                    "verse": "fu 'l suono e 'l canto più oltre ch'umano!"
                }
            ],
            [
                {
                    "verse": "Tornando Donatello e Pulicreto"
                },
                {
                    "verse": "per ritrar questa con la cetra in mano,"
                },
                {
                    "verse": "nissun certo somiglia altro che Apollo."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000635",
        "author": "Niccolò Morandi",
        "collection": "Poesie",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quell'aspetto real ch'è mio signore,"
                },
                {
                    "verse": "sanz'esso il cor come sanz'alma rugge,"
                },
                {
                    "verse": "e quanto si discosta e quanto fugge"
                },
                {
                    "verse": "più studio il passo e 'l corpo ha men valore."
                }
            ],
            [
                {
                    "verse": "Così m'avien che, quanto 'l suo splendore"
                },
                {
                    "verse": "più s'allontana, più mi scalda e strugge,"
                },
                {
                    "verse": "e non mi val di mura o di frond'ugge"
                },
                {
                    "verse": "che, dov'io vada, non mi segui Amore."
                }
            ],
            [
                {
                    "verse": "Così le luce e le bellezze vaghe"
                },
                {
                    "verse": "vo seguitando, ed or manco tra via,"
                },
                {
                    "verse": "ma doppo morte una grazia disiro:"
                }
            ],
            [
                {
                    "verse": "che 'l cor portiate alla nimica mia,"
                },
                {
                    "verse": "ché mai le 'ncrebbe proveder mie piaghe"
                },
                {
                    "verse": "d'una lagrima sola o d'un sospiro."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000635",
        "author": "Niccolò Morandi",
        "collection": "Poesie",
        "title": "V",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Fili portati intorno al collo d'oro"
                },
                {
                    "verse": "al cor penoso una catena fanno;"
                },
                {
                    "verse": "e l'arme tutte raddoppiate m'hanno"
                },
                {
                    "verse": "Amore e 'l mio signor, ch'i' tanto onoro."
                }
            ],
            [
                {
                    "verse": "Non mi togli' ombra di faggio o d'alloro"
                },
                {
                    "verse": "l'usato stil nel destinato affanno;"
                },
                {
                    "verse": "le piagge, i boschi e le fontane il sanno"
                },
                {
                    "verse": "s'io mi trasformo in compagnia di loro."
                }
            ],
            [
                {
                    "verse": "Però, se Morte non divide o rompe"
                },
                {
                    "verse": "il bel pensier ch'ogni dolcezza ingombra"
                },
                {
                    "verse": "ne' lunghi afflitti mie sospiri e danni,"
                }
            ],
            [
                {
                    "verse": "se vive in terra o dove vuol facci ombra,"
                },
                {
                    "verse": "tutte mie pene chiamo glorie e pompe,"
                },
                {
                    "verse": "s'e' quant' el sole ed io quanto vuol anni."
                }
            ]
        ]
    }
]