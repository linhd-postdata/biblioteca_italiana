[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "1",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Amor, lunga stagion ebbe ardimento"
                },
                {
                    "verse": "ogni vista sprezzar,ch'agli occhi piace,"
                },
                {
                    "verse": "che ne sostiene in debile e fallace"
                },
                {
                    "verse": "sperar, ch'ogni piacer volge in tormento;"
                }
            ],
            [
                {
                    "verse": "e spiegar le mie vele a miglior vento,"
                },
                {
                    "verse": "per giugner a quel ben, che mai non spiace;"
                },
                {
                    "verse": "ma tu, che 'nvidia avesti di mia pace,"
                },
                {
                    "verse": "l'aura e l'ardir spengesti in un momento."
                }
            ],
            [
                {
                    "verse": "Nova beltà, dolcissime parole"
                },
                {
                    "verse": "mi piacquer tanto, che lo strale e 'l laccio"
                },
                {
                    "verse": "mi ferì insieme, e annodomm' il core."
                }
            ],
            [
                {
                    "verse": "Così caddi a la rete: e cos' il sole"
                },
                {
                    "verse": "de' begli occhi disfece il duro ghiaccio,"
                },
                {
                    "verse": "che chiudea 'l varco a ogni terreno ardore."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "2",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Bella guerriera mia, s'un solo sguardo"
                },
                {
                    "verse": "mi fe' cader a l'amorosa rete;"
                },
                {
                    "verse": "e se le note sì soavi e quete,"
                },
                {
                    "verse": "l'esca furo del foco, ond'io tutt'ardo;"
                }
            ],
            [
                {
                    "verse": "s'al ben lento, e veloce più che pardo"
                },
                {
                    "verse": "al mio mal corsi, ond'ho poche ore liete;"
                },
                {
                    "verse": "e se stato provar senza quiete"
                },
                {
                    "verse": "volli, quand'io fui segno a l'altrui dardo,"
                }
            ],
            [
                {
                    "verse": "vinca 'l cor vostro omai di me pietate:"
                },
                {
                    "verse": "E poiché Dio dispose e dielmi in fato,"
                },
                {
                    "verse": "ch'io v'amassi con salda e pura fede,"
                }
            ],
            [
                {
                    "verse": "non mi mostrate il viso almen turbato:"
                },
                {
                    "verse": "che solo il contemplar tanta beltate,"
                },
                {
                    "verse": "mi sarà d'ogni affanno ampia mercede."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "3",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Chi vuol veder de l'altre belle il fiore"
                },
                {
                    "verse": "sparso, tutto raccolto in un soggetto,"
                },
                {
                    "verse": "miri costei, dal cui gentile aspetto"
                },
                {
                    "verse": "piove grazia, piacer, senno e valore."
                }
            ],
            [
                {
                    "verse": "Miri i begli occhi, onde sovente Amore"
                },
                {
                    "verse": "move lo strale, che m'impiaga il petto;"
                },
                {
                    "verse": "la fronte piena di cortese affetto,"
                },
                {
                    "verse": "e le man preste ad involarmi il core."
                }
            ],
            [
                {
                    "verse": "Miri l'andar soavemente altero,"
                },
                {
                    "verse": "oda 'l parlar alteramente umile,"
                },
                {
                    "verse": "e 'l cantar, ch' armonia celeste spira."
                }
            ],
            [
                {
                    "verse": "E poi ch'averà ben estimato il vero,"
                },
                {
                    "verse": "- Questa, dica fra sé, vince ogni stile:"
                },
                {
                    "verse": "beato adunque chi per lei sospira -."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "4",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Crespe chiome d'or fin, che sparse al vento"
                },
                {
                    "verse": "fanno ondeggiando al sole invidia e scorno;"
                },
                {
                    "verse": "occhi leggiadri e più chiari che 'l giorno,"
                },
                {
                    "verse": "da far de la lor vista ogni uom contento;"
                }
            ],
            [
                {
                    "verse": "riso che 'n ver gioir cangia 'l tormento,"
                },
                {
                    "verse": "d'accender Giove bel sembiante adorno;"
                },
                {
                    "verse": "cantar che 'l cielo rasserena intorno,"
                },
                {
                    "verse": "e fa in vita tornar chi fosse spento;"
                }
            ],
            [
                {
                    "verse": "uscio d'avorio, ond'escono parole"
                },
                {
                    "verse": "piene d'alto conforto e di salute;"
                },
                {
                    "verse": "atti vaghi, non visti unqua fra noi,"
                }
            ],
            [
                {
                    "verse": "giunta a rara beltà rara virtute:"
                },
                {
                    "verse": "son vostri pregi, e fan donna per voi"
                },
                {
                    "verse": "che le lagrime mie si spargon sole."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "5",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Pinger talor vostra bellezza in carte"
                },
                {
                    "verse": "cantando estimo, e prendo in man lo stile,"
                },
                {
                    "verse": "per dar principio a l'opera gentile,"
                },
                {
                    "verse": "vostre ombreggiando alcune lode sparte."
                }
            ],
            [
                {
                    "verse": "Ma poi ch'incontro la divina parte"
                },
                {
                    "verse": "degna d'altro lavoro e più sottile,"
                },
                {
                    "verse": "e riconosco il mio dir basso, umile,"
                },
                {
                    "verse": "ivi vien meno l'ardimento e l'arte."
                }
            ],
            [
                {
                    "verse": "Così lo stame solamente ordisco"
                },
                {
                    "verse": "de' vostri onori, né 'l gran subbio ch'empio"
                },
                {
                    "verse": "de le fila votar tessendo ardisco."
                }
            ],
            [
                {
                    "verse": "Però, se questa tela i'non adempio,"
                },
                {
                    "verse": "basta ch'incominciarla pur m'arrisco,"
                },
                {
                    "verse": "per dar forse di me non basso essempio."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "6",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Pioggia di lagrimar, di sospir folta"
                },
                {
                    "verse": "nebbia che 'l seme di mio frutto adugge;"
                },
                {
                    "verse": "pena, che quanto più la tengo accolta,"
                },
                {
                    "verse": "più veghiando e dormendo mi distrugge;"
                }
            ],
            [
                {
                    "verse": "voglia senz'ale di volare stolta;"
                },
                {
                    "verse": "pensier, che sempre consumando fugge"
                },
                {
                    "verse": "miei spirti, ond'io la vita odio talvolta,"
                },
                {
                    "verse": "benché rapidamente ella sen' fugge,"
                }
            ],
            [
                {
                    "verse": "mi dan dì e notte sì crudele assalto,"
                },
                {
                    "verse": "ch'a schermirmi da loro i'non ho arme,"
                },
                {
                    "verse": "e 'l vostro util soccorso indarno chiamo."
                }
            ],
            [
                {
                    "verse": "Così mi vivo: et oh piegar vostr'alto"
                },
                {
                    "verse": "sdegno potessi con umiliarme:"
                },
                {
                    "verse": "in tale stella presi l'esca e l'amo."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "7",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Amor, che nel bel lume godi e stai"
                },
                {
                    "verse": "del vivo sol de la mia donna e diva,"
                },
                {
                    "verse": "onde più d'una piaga acerba e schiva"
                },
                {
                    "verse": "con l'aspre tue saette al cor mi fai;"
                }
            ],
            [
                {
                    "verse": "se pietà ti commove de'miei guai,"
                },
                {
                    "verse": "che 'n mar m'han posto, che no' ha fondo o riva,"
                },
                {
                    "verse": "mira mia vita di dolzeza priva"
                },
                {
                    "verse": "e quindi ascolta i miei pietosi lai."
                }
            ],
            [
                {
                    "verse": "Scalda poscia costei, che prende in gioco"
                },
                {
                    "verse": "le tue quadrella, del tuo santo ardore,"
                },
                {
                    "verse": "a cui son di mercé chiamar già roco:"
                }
            ],
            [
                {
                    "verse": "ch'io temo forte che 'l vitale umore"
                },
                {
                    "verse": "solo non basti a sostener il foco,"
                },
                {
                    "verse": "che smisuratamente m'arde il core."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "8",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Occhi leggiadri, che soavemente"
                },
                {
                    "verse": "sol col mirar mi depredaste il core,"
                },
                {
                    "verse": "nel giorno ch'io non oprai 'ncontr'Amore"
                },
                {
                    "verse": "l'arme da lui che mi campar sovente,"
                }
            ],
            [
                {
                    "verse": "e voi man preste a innamorar la gente,"
                },
                {
                    "verse": "la cui beltà fa 'l mio foco maggiore,"
                },
                {
                    "verse": "se vostra vista acqueta ogni altro ardore,"
                },
                {
                    "verse": "ond' è che sete sì a celarv' intente?"
                }
            ],
            [
                {
                    "verse": "Ne le fortune mie voi sete il porto"
                },
                {
                    "verse": "e de la mente travagliata e stanca"
                },
                {
                    "verse": "voi la difesa e voi 'l conforto solo."
                }
            ],
            [
                {
                    "verse": "Ma se l'alto soccorso indi mi manca,"
                },
                {
                    "verse": "con tutto quel che con industria involo,"
                },
                {
                    "verse": "io morrò pria che 'l mio fin giunga a torto."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "9",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Nobil pensier, che nel mio cor s'annida"
                },
                {
                    "verse": "e d'un cortese desiar s'appaga,"
                },
                {
                    "verse": "la voglia rende innamorata e vaga"
                },
                {
                    "verse": "de la vostra beltà, che 'n ciel mi guida."
                }
            ],
            [
                {
                    "verse": "E tanto l'alma nel valor suo fida,"
                },
                {
                    "verse": "che non teme lo sdegno che l'impiaga,"
                },
                {
                    "verse": "né virtù d'erbe o forza d'arte maga,"
                },
                {
                    "verse": "che dal primo voler mai la divida."
                }
            ],
            [
                {
                    "verse": "Ma voi, che sete alma celeste e diva,"
                },
                {
                    "verse": "come creder potete che 'n voi brami"
                },
                {
                    "verse": "altro ch'aver co' bei vostr' occhi pace?"
                }
            ],
            [
                {
                    "verse": "Io non voglio altro che vedervi priva"
                },
                {
                    "verse": "d'odio e pregarvi, poi ch'è forza ch'ami,"
                },
                {
                    "verse": "che non vi spiaccia almen, se non vi piace."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "10",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Mal s'agguaglia a mia doglia un dolce sguardo,"
                },
                {
                    "verse": "perché l'un fugge qual seren di verno"
                },
                {
                    "verse": "e si fa l'altro eterno,"
                },
                {
                    "verse": "ch'al desir tronca l'ale, ond'io tutt'ardo."
                }
            ],
            [
                {
                    "verse": "E mal s'accorda un riso con l'offese,"
                },
                {
                    "verse": "perché l'un cresce e l'altro fugge via,"
                },
                {
                    "verse": "che potria dar conforto al mio languire."
                },
                {
                    "verse": "Crudele il dardo e fu la fiamma ria,"
                }
            ],
            [
                {
                    "verse": "ond'Amor l'alma mi trafisse e accese"
                },
                {
                    "verse": "il primo dì, ch'a voi non ebbe ardire"
                },
                {
                    "verse": "punger la gonna pur, non che ferire."
                }
            ],
            [
                {
                    "verse": "Ben fu fanciullo: ché, se voi prendea,"
                },
                {
                    "verse": "maggior preda facea,"
                },
                {
                    "verse": "e meglio oprato avria la fiamma e 'l dardo."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "11",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quando più l'alma, che no' ad altro attende,"
                },
                {
                    "verse": "in aspetto pensosa e 'n sé romita"
                },
                {
                    "verse": "a contemplar vostra beltà infinita,"
                },
                {
                    "verse": "per grand'amor nel gran piacer s'accende,"
                }
            ],
            [
                {
                    "verse": "lo sfrenato desio che 'n lei risplende"
                },
                {
                    "verse": "alor più cresce e a sospirar l'invita,"
                },
                {
                    "verse": "tal ch'in un punto provo morte e vita;"
                },
                {
                    "verse": "né però contra 'l suol mal si difende."
                }
            ],
            [
                {
                    "verse": "E s'avien che talora in sé raccoglia"
                },
                {
                    "verse": "alcun conforto de la rimembranza"
                },
                {
                    "verse": "de l'alma vista, che sparir fa il sole,"
                }
            ],
            [
                {
                    "verse": "non s'assicura sua timida voglia:"
                },
                {
                    "verse": "per ch'ella vive fuor de la speranza,"
                },
                {
                    "verse": "ch'a' suoi seguaci Amor prometter suole."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "12",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Mirando 'l sole, che giamai levarse"
                },
                {
                    "verse": "non fu visto più bello dal suo letto,"
                },
                {
                    "verse": "dal dì che 'l paradiso fu interdetto"
                },
                {
                    "verse": "a ch'in saper a Dio volle agguagliarse;"
                }
            ],
            [
                {
                    "verse": "da l'altra parte insiememente apparse"
                },
                {
                    "verse": "l'altro sole, ond'io vivo, sott'un schietto"
                },
                {
                    "verse": "vestir, con l'aureo crin crespo e negletto,"
                },
                {
                    "verse": "e 'n somma tal, che 'l cor di subit' arse."
                }
            ],
            [
                {
                    "verse": "Et io, almo pianeta, a cui soggiace"
                },
                {
                    "verse": "quanto cria 'l tempo e quanto poi ruina,"
                },
                {
                    "verse": "onde 'l nostro desir non è mai pago,"
                }
            ],
            [
                {
                    "verse": "siami lecito dir con la tua pace"
                },
                {
                    "verse": "che perché la tua luce sia divina,"
                },
                {
                    "verse": "che 'l mortal par più del celeste vago."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "13",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Con sì dolce catena e con tal nodo"
                },
                {
                    "verse": "da voi legato 'l cor, donna, mi sento,"
                },
                {
                    "verse": "che, se più lo stringete, più contento"
                },
                {
                    "verse": "vivo e, se men, de l'allentar men godo."
                }
            ],
            [
                {
                    "verse": "Se volete però sempre ad un modo"
                },
                {
                    "verse": "farmi beato e fuor d'ogni tormento"
                },
                {
                    "verse": "ch'io meni i giorni, mentre il lume spento"
                },
                {
                    "verse": "non fia degli occhi e mentr'io parlo e odo;"
                }
            ],
            [
                {
                    "verse": "e se v'aggrada ch'io non cessi mai"
                },
                {
                    "verse": "d'empir di vostre glorie mille carte,"
                },
                {
                    "verse": "benché peso mi par non del mio braccio;"
                }
            ],
            [
                {
                    "verse": "poi ché né puote né potrà giamai"
                },
                {
                    "verse": "star il mio cor in più beata parte,"
                },
                {
                    "verse": "tirate sempre e ristringete il laccio."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "14",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quando Amor i begli occhi intorno gira,"
                },
                {
                    "verse": "che fanno al sole meraviglia e scorno,"
                },
                {
                    "verse": "et apportano al giorno un novo giorno"
                },
                {
                    "verse": "d'un aura pieno, che soave spira,"
                }
            ],
            [
                {
                    "verse": "non ponno in lor i miei far lunga mira;"
                },
                {
                    "verse": "ond'io li chiudo e desioso torno"
                },
                {
                    "verse": "spesso ad aprirli e arroscio e imbianco e scorno,"
                },
                {
                    "verse": "per la vergogna c'ho di chi mi mira."
                }
            ],
            [
                {
                    "verse": "L'anima ancora langue di dolcezza,"
                },
                {
                    "verse": "e 'l cor doglioso acerba pena sente,"
                },
                {
                    "verse": "allegrezza provando e noia estreme."
                }
            ],
            [
                {
                    "verse": "Il piacer dal mirar tanta bellezza"
                },
                {
                    "verse": "e 'l duol dal disperar nasce a la mente:"
                },
                {
                    "verse": "così beato e son misero insieme."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000278",
        "author": "Diego Sandoval De Castro",
        "collection": "Rime",
        "title": "15",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Questi son gli occhi, onde di strali e foco"
                },
                {
                    "verse": "mille messi d'Amor armati usciro,"
                },
                {
                    "verse": "quando nel cor giunse 'l primier sospiro,"
                },
                {
                    "verse": "che 'l penar mi fa dolce e 'l pianger gioco."
                }
            ],
            [
                {
                    "verse": "Queste son quelle chiome, a cui son roco"
                },
                {
                    "verse": "gridar ch'allentin l'aspro mio martiro."
                },
                {
                    "verse": "Questo è quel chiaro volto in cui respiro,"
                },
                {
                    "verse": "che del mio male e d'altro gli cal poco."
                }
            ],
            [
                {
                    "verse": "Queste son le bellezze ch'io tant'amo,"
                },
                {
                    "verse": "che dì e notte di me lor voglia fanno"
                },
                {
                    "verse": "e mi dan poche dolci ore serene."
                }
            ],
            [
                {
                    "verse": "Questa è quella sì vaga del mio danno,"
                },
                {
                    "verse": "la qual io sempre ne' sospir miei chiamo,"
                },
                {
                    "verse": "che potria sola rallentar mie pene."
                }
            ]
        ]
    },
