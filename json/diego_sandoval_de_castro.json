        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Com'al vostr'apparir chiari e lucenti"
                },
                {
                    "verse": "tornar d'Apollo i nubilosi rai,"
                },
                {
                    "verse": "e le piagge fiorite e i giorni gai,"
                },
                {
                    "verse": "e il mar tranquillo e queti in aria i venti;"
                }
            ],
            [
                {
                    "verse": "e come dal mio cor gli aspri tormenti"
                },
                {
                    "verse": "fuggiro in tutto e i dolorosi guai,"
                },
                {
                    "verse": "ond'io col dolce canto il suon temprai"
                },
                {
                    "verse": "de'passati dogliosi e tristi accenti;"
                }
            ],
            [
                {
                    "verse": "così, al partire, oscuro il sol divenne;"
                },
                {
                    "verse": "tristi i dì e secche le campagne erbose,"
                },
                {
                    "verse": "e fur Eolo e Nettun sempre in disparte; "
                }
            ],
            [
                {
                    "verse": "e così la mia lingua e 'l cor ritenne"
                },
                {
                    "verse": "l'usato stile, e 'n rime aspre e dogliose,"
                },
                {
                    "verse": "sfogo 'l dolor, che mai da me non parte."
                }
            ]
        ]
