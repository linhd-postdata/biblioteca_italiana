        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Sogno, che solo fai lieta e contenta"
                },
                {
                    "verse": "la mia dogliosa e disperata vita"
                },
                {
                    "verse": "e ch'io ritrovi ancor guerra finita"
                },
                {
                    "verse": "al continuo dolor che mi tormenta,"
                }
            ],
            [
                {
                    "verse": "qual Angel fu la sù, che così intenta"
                },
                {
                    "verse": "la vista tenne a scorger mia infinita"
                },
                {
                    "verse": "miseria e ti mandò giù a darmi aita,"
                },
                {
                    "verse": "perch'io senz'alma vera gloria senta?"
                }
