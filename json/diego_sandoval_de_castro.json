        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Amor cui tutto quel che di tormento"
                },
                {
                    "verse": "m'è cagione e di lagrime e sospiri,"
                },
                {
                    "verse": "piacer apporta, e tutti i miei martiri"
                },
                {
                    "verse": "sembran riposo e riso il mio lamento."
                }
            ],
            [
                {
                    "verse": "Se non vuoi che m'ancida il duol ch'io sento,"
                },
                {
                    "verse": "fa che sian moderati i miei desiri,"
                },
                {
                    "verse": "o che pietosamente gli occhi giri"
                },
                {
                    "verse": "quella ver me, che mi può far contento."
                }
            ],
            [
                {
                    "verse": "Opra il primo miracolo, ch'è tale"
                },
                {
                    "verse": "che maggior nol potresti adoprar mai;"
                },
                {
                    "verse": "e, se manca il secondo, fia men male."
                }
            ],
            [
                {
                    "verse": "Ma s'offese punir mille vorrai,"
                },
                {
                    "verse": "mostra pur l'arco e aventa in lei lo strale"
                },
                {
                    "verse": "tal che sempr'arda de' suoi caldi rai."
                }
            ]
        ]
