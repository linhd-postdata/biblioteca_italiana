        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Alta cagion del mio sì dolce foco,"
                },
                {
                    "verse": "mirate com'io ardo e come gelo,"
                },
                {
                    "verse": "mentre chiudo nel cor l'ardente zelo"
                },
                {
                    "verse": "e 'l timor freddo, onde vi cal sì poco."
                }
            ],
            [
                {
                    "verse": "Se turbata vi miro, tremo e loco"
                },
                {
                    "verse": "non trovo da celarmi in terra o 'n cielo;"
                },
                {
                    "verse": "se 'l dolce lume de' vostr'occhi il gelo"
                },
                {
                    "verse": "disfà, che volge ogni tormento in gioco,"
                }
            ],
            [
                {
                    "verse": "quella virtù ch'al petto ogni or verdeggia"
                },
                {
                    "verse": "si desta e corre per le vene un caldo"
                },
                {
                    "verse": "al cor che, com'ei fosse esca, l'alluma."
                }
            ],
            [
                {
                    "verse": "Ma qual sia più, se l'un l'altro pareggia,"
                },
                {
                    "verse": "lasso, nol so. Ma so ch'io mi sto saldo"
                },
                {
                    "verse": "et al fin questo e quello mi consuma."
                }
            ]
        ]
    },
    {
