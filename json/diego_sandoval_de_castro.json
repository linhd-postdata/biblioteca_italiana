            [
                {
                    "verse": "Ben mi rendi felice in questo stato:"
                },
                {
                    "verse": "se non che 'l tuo fuggir ratto mi spoglia"
                },
                {
                    "verse": "tosto del ben, onde gioir mi fai."
                }
            ],
            [
                {
                    "verse": "almen ritorna e pel sentiero usato"
                },
                {
                    "verse": "empi di quel diletto questa spoglia,"
                },
                {
                    "verse": "ch'altri che tu non potria darmi mai."
                }
            ]
        ]
