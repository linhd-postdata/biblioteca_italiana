        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Apollo, se 'l veder la bell'Aurora"
                },
                {
                    "verse": "umido gli occhi, quando in mar ti posi,"
                },
                {
                    "verse": "scender dal ciel e i cari tuo' riposi"
                },
                {
                    "verse": "romper nel mezzo t'increbbe talora;"
                }
            ],
            [
                {
                    "verse": "creder potrai quanto a me pesi ancora"
                },
                {
                    "verse": "il vederti bagnar il carro e ascosi"
                },
                {
                    "verse": "tener ne l'onde tuoi destrier focosi,"
                },
                {
                    "verse": "mentre a seguir tua scorta è lunga l'ora."
                }
            ],
            [
                {
                    "verse": "Perché mentre tu dai la luce al die,"
                },
                {
                    "verse": "dà il lume agli occhi il mio più chiaro sole"
                },
                {
                    "verse": "e mi s'asconde, se ti celi altrui."
                }
            ],
            [
                {
                    "verse": "Resta dunque: e s' a gir per torte vie"
                },
                {
                    "verse": "natura altrove richiamar ti vuole,"
                },
                {
                    "verse": "riten il corso e mostra lei per cui."
                }
            ]
        ]
