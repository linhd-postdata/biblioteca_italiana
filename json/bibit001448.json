[
    {
        "author": "Dante Alighieri",
        "work": "Detto d'amore",
        "title": "Detto d'amore",
        "structure": "versi",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001448",
        "text": [
            [
                {
                    "verse": "Amor s\u00ec vuole e par-li,"
                },
                {
                    "verse": "Ch'i' 'n ogni guisa parli"
                },
                {
                    "verse": "E ched i' faccia un detto,"
                },
                {
                    "verse": "Che sia per tutto detto,"
                },
                {
                    "verse": "Ch'i' l'aggia ben servito."
                },
                {
                    "verse": "Po' ch'e' m'ebbe 'nservito"
                },
                {
                    "verse": "E ch'i' gli feci omaggio,"
                },
                {
                    "verse": "I' l'\u00f2 tenuto o\u00b7maggio"
                },
                {
                    "verse": "E terrr\u00f2 gi\u00e0m\u00e0' sempre;"
                },
                {
                    "verse": "E questo, fin asempr' \u00e8"
                },
                {
                    "verse": "A ciascun amoroso,"
                },
                {
                    "verse": "S\u00ec ch'Amor amoroso"
                },
                {
                    "verse": "No\u00b7gli sia nella fine,"
                },
                {
                    "verse": "Anzi ch'e' metta a fine"
                },
                {
                    "verse": "Ci\u00f2 ch'e' disira avere,"
                },
                {
                    "verse": "Che val me' ch'altro avere."
                },
                {
                    "verse": "Ed egli \u00e8 s\u00ec cortese"
                },
                {
                    "verse": "Che chi gli sta cortese"
                },
                {
                    "verse": "Od a man giunte avante,"
                },
                {
                    "verse": "Esso s\u00ec 'l mette avante"
                },
                {
                    "verse": "Di ci\u00f2 ched e' disira,"
                },
                {
                    "verse": "E di tutto il dis-ira."
                },
                {
                    "verse": "Amor non vuol logaggio,"
                },
                {
                    "verse": "Ma e' vuol ben, lo gaggio,"
                },
                {
                    "verse": "Ch'\u00e8 'l tu' cuor, si' a lu' fermo."
                },
                {
                    "verse": "Allor dice: \u00abI' t'afermo"
                },
                {
                    "verse": "Di ci\u00f2 che\u00b7ttu domandi,"
                },
                {
                    "verse": "Sanza che\u00b7ttu do\u00b7mandi\u00bb;"
                },
                {
                    "verse": "E d\u00f3nati in presente,"
                },
                {
                    "verse": "Sanz' esservi presente"
                },
                {
                    "verse": "Di fino argento o d'oro,"
                },
                {
                    "verse": "Perch'i' a\u00b7llui m'adoro"
                },
                {
                    "verse": "Come leal amante."
                },
                {
                    "verse": "A\u00b7llu' fo grazze, amante"
                },
                {
                    "verse": "Quella che d'ogne bene"
                },
                {
                    "verse": "\u00c8 s\u00ec guernita bene"
                },
                {
                    "verse": "Che 'n le' non trov' uon pare;"
                },
                {
                    "verse": "E quand' ella m'apare,"
                },
                {
                    "verse": "S\u00ec grande gioia mi dona,"
                },
                {
                    "verse": "Che lo me' cor s'adona"
                },
                {
                    "verse": "A le' sempre servire,"
                },
                {
                    "verse": "E di le' vo' serv' ire,"
                },
                {
                    "verse": "Tant' \u00e0 in le' piacimento."
                },
                {
                    "verse": "Non so se piacimento"
                },
                {
                    "verse": "Le' fia ched i' la serva:"
                },
                {
                    "verse": "Almen pu\u00f2 dir che serv' \u00e0,"
                },
                {
                    "verse": "Come ch'i' poco vaglia."
                },
                {
                    "verse": "Amor nessun non vaglia,"
                },
                {
                    "verse": "Ma ciascun vuole ed ama,"
                },
                {
                    "verse": "Chi di lui ben s'inama,"
                },
                {
                    "verse": "E di colu' fa forza"
                },
                {
                    "verse": "Che 'n compiacer fa forza"
                },
                {
                    "verse": "E nonn-\u00e0, i\u00b7nulla, parte."
                },
                {
                    "verse": "Amor i\u00b7nulla part' \u00e8"
                },
                {
                    "verse": "Ch'e' non sia tutto presto"
                },
                {
                    "verse": "A fine amante presto."
                },
                {
                    "verse": "Cos\u00ec sue cose livera"
                },
                {
                    "verse": "A chi l'amor no\u00b7llivera"
                },
                {
                    "verse": "E mette pene e 'ntenza"
                },
                {
                    "verse": "In far sua penetenza"
                },
                {
                    "verse": "Tal chente Amor comanda"
                },
                {
                    "verse": "A chi a\u00b7llu' s'acomanda;"
                },
                {
                    "verse": "E chi la porta in grado,"
                },
                {
                    "verse": "Il mette in alto grado"
                },
                {
                    "verse": "Di ci\u00f2 ched e' disia:"
                },
                {
                    "verse": "Per me cotal d\u00ec sia!"
                },
                {
                    "verse": "Per ch'i' gi\u00e0 non dispero,"
                },
                {
                    "verse": "Ma ciaschedun d\u00ec spero"
                },
                {
                    "verse": "Merz\u00e9, po' 'n su' travaglio"
                },
                {
                    "verse": "I' son sanza travaglio,"
                },
                {
                    "verse": "E sonvi s\u00ec legato"
                },
                {
                    "verse": "Ch'i' non vo' che legato"
                },
                {
                    "verse": "Giamai me ne prosciolga:"
                },
                {
                    "verse": "Se nn'\u00e0 d'altri pro', sciolga!"
                },
                {
                    "verse": "Ch'i' vo' ch'Amor m'aleghi,"
                },
                {
                    "verse": "Che che Ragion m'alleghi:"
                },
                {
                    "verse": "Di lei il me' cor sicura,"
                },
                {
                    "verse": "N\u00e9 pi\u00f9 di lei non cura;"
                },
                {
                    "verse": "Ella si fa d\u00efessa:"
                },
                {
                    "verse": "N\u00e9\u00b7ffu n\u00e9 fia di essa."
                },
                {
                    "verse": "Amor blasma ed isfama"
                },
                {
                    "verse": "E dice ch'e' disfama,"
                },
                {
                    "verse": "Ma non del mi', certano:"
                },
                {
                    "verse": "Perch'i' per le' certan \u00f2"
                },
                {
                    "verse": "Che ciaschedun s'abatte;"
                },
                {
                    "verse": "Me' ched Amor sa, batte."
                },
                {
                    "verse": "Ed a me dice: \u00abFolle,"
                },
                {
                    "verse": "Perch\u00e9 cos\u00ec t'afolle"
                },
                {
                    "verse": "D'aver tal signoria?"
                },
                {
                    "verse": "I' dico, sign\u00f3\u00b7ri' \u00e0"
                },
                {
                    "verse": "Chi porta su' suggello."
                },
                {
                    "verse": "I' per me non suggello,"
                },
                {
                    "verse": "Della sua 'mprenta, breve,"
                },
                {
                    "verse": "Ch'\u00e8 troppo corta e breve"
                },
                {
                    "verse": "La gioia, e la noia lunga."
                },
                {
                    "verse": "Or taglia ' geti, e lunga"
                },
                {
                    "verse": "Da lui, ch'egli \u00e8 di parte"
                },
                {
                    "verse": "Che, chi da lu' si parte,"
                },
                {
                    "verse": "E' fugge e s\u00ec va via."
                },
                {
                    "verse": "Or non tener sua via,"
                },
                {
                    "verse": "Se vuo' da\u00b7llu' campare;"
                },
                {
                    "verse": "E se non, mal camp' are,"
                },
                {
                    "verse": "Che biado non vi grana,"
                },
                {
                    "verse": "Anzi perde la grana"
                },
                {
                    "verse": "Chiunque la vi getta."
                },
                {
                    "verse": "Perdio, or te ne getta"
                },
                {
                    "verse": "Di quel falso diletto,"
                },
                {
                    "verse": "E fa che si'a diletto"
                },
                {
                    "verse": "Del mi', ched egli \u00e8 fine,"
                },
                {
                    "verse": "Che d\u00e0 gioia sanza fine."
                },
                {
                    "verse": "Lo dio dov' \u00e0i credenza"
                },
                {
                    "verse": "Non ti far\u00e0 credenza"
                },
                {
                    "verse": "Se non come Fortuna."
                },
                {
                    "verse": "Tu\u00b7sse' in gran fortuna,"
                },
                {
                    "verse": "Se non prendi buon porto"
                },
                {
                    "verse": "Per quel ched i' t'\u00f2 porto,"
                },
                {
                    "verse": "Ed a me non t'aprendi"
                },
                {
                    "verse": "E 'l mi' sermone aprendi."
                },
                {
                    "verse": "Or mi rispondi e di',"
                },
                {
                    "verse": "Ch'egli \u00e8 ancor gran d\u00ec"
                },
                {
                    "verse": "A farmi tua risposta;"
                },
                {
                    "verse": "Ma non mi far risposta"
                },
                {
                    "verse": "A ci\u00f2 ch'i' \u00f2 proposato."
                },
                {
                    "verse": "D\u00ec tu se pro' posat' \u00f2\u00bb."
                },
                {
                    "verse": ""
                },
                {
                    "verse": "E, quand' i' ebbi intesa"
                },
                {
                    "verse": "Ragion, ch' \u00e8 stata intesa"
                },
                {
                    "verse": "A trarmi de la regola"
                },
                {
                    "verse": "D'Amor, che 'l mondo regola,"
                },
                {
                    "verse": "I' le dissi: \u00abRagione,"
                },
                {
                    "verse": "I' \u00f2 salda ragione"
                },
                {
                    "verse": "Con Amor, e d'acordo"
                },
                {
                    "verse": "Si\u00e0n ben del nostro acordo,"
                },
                {
                    "verse": "Ed \u00e8 scritto a mi' conto"
                },
                {
                    "verse": "Ch'i' non sia pi\u00f9 tu' conto."
                },
                {
                    "verse": "\u00c8 la ragion dannata;"
                },
                {
                    "verse": "Perch'i' t'\u00f2 per dannata,"
                },
                {
                    "verse": "Ed ebbi, per convento,"
                },
                {
                    "verse": "Po' ch'i' fu' del convento"
                },
                {
                    "verse": "D'Amor, cu' Dio man tenga,"
                },
                {
                    "verse": "E sempr' e' me mantenga."
                },
                {
                    "verse": "Tu mi vuo' trar d'amare"
                },
                {
                    "verse": "E di' ch'Amor amar' \u00e8:"
                },
                {
                    "verse": "I' 'l truov\u00e0' dolce e fine,"
                },
                {
                    "verse": "E su' comincio e fine"
                },
                {
                    "verse": "Mi piacque e piacer\u00e0,"
                },
                {
                    "verse": "Ch\u00e9 'n s\u00e9 gran piacer' \u00e0."
                },
                {
                    "verse": "Or come viver\u00eao?"
                },
                {
                    "verse": "Sanz'Amor vive reo"
                },
                {
                    "verse": "Chi si governa al mondo;"
                },
                {
                    "verse": "Sanz'Amor egli \u00e8 mondo"
                },
                {
                    "verse": "D'ogne buona vertute,"
                },
                {
                    "verse": "N\u00e9 non pu\u00f2 far vertute;"
                },
                {
                    "verse": "Sanz'Amor s\u00ec \u00e8 'nu\u00eca,"
                },
                {
                    "verse": "Che, con cu' regna, envia"
                },
                {
                    "verse": "D'andarne dritto al luogo"
                },
                {
                    "verse": "L\u00e0 dove Envia \u00e0\u00b7lluogo."
                },
                {
                    "verse": "E perci\u00f2 non ti credo,"
                },
                {
                    "verse": "Se\u00b7ttu dicess' il Credo"
                },
                {
                    "verse": "E 'l Paternostro e\u00b7ll'Ave,"
                },
                {
                    "verse": "S\u00ec poco in te senn' \u00e0ve."
                },
                {
                    "verse": "Adio, ched i' mi torno,"
                },
                {
                    "verse": "E fine amante torno"
                },
                {
                    "verse": "Per devisar partita"
                },
                {
                    "verse": "Com' ell' \u00e8 ben partita"
                },
                {
                    "verse": "E di cors e di membra,"
                },
                {
                    "verse": "S\u00ec come a me mi membra\u00bb."
                },
                {
                    "verse": ""
                },
                {
                    "verse": "Cape' d'oro battuto"
                },
                {
                    "verse": "Paion, che m'\u00e0n battuto,"
                },
                {
                    "verse": "Quelli che porta in capo,"
                },
                {
                    "verse": "Per ch'i' a\u00b7llor fo capo."
                },
                {
                    "verse": "La sua piacente ciera"
                },
                {
                    "verse": "Nonn-\u00e8 sembiante a cera,"
                },
                {
                    "verse": "Anz' \u00e8 s\u00ec fresca e bella"
                },
                {
                    "verse": "Che lo me' cor s'abella"
                },
                {
                    "verse": "Di non le mai affare,"
                },
                {
                    "verse": "Tant' \u00e0 piacente affare."
                },
                {
                    "verse": "La sua fronte, e le ciglia,"
                },
                {
                    "verse": "Bielt\u00e0 d'ogn' altr' eciglia:"
                },
                {
                    "verse": "Tanto son ben voltati"
                },
                {
                    "verse": "Che ' mie' pensier' voltati"
                },
                {
                    "verse": "\u00c0nno ver' lei, che gioia"
                },
                {
                    "verse": "Mi d\u00e0 pi\u00f9 ch'altra gioia"
                },
                {
                    "verse": "In su' dolze riguardo."
                },
                {
                    "verse": "Di niu\u00b7mal \u00e0 riguardo"
                },
                {
                    "verse": "Cu' ella guarda in viso,"
                },
                {
                    "verse": "Tant' \u00e0 piacente aviso;"
                },
                {
                    "verse": "Ed \u00e0 s\u00ec chiara luce"
                },
                {
                    "verse": "Ch'al sol to' la sua luce,"
                },
                {
                    "verse": "E l'oscura e l'aluna"
                },
                {
                    "verse": "S\u00ec come il sol la luna."
                },
                {
                    "verse": "Per ch'i' a quella spera"
                },
                {
                    "verse": "\u00d2 messa la mia spera,"
                },
                {
                    "verse": "E s'i' ben co\u00b7llei regno,"
                },
                {
                    "verse": "I' non vogli' altro regno."
                },
                {
                    "verse": "La bocca e 'l naso e 'l mento"
                },
                {
                    "verse": "\u00c0 pi\u00f9 belli, e non mento,"
                },
                {
                    "verse": "Ch'unque nonn-ebbe Alena;"
                },
                {
                    "verse": "Ed \u00e0 pi\u00f9 dolce alena"
                },
                {
                    "verse": "Che nessuna pantera."
                },
                {
                    "verse": "Per ch'i' ver' sua pantera"
                },
                {
                    "verse": "I' mi sono, 'n fed', ito,"
                },
                {
                    "verse": "E dentro v'\u00f2 fedito;"
                },
                {
                    "verse": "Ed \u00e8mene s\u00ec preso"
                },
                {
                    "verse": "Ched i' vi son s\u00ec preso"
                },
                {
                    "verse": "Che mai, di mia partita,"
                },
                {
                    "verse": "No\u00b7mi far\u00f2 partita."
                },
                {
                    "verse": "La gola sua, e 'l petto,"
                },
                {
                    "verse": "S\u00ec chiar' \u00e8, ch'a Dio a petto"
                },
                {
                    "verse": "Mi par esser la dia"
                },
                {
                    "verse": "Ch'i' veggio quella dia."
                },
                {
                    "verse": "Tant' \u00e8 bianca e lattata,"
                },
                {
                    "verse": "Che ma' non fu alattata"
                },
                {
                    "verse": "Nulla di tal valuta."
                },
                {
                    "verse": "A me tropp' \u00e8 valuta,"
                },
                {
                    "verse": "Ched ella s\u00ec m'\u00e0 dritto"
                },
                {
                    "verse": "In saper tutto 'l dritto"
                },
                {
                    "verse": "Ch'Amor usa in sua corte,"
                },
                {
                    "verse": "Ch'e' non v'\u00e0 legge corte."
                },
                {
                    "verse": "Mani \u00e0 lunghette e braccia,"
                },
                {
                    "verse": "E chi co\u00b7llei s'abraccia"
                },
                {
                    "verse": "Giamai mal nonn-\u00e0 gotta"
                },
                {
                    "verse": "N\u00e9 di ren' n\u00e9 di gotta:"
                },
                {
                    "verse": "Il su' nobile stato"
                },
                {
                    "verse": "S\u00ec mette in buono stato"
                },
                {
                    "verse": "Chiunque la rimira."
                },
                {
                    "verse": "Per che 'l me' cor si mira"
                },
                {
                    "verse": "In lei e notte e giorno,"
                },
                {
                    "verse": "E sempre a\u00b7llei aggiorno,"
                },
                {
                    "verse": "Ch'Amor s\u00ec\u00b7ll'\u00e0 inchesto,"
                },
                {
                    "verse": "N\u00e9d e' non \u00e0 inchesto"
                },
                {
                    "verse": "Se potesse aver termine,"
                },
                {
                    "verse": "Ch'amar vorria san' termine."
                },
                {
                    "verse": "E quando va per via,"
                },
                {
                    "verse": "Ciascun di lei \u00e0 'nvia"
                },
                {
                    "verse": "Per l'andatura gente;"
                },
                {
                    "verse": "E quando parla a gente,"
                },
                {
                    "verse": "S\u00ec umilmente parla"
                },
                {
                    "verse": "Che boce d'agnol par l\u00e0."
                },
                {
                    "verse": "Il su' danzar e 'l canto"
                },
                {
                    "verse": "Val vie pi\u00f9 ad incanto"
                },
                {
                    "verse": "Che di nulla serena,"
                },
                {
                    "verse": "Ch\u00e9\u00b7ll'aria fa serena:"
                },
                {
                    "verse": "Quando la boce lieva,"
                },
                {
                    "verse": "Ogne nuvol si lieva"
                },
                {
                    "verse": "E l'aria riman chiara."
                },
                {
                    "verse": "Per che 'l me' cor s\u00ec chiar' \u00e0"
                },
                {
                    "verse": "Di non far giamai cambio"
                },
                {
                    "verse": "Di lei a nessun cambio;"
                },
                {
                    "verse": "Ch'ell' \u00e8 di s\u00ec gran pregio"
                },
                {
                    "verse": "Ch'i' non troveria pregio"
                },
                {
                    "verse": "Nessun, che mai la vaglia."
                },
                {
                    "verse": "Amor, se Dio mi vaglia,"
                },
                {
                    "verse": "Il terrebbe a\u00b7ffollore,"
                },
                {
                    "verse": "E ben seria foll' o re'"
                },
                {
                    "verse": "Quand' io il pensasse punto."
                },
                {
                    "verse": "M'Amor l'\u00e0 s\u00ec a punto"
                },
                {
                    "verse": "Nella mia mente pinta,"
                },
                {
                    "verse": "Ch'i' la mi veggio pinta"
                },
                {
                    "verse": "Nel cor, s'i' dormo o veglio."
                },
                {
                    "verse": "Unque asessino a\u00b7Veglio"
                },
                {
                    "verse": "Non fu giamai s\u00ec presto,"
                },
                {
                    "verse": "N\u00e9 a Dio mai il Presto,"
                },
                {
                    "verse": "Com' io a servir amante"
                },
                {
                    "verse": "Per le vert\u00f9 ch'\u00e0 mante."
                },
                {
                    "verse": "E s'io in lei pietanza"
                },
                {
                    "verse": "Truov', o d'una pietanza"
                },
                {
                    "verse": "Del su'amor son contento,"
                },
                {
                    "verse": "I' sar\u00f2 pi\u00f9 contento,"
                },
                {
                    "verse": "Per la sua gran valenza,"
                },
                {
                    "verse": "Che s'io avesse Valenza."
                },
                {
                    "verse": "Se Gelosia \u00e0 'n s\u00e9 gina"
                },
                {
                    "verse": "Di tormene segina,"
                },
                {
                    "verse": "Lo Dio d'Amor mi mente:"
                },
                {
                    "verse": "Ch\u00e9d i' \u00f2 ben a mente"
                },
                {
                    "verse": "Ci\u00f2 ched e' m'ebbe in grado"
                },
                {
                    "verse": "Sed i' 'l servisse a grado."
                },
                {
                    "verse": "Ben ci \u00e0 egli un camino"
                },
                {
                    "verse": "Pi\u00f9 corto, n\u00e9 'l camino,"
                },
                {
                    "verse": "Perci\u00f2 ch'i' nonn-\u00f2 entrata"
                },
                {
                    "verse": "Ched i' per quell'entrata"
                },
                {
                    "verse": "Potesse entrar un passo."
                },
                {
                    "verse": "Ricchezza guarda il passo,"
                },
                {
                    "verse": "Che non fa buona cara"
                },
                {
                    "verse": "A que' che no\u00b7ll'\u00e0 cara."
                },
                {
                    "verse": "E s\u00ec fu' i' s\u00ec saggio"
                },
                {
                    "verse": "Ched i' ne feci saggio"
                },
                {
                    "verse": "S'i' potesse oltre gire."
                },
                {
                    "verse": "\u00abPer neente t'aggire\u00bb,"
                },
                {
                    "verse": "Mi disse, e co\u00b7mal viso:"
                },
                {
                    "verse": "\u00abTu\u00b7sse' da me diviso,"
                },
                {
                    "verse": "Perci\u00f2 il passo ti vieto;"
                },
                {
                    "verse": "Non perch\u00e9\u00b7ttu sie vieto,"
                },
