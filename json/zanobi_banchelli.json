    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000239",
        "author": "Zanobi Banchelli",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Non può che 'n savio vera amistà cadere,"
                },
                {
                    "verse": "e savio consentiamo esser beato;"
                },
                {
                    "verse": "beato ha in sé perfetto, congregato"
                },
                {
                    "verse": "stato di tutto ben che può capere."
                }
            ],
            [
                {
                    "verse": "E se in sé di sé in suo volere"
                },
                {
                    "verse": "può contentar sé, fuor di sé a suo stato"
                },
                {
                    "verse": "agiugnere apetisce non beato;"
                },
                {
                    "verse": "ma' per servo Fortuna il può tenere."
                }
            ],
            [
                {
                    "verse": "A ben viver di sé contento il dico;"
                },
                {
                    "verse": "amico vuol, per non essere sciolto"
                },
                {
                    "verse": "da virtù d'amistà caritativa."
                }
            ],
            [
                {
                    "verse": "Non per trar beneficio elegge amico,"
                },
                {
                    "verse": "ma, altrui dando, in sé stima ricolto:"
                },
                {
                    "verse": "così vera amistà virtù cultiva."
                }
            ]
        ]
    }
]