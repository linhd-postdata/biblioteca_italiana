    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001122",
        "author": "Jacopo Tedaldi",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Prima vedrassi el mar privo de l'onde"
                },
                {
                    "verse": "e quelle divenir dolce e soave,"
                },
                {
                    "verse": "e gl'ignoranti vinceranno i savi"
                },
                {
                    "verse": "e l'anime dannate ancor fien monde;"
                }
            ],
            [
                {
                    "verse": "prima convien che 'n tutto Apol s'asconde"
                },
                {
                    "verse": "e' monti tutti fien concavi e cavi"
                },
                {
                    "verse": "e che in grazia di Dio fien i più pravi"
                },
                {
                    "verse": "e le facce de' morti rubiconde;"
                }
            ],
            [
                {
                    "verse": "prima serà la terra sanza frutto"
                },
                {
                    "verse": "e nella rena fie fertilitade"
                },
                {
                    "verse": "e la femina ancor costante e forte;"
                }
            ],
            [
                {
                    "verse": "prima serà nel ciel gran pianto e lutto"
                },
                {
                    "verse": "e ne l'enferno sol tranquillitade"
                },
                {
                    "verse": "ch'i' ti possa lassar se non per morte."
                }
            ]
        ]
    }
]