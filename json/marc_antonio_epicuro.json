[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Heu dolor, es prima raptus Caldora iuventa;"
                },
                {
                    "verse": "\ncum socio belli dum simulacra cies. "
                },
                {
                    "verse": "\nNam male texisses pulchros quum casside vultus, "
                },
                {
                    "verse": "\ngutture ab infesta cuspide laesa iaces. "
                },
                {
                    "verse": "\nQuodque magis miserum est, non hanc ferus intulit hostis; "
                },
                {
                    "verse": "\nsed qui fraterno vulnere caesus obis. "
                },
                {
                    "verse": "\nEt tecum extincta est patriae spes alta domusque: "
                },
                {
                    "verse": "\nhoc genitrix telo, hoc corruit icta soror. "
                },
                {
                    "verse": "\nHinc igitur quam fluxa hominum sint fata monemur; "
                },
                {
                    "verse": "\nqui necat, ille sua morte tulisset opem. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Lucia felicis baccas carpebat olivae, "
                },
                {
                    "verse": "\nrustica, sed facie digna puella polo. "
                },
                {
                    "verse": "\nArsit in hanc subito letali tabe Cupido, "
                },
                {
                    "verse": "\noptat et amplexus delitiasque puer. "
                },
                {
                    "verse": "\nAt genitrix nato indignos proclamat amores, "
                },
                {
                    "verse": "\nincrepat atque artes, increpat atque genus. "
                },
                {
                    "verse": "\nTunc Amor: Indignam, mater, quis credere posset, "
                },
                {
                    "verse": "\nquarti sic, post formae munera, Pallas amat? "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Dulcia cui dono mittam, mi culte Rotili, "
                },
                {
                    "verse": "\nni tibi, qui semper grandia dulce canis? "
                },
                {
                    "verse": "\nAn si forte cupis munuscula dulcia nosse, "
                },
                {
                    "verse": "\ndaedala quae tanta struxerit arte manus: "
                },
                {
                    "verse": "\nPinsuit alma Venus, digitis Aurora subegit, "
                },
                {
                    "verse": "\naurarunt Charites, et face coxit Amor. "
                },
                {
                    "verse": "\nNectare Pierides, violis sparsÃªre Napaeae, "
                },
                {
                    "verse": "\nSirenes myrtis, Antiniana rosis. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quod te quisque vocet Magnatem, agnomine miror, "
                },
                {
                    "verse": "\nquum te Magnetem iure vocare queat. "
                },
                {
                    "verse": "\nNon quia tu ferrum rapias, est causa: sed ista, "
                },
                {
                    "verse": "\nPaule, quod ex omni pectore corda trahas. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "V",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Nata (eheu miserum) misero mihi nata parenti, "
                },
                {
                    "verse": "\nunicus ut fieres, unica nata, dolor; "
                },
                {
                    "verse": "\nnam tibi dumque virum tedas thalamumque parabam, "
                },
                {
                    "verse": "\nfunera et inferias anxius ecce paro. "
                },
                {
                    "verse": "\nDebuimus tecum poni materque paterque, "
                },
                {
                    "verse": "\nut tribus haec miseris urna parata foret. "
                },
                {
                    "verse": "\nAt nos perpetui luctus, tu, nata, sepulchri"
                },
                {
                    "verse": "\nesto heres, ubi sic impia fata volunt. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "VI",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quae mihi debebas supremae munera vitae, "
                },
                {
                    "verse": "\ninfelix solvo nunc tibi, nate, prior. "
                },
                {
                    "verse": "\nFortuna inconstans et lex variabilis aevi! "
                },
                {
                    "verse": "\nDebueras cineri iam superesse meo. "
                },
                {
                    "verse": "\nHaec dat matris amor rapti solamina nata, "
                },
                {
                    "verse": "\ninvida cum Lachesis tam breve nectit opus. "
                },
                {
                    "verse": "\nNate, iaces: vivo contra mea vota superstes; "
                },
                {
                    "verse": "\nvox gemitus posthac, lux mihi erunt tenebrae. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "VII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Stratus humi Python exhausta, Phoebe, pharetra"
                },
                {
                    "verse": "\naesculea cinxit si tibi fronde caput, "
                },
                {
                    "verse": "\nlaurea quae Tuccae, febres qui perdidit omnes? "
                },
                {
                    "verse": "\nUnica perdendi potio causa fuit. "
                },
                {
                    "verse": "\nQuod Phoebus Phoeboque satus nequere tot annos, "
                },
                {
                    "verse": "\npro pudor, hoc nobis quarti cito Tucca dedit. "
                },
                {
                    "verse": "\nServatus mundus, servata est delphica tellus, "
                },
                {
                    "verse": "\nhumanus Tucca est, filius ille Iovis. "
                },
                {
                    "verse": "\nGloria quae maior? maior quae liberat orbem, "
                },
                {
                    "verse": "\nPlaudite mortales, vicimus ecce Deum. "
                },
                {
                    "verse": "\nPhoebe, tuis radiis posthac lustrabis Olympum, "
                },
                {
                    "verse": "\nlustrabit mundum Tucca salutis ope. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "VIII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Dum canis heroas, celebras dum carmine collem"
                },
                {
                    "verse": "\nvertice qui cunctis celsior astra petit, "
                },
                {
                    "verse": "\nPierides numeros, Charites tribuere leporem, "
                },
                {
                    "verse": "\nSirenes cantum, Cynthius ipse lyram. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "IX",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Tibi uni Caelitum, Phoebe clarissime, "
                },
                {
                    "verse": "\nhunc locum, quin se ipsos Sereni tui dedicant. "
                },
                {
                    "verse": "\nTu illis faveas praesensque adsis, "
                },
                {
                    "verse": "\net non ut scripta perpetuo serena silent. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "X",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Dum Fabio de te refero, Lysistrate, quot sint"
                },
                {
                    "verse": "\nquae nullo scribis carmina iudicio: "
                },
                {
                    "verse": "\nscribat, ait, nam dispeream, si iustior ulla est"
                },
                {
                    "verse": "\npoena hac, a magnis quam precer huic superis. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "XI",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Omnia naturae sic quondam sensa latebant, "
                },
                {
                    "verse": "\nut vix e latebris Delius eliceret, "
                },
                {
                    "verse": "\nantiquum inque chaos rursus lapsura timebat, "
                },
                {
                    "verse": "\nni Zimara in tenebris forte tulisset opem, "
                },
                {
                    "verse": "\ncui nunc aeterno praefertur lumine lampas, "
                },
                {
                    "verse": "\nlampas apollineo sydere clara magis, "
                },
                {
                    "verse": "\nqua reserat sophiae nodos, qua nubila pellit, "
                },
                {
                    "verse": "\nnubila quae nullus dispulit ante sophus; "
                },
                {
                    "verse": "\nignotasque vias rerumque arcana retexens, "
                },
                {
                    "verse": "\nimmensum mundi rite recludit opus. "
                },
                {
                    "verse": "\nErgo quicquid habet veri natura, Zimarra, "
                },
                {
                    "verse": "\niure tibi debet: debet uterque polus. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "XII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Tot sua non inter miracula Iuppiter unquam"
                },
                {
                    "verse": "\nrestituit vivis mortua membra Deus. "
                },
                {
                    "verse": "\nHinc humerum Pelopi mutilum reparavit eburnum"
                },
                {
                    "verse": "\nabscissae nequiens reddere partis opem. "
                },
                {
                    "verse": "\nPars sophiae lacera, abiecta et miseranda iacebat, "
                },
                {
                    "verse": "\nmille gerens facie vulnera, mille manu: "
                },
                {
                    "verse": "\nen Zimarae auxilio recte solidatur ad unguem"
                },
                {
                    "verse": "\natque reviviscit mortua quae fuerat. "
                },
                {
                    "verse": "\nVel Iove maiorem se Zimara praestitit; at si"
                },
                {
                    "verse": "\nmaiorem nolim dicere, dico parem. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Qui cupit obsessam Gallis, terraque marique, "
                },
                {
                    "verse": "\ncrinibus abscissis cernere Parthenopen; "
                },
                {
                    "verse": "\nsirenesque alias flentes, nymphasque per omneis"
                },
                {
                    "verse": "\nanfractus pulsas patria flere loca; "
                },
                {
                    "verse": "\nocclusumque antro disrupta et funditus urna, "
                },
                {
                    "verse": "\nSebethum lachrymis ora rigare novis; "
                },
                {
                    "verse": "\net pariter lauros, myrtos et citrea rura"
                },
                {
                    "verse": "\n(pro facinus) positis collachrymare comis; "
                },
                {
                    "verse": "\nregibus et Musis fontes et praedia sacra"
                },
                {
                    "verse": "\ndiruta ab hostili quaeque iacere manu; "
                },
                {
                    "verse": "\net castra, obsessis iam iam quae mille parabant"
                },
                {
                    "verse": "\nfunera, vincla, cruces, captaque caesa simul: "
                },
                {
                    "verse": "\nprincipis aonidum legat haec monimenta Camilli, "
                },
                {
                    "verse": "\nbelligeros nati tollere in astra duces. "
                },
                {
                    "verse": "\nTuque tuos longos Siren solare labores, "
                },
                {
                    "verse": "\nnamque uno aeterna et libera facta die es. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Gallorum cepisse duces regemque superbum, "
                },
                {
                    "verse": "\nsolvere Parthenopen obsidione, fame: "
                },
                {
                    "verse": "\nhaec te sal notum duplex victoria fecit"
                },
                {
                    "verse": "\nquaque patent terrae, quaque patent maria. "
                },
                {
                    "verse": "\nSed vicisse trucem romani nominis hostem, "
                },
                {
                    "verse": "\nquem capiet, Caesar, tertia palma locum? "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Iam sileat iactare fidem flammasque Saguntum, "
                },
                {
                    "verse": "\nMassilia obsidium, moenia hetrusca famem. "
                },
                {
                    "verse": "\nCaesareis nam sponte sua fidissima signis, "
                },
                {
                    "verse": "\nParthenope his voluit horridiora pati. "
                },
                {
                    "verse": "\nPlaude tamen, quoniam datur, ut si barbara castra"
                },
                {
                    "verse": "\nte lacerent semper, semper humare tuum est. "
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000110",
        "author": "Marc'Antonio Epicuro",
        "collection": "Poesie latine",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quis modo rauco sono cunctas strepere ore cicadas"
                },
                {
                    "verse": "\ndicat, vel querulo taedia ferre sono? "
                },
                {
                    "verse": "\nEn hunc nonne putet qui audit tam dulce canentem"
                },
                {
                    "verse": "\nSirenum lepidos exuperare sonos? "
                },
                {
                    "verse": "\nLaudat, amat, queritur, rogat, impetrat omnia versu, "
                },
                {
                    "verse": "\nflectit et argutae dexteritate lyrae. "
                },
                {
                    "verse": "\nUnde Paphum Cyprumque simul Venus atque Cupido"
                },
                {
                    "verse": "\nIdalium et Charites deseruere nemus"
                },
                {
                    "verse": "\nnec cupiunt alio lachrymas, suspiria posthae"
                },
                {
                    "verse": "\nnon alio flammas dicier ore suas. "
                }
            ]
        ]
    }
]