[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000387",
        "author": "Antonio Manetti",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Veloce in alto mar solcar vedemo"
                },
                {
                    "verse": "un burchielletto assai leggiero e snello,"
                },
                {
                    "verse": "carco di gran tesauro e d'un gioiello"
                },
                {
                    "verse": "bel sì ch'un simil mai veder potemo."
                }
            ],
            [
                {
                    "verse": "Nove donne el movien, benché 'l supremo"
                },
                {
                    "verse": "tenea Caliopè e dal castello"
                },
                {
                    "verse": "el timon dirizzando di pennello"
                },
                {
                    "verse": "con l'occhio al polo, e l'altre erano al remo."
                }
            ],
            [
                {
                    "verse": "E quanto rallegrar vedemo e porti"
                },
                {
                    "verse": "dove toccò, perch'or lo cuopre l'onde,"
                },
                {
                    "verse": "tanto pianger veggiamo e far querela."
                }
            ],
            [
                {
                    "verse": "E se nulla è che 'l lor viver conforti,"
                },
                {
                    "verse": "è che 'l gioiel rinvolto nelle fronde"
                },
                {
                    "verse": "d'un lauro verde alcun'acqua non vela."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000387",
        "author": "Antonio Manetti",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Fia ogni cosa chiara e sempre bruna,"
                },
                {
                    "verse": "gli uomini sottoposti agli animali,"
                },
                {
                    "verse": "e gli uselletti voleran sanza ali,"
                },
                {
                    "verse": "figliuo' perfetti non con donna alcuna;"
                }
            ],
            [
                {
                    "verse": "e il governo de' pesci fortuna,"
                },
                {
                    "verse": "in paradiso in tenebrosi mali"
                },
                {
                    "verse": "e in tranquillo stato l'infernali,"
                },
                {
                    "verse": "dov'è ogni gloria passïon s'aduna;"
                }
            ],
            [
                {
                    "verse": "e non si vedrà in ciel pianeti o stelle,"
                },
                {
                    "verse": "il mare arato com'è or la terra,"
                },
                {
                    "verse": "e primavera priva di verdezze,"
                }
            ],
            [
                {
                    "verse": "e sarà una tutte le favelle"
                },
                {
                    "verse": "e io sentirò l'alma in mortal guerra,"
                },
                {
                    "verse": "prima ch'i' lasci le tuo gran bellezze."
                }
            ]
        ]
    }
]