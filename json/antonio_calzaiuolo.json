    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000166",
        "author": "Antonio Calzaiuolo",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "La volubil fortuna, i cieli e' fati,"
                },
                {
                    "verse": "il ver che molto ha forza e la ragione"
                },
                {
                    "verse": "han fatto far di nuovo un paragone"
                },
                {
                    "verse": "d'un or, ch'è ventiquattro o più carati."
                }
            ],
            [
                {
                    "verse": "Adunque, ingegni divini, almi beati,"
                },
                {
                    "verse": "diventate nel dire ognun precone,"
                },
                {
                    "verse": "porgendo laulde e gran reputazione"
                },
                {
                    "verse": "al nostro Marïotto Davanzati,"
                }
            ],
            [
                {
                    "verse": "c'ha aùto appetto un nuovo barberesco"
                },
                {
                    "verse": "veloce molto, e da lui venne il gaggio,"
                },
                {
                    "verse": "nol credendo trovar nel corso in punto;"
                }
            ],
            [
                {
                    "verse": "ma è ereda di messer Francesco:"
                },
                {
                    "verse": "gli diè le mosse e del terren vantaggio,"
                },
                {
                    "verse": "poi lasciò al prato, ed egli al palio è giunto."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000166",
        "author": "Antonio Calzaiuolo",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Muovasi un tigre o qualche caldo leo,"
                },
                {
                    "verse": "che strazi ogni tuo membro e sbrani el core,"
                },
                {
                    "verse": "e Giove fabbricar faccia un furore,"
                },
                {
                    "verse": "che fulmini el tuo corpo iniquo e reo."
                }
            ],
            [
                {
                    "verse": "Sentir postù di morsi di Tideo,"
                },
                {
                    "verse": "da poi che se' più falso traditore"
                },
                {
                    "verse": "che non fu Gano o 'l troiano Antenòre,"
                },
                {
                    "verse": "o Giuda Antipateo o Tolomeo."
                }
            ],
            [
                {
                    "verse": "Diventare postù più oscuro e magro"
                },
                {
                    "verse": "che 'l popol ch'assediò Vespesïano;"
                },
                {
                    "verse": "faccino i can di te qual d'Ateonne."
                }
            ],
            [
                {
                    "verse": "Arder postù com'arse Meleagro,"
                },
                {
                    "verse": "muggir com fece il bu' cicilïano"
                },
                {
                    "verse": "e poi senta del remo di Caronne."
                }
            ],
            [
                {
                    "verse": "O volto di Gorgonne,"
                },
                {
                    "verse": "ruinar postù fin nella Giudecca,"
                },
                {
                    "verse": "da poi che 'n te pietà è rasa e secca!"
                }
            ]
        ]
    }
]