        "url": "http://www.bibliotecaitaliana.it/testo/bibit000627",
        "text": [
            [
                {
                    "verse": "Offeso un giorno Amore"
                },
                {
                    "verse": "Da un mal accorto amante"
                },
                {
                    "verse": "Giur\u00f2 a la madre innante"
                },
                {
                    "verse": "Che avr\u00eca dell'offensore"
                },
                {
                    "verse": "Dato un esempio eterno;"
                },
                {
                    "verse": "Indi scese all'inferno."
                },
                {
                    "verse": "Ol\u00e0 monarca immite"
                },
                {
                    "verse": "Del tenebroso Dite,"
                },
                {
                    "verse": "Se di teneri affetti"
                },
                {
                    "verse": "E d'ignoti diletti"
                },
                {
                    "verse": "Ti fui largo una volta,"
                },
                {
                    "verse": "Oggi tu pur m'ascolta."
                },
                {
                    "verse": "Fra le crudeli pene"
                },
                {
                    "verse": "Che la tua chiostra tiene,"
                },
                {
                    "verse": "Qual cagiona pi\u00f9 pianti"
                },
                {
                    "verse": "A i delicati amanti?"
                },
                {
                    "verse": "Qual \u00e8 che pi\u00f9 li coce,"
                },
                {
                    "verse": "E qual \u00e8 la pi\u00f9 atroce?"
                },
                {
                    "verse": "Or quella a me concedi"
                },
                {
                    "verse": "Per punire un mortale."
                },
                {
                    "verse": "Amor, ci\u00f2 che tu chiedi"
                },
                {
                    "verse": "Si faccia nel mio regno,"
                },
                {
                    "verse": "Disse il prence infernale;"
                },
                {
                    "verse": "E con la man die' segno."
                },
                {
                    "verse": "Ecco per l'ombre oscure"
                },
                {
                    "verse": "Tosto venir le cure"
                },
                {
                    "verse": "A cruciar destinate"
                },
                {
                    "verse": "L'anime innamorate."
                },
                {
                    "verse": "V'\u00e8 il rigore indiscreto,"
                },
                {
                    "verse": "V'\u00e8 il capriccio inquieto,"
                },
                {
                    "verse": "Lo sdegno minacciante,"
                },
                {
                    "verse": "Lo scherno umil\u00edante,"
                },
                {
                    "verse": "La dubbiosa incostanza,"
                },
                {
                    "verse": "L'ansiosa lontananza,"
                },
                {
                    "verse": "Il rifiuto ostinato,"
                },
                {
                    "verse": "Il bando disperato."
                },
                {
                    "verse": "Sull'adunata schiera"
                },
                {
                    "verse": "Incerto Amor pendea:"
                },
                {
                    "verse": "E far la barba nera"
                },
                {
                    "verse": "Plutone sorridea;"
                },
                {
                    "verse": "E cos\u00ec gli dicea:"
                },
                {
                    "verse": "Ben abile tu sei,"
                },
                {
                    "verse": "Domator de gli Dei,"
                },
                {
                    "verse": "A scegliere i piaceri"
                },
                {
                    "verse": "Pi\u00f9 graditi a i viventi;"
                },
                {
                    "verse": "Ma non sai fra i tormenti"
                },
                {
                    "verse": "Conoscere i pi\u00f9 fieri."
                },
                {
                    "verse": "Non vedi, fra i tormenti"
                },
                {
                    "verse": "Che la mia chiostra tiene,"
                },
                {
                    "verse": "Con tranquilla apparenza"
                },
                {
                    "verse": "La fredda indifferenza?"
                },
                {
                    "verse": "Quella \u00e8 il maggior cimento"
                },
                {
                    "verse": "De gli animi costanti;"
                },
                {
                    "verse": "Quella \u00e8 il peggior tormento"
                },
                {
                    "verse": "De i delicati amanti."
                },
                {
                    "verse": "Ahim\u00e8! l'irato Dio"
                },
                {
                    "verse": "Prese quel mostro rio;"
                },
                {
                    "verse": "E con mano sdegnata"
                },
                {
                    "verse": "Ad abitar lo pose"
                },
                {
                    "verse": "Ne le luci vezzose"
                },
                {
                    "verse": "De la fanciulla amata."
                },
                {
                    "verse": "Lo sventurato amante"
                },
                {
                    "verse": "Sofferto avr\u00eca costante"
                },
                {
                    "verse": "Il rigore indiscreto,"
                },
                {
                    "verse": "Il capriccio inquieto,"
                },
                {
                    "verse": "Lo sdegno minacciante"
                },
                {
                    "verse": "Lo scherno umil\u00edante,"
                },
                {
                    "verse": "La dubbiosa incostanza,"
                },
                {
                    "verse": "L'ansiosa lontananza,"
                },
                {
                    "verse": "Il rifiuto ostinato,"
                },
                {
                    "verse": "Il bando disperato;"
                },
                {
                    "verse": "Ma non pote' soffrire"
                },
                {
                    "verse": "La tranquilla apparenza;"
                },
                {
                    "verse": "E lo fece morire"
                },
                {
                    "verse": "La fredda indifferenza."
                }
            ]
        ]
    },
    {
        "author": "Giuseppe Parini",
        "work": "Canzonette",
        "title": "IV.",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000627",
        "text": [
            [
                {
                    "verse": "Stava un giorno Citerea"
                },
                {
                    "verse": "Di Vulcano a la fucina:"
                },
                {
                    "verse": "N\u00e9 difender si sapea"
                },
                {
                    "verse": "Da la fiamma a lei vicina;"
                },
                {
                    "verse": "N\u00e9 salavar le fresche rose"
                },
                {
                    "verse": "De le gote sue vezzose."
                },
                {
                    "verse": "Opponeva or destra or manca"
                },
                {
                    "verse": "Al gran foco ivi raccolto;"
                },
                {
                    "verse": "Ma la man picciola e bianca"
                },
                {
                    "verse": "Vano scudo era al bel volto:"
                },
                {
                    "verse": "Ch\u00e9 feriva e volto e mano"
                },
                {
                    "verse": "La gran vampa di Vulcano."
                },
                {
                    "verse": "De la Dea vide i tormenti;"
                },
                {
                    "verse": "A pietade Amor si mosse;"
                },
                {
                    "verse": "E dell'ali rinascenti"
                },
                {
                    "verse": "Una subito strapposse;"
                },
                {
                    "verse": "Poi con atto dolce e caro:"
                },
                {
                    "verse": "Ecco, disse, il tuo riparo."
                },
                {
                    "verse": "Seren\u00f2 venere il ciglio;"
                },
                {
                    "verse": "E il celeste almo sorriso"
                },
                {
                    "verse": "Rivolgendo al caro figlio"
                },
                {
                    "verse": "Abbassossi, e il baci\u00f2 in viso;"
                },
                {
                    "verse": "Poi fe' schermo al gran calore"
                },
                {
                    "verse": "Con quell'ala dell'Amore."
                },
                {
                    "verse": "Ma la Dea sagace apprese,"
                },
                {
                    "verse": "Riparando il foco ardente,"
                },
                {
                    "verse": "Di quel vago e novo arnese"
                },
                {
                    "verse": "Ad usar pi\u00f9 dolcemente:"
                },
                {
                    "verse": "Onde rise il Nume armato"
                },
                {
                    "verse": "Che le stava all'altro lato."
                },
                {
                    "verse": "Ella i guardi a lui volgeva,"
                },
                {
                    "verse": "All'orecchio gli parlava,"
                },
                {
                    "verse": "E il bel volto nascondeva"
                },
                {
                    "verse": "Dal marito che guardava,"
                },
                {
                    "verse": "E cos\u00ec sfogava il core"
                },
                {
                    "verse": "Sotto all'ala dell'Amore."
                },
                {
                    "verse": "Spesso ancor si ricopria"
                },
                {
                    "verse": "La met\u00e0 de le pupille;"
                },
                {
                    "verse": "E pi\u00f9 forte l'assal\u00eca"
                },
                {
                    "verse": "Condensando le faville"
                },
                {
                    "verse": "Che ferien con pi\u00f9 rigore"
                },
                {
                    "verse": "Sotto all'ala dell'Amore."
                },
                {
                    "verse": "Or dal sommo de' bei labri"
                },
                {
                    "verse": "Accennava i molli baci:"
                },
                {
                    "verse": "Ora usc\u00ecen de' bei cinabri"
                },
                {
                    "verse": "Sospiretti o ghigni auadaci,"
                },
                {
                    "verse": "Or nasceva un bel rossore"
                },
                {
                    "verse": "Sotto all'ala dell'Amore."
                },
                {
                    "verse": "Tale in tanto che Vulcano"
                },
                {
                    "verse": "Fabbricava arme a gli Dei"
                },
                {
                    "verse": "Citerea cos\u00ec pian piano"
                },
                {
                    "verse": "Accresceva i suoi trofei"
                },
                {
                    "verse": "Sopra il Nume vincitore"
                },
                {
                    "verse": "Sotto all'ala dell'Amore."
                },
                {
                    "verse": "Belle mie, voi m'intendete:"
                },
                {
                    "verse": "Dell'Amor l'ala son io."
                },
                {
                    "verse": "Come Venere potete"
                },
                {
                    "verse": "E spiegar pi\u00f9 d'un des\u00eco"
                },
                {
                    "verse": "E temprar l'occulto ardore"
                },
                {
                    "verse": "Sotto all'ala dell'Amore."
                }
            ]
        ]
    },
    {
        "author": "Giuseppe Parini",
        "work": "Canzonette",
        "title": "V.",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000627",
        "text": [
            [
                {
                    "verse": "Nel mirar quegli occhi tuoi"
                },
                {
                    "verse": "S\u00ec soavi al giro, al guardo,"
                },
                {
                    "verse": "Tirsi mio, mi struggo, ed ardo,"
                },
                {
                    "verse": "E ritorno a' miei sospir."
                },
                {
                    "verse": "Ma pi\u00f9 ancor mi trema il core"
                },
                {
                    "verse": "Di tua voce al dolce incanto"
                },
                {
                    "verse": "Che confusa insiem col pianto"
                },
                {
                    "verse": "Notte e d\u00ec mi fa languir."
                },
                {
                    "verse": "Ah! perdona: il mio destino"
                },
                {
                    "verse": "S\u00ec crudel che te mi rende,"
                },
                {
                    "verse": "Che se Amor il cor m'accende,"
                },
                {
                    "verse": "Pur gi\u00e0 mai si piegher\u00e0."
                },
                {
                    "verse": "A quel laccio avvinata sono,"
                },
                {
                    "verse": "A quel barbaro dovere"
                },
                {
                    "verse": "Che avvelena ogni piacere"
                },
                {
                    "verse": "Che ci vieta aver piet\u00e0."
                },
                {
                    "verse": "Io giurai sull'ara un giorno"
                },
                {
                    "verse": "Fede eterna ad altro oggetto,"
                },
                {
                    "verse": "E giurai che un solo affetto"
                },
                {
                    "verse": "Io nodtrito avrei nel cor."
                },
                {
                    "verse": "Nel tuo dolce, e vago aspetto"
                },
                {
                    "verse": "Poi m'avvenni, e s\u00ec mi piacque"
                },
                {
                    "verse": "Che s'estinse il primo, e nacque"
                },
                {
                    "verse": "Un pi\u00f9 forte e vivo ardor."
                },
                {
                    "verse": "Fra la tema ed il des\u00eco"
                },
                {
                    "verse": "M'agitar gl'incerti affetti:"
                },
                {
                    "verse": "Cari sguardi, e molli detti"
                },
                {
                    "verse": "Mal sostenne la mia fe."
                },
                {
                    "verse": "Io sperai trovarti un core"
                },
                {
                    "verse": "Non eguale al bel sembiante:"
                },
                {
                    "verse": "Chiesi al ciel che un incostante"
                },
                {
                    "verse": "Ritrovar potessi in te."
                },
                {
                    "verse": "Ma fu sordo a' voti miei"
                },
                {
                    "verse": "Anche il cielo, e parve allora"
                },
                {
                    "verse": "Che un bel cor che c'innamora"
                },
                {
                    "verse": "Innamori il cielo ancor."
                },
                {
                    "verse": "De' suoi pregi adorno ei viene"
                },
                {
                    "verse": "A far guerra al debol sesso,"
                },
                {
                    "verse": "Poi si chiama folle eccesso"
                },
                {
                    "verse": "Ci\u00f2 ch'\u00e8 premio al suo valor."
                },
                {
                    "verse": "Ah! mio Tirsi, se tu m'ami,"
                },
                {
                    "verse": "Lascia al men ch'io viva in pace:"
                },
                {
                    "verse": "Lo sai pur che a quel che piace"
                },
                {
                    "verse": "Lieve scudo \u00e8 la virt\u00f9,"
                },
                {
                    "verse": "Che dell'anime pi\u00f9 belle"
                },
                {
                    "verse": "Volge i moti a suo talento,"
                },
                {
                    "verse": "Ma s'invola a un solo accento"
                },
                {
                    "verse": "De la calda giovent\u00f9."
                },
                {
                    "verse": "Tu ben sai che un sol pensiero"
                },
                {
                    "verse": "Colpa atroce in me diviene,"
                },
                {
                    "verse": "Che il des\u00eco del proprio bene"
                },
                {
                    "verse": "\u00c8 delitto assai maggior;"
                },
                {
                    "verse": "Che se cedo, ed abbandono"
                },
                {
                    "verse": "Ad Amore i sensi miei,"
                },
                {
                    "verse": "Mi condannano gli Dei"
                },
                {
                    "verse": "Al rimorso, ed al terror."
                },
                {
                    "verse": "Ah! mio Tirsi, se tu m'ami"
                },
                {
                    "verse": "Deh t'invola al pianto mio!..."
                },
                {
                    "verse": "Non mi dar l'estremo addio"
                },
                {
                    "verse": "Che soffrirlo il cor non sa."
                },
                {
                    "verse": "Ma tu piangi, e tu t'arresti?"
                },
                {
                    "verse": "Tu miguardi, e poi sospiri?"
                },
                {
                    "verse": "Sempre intorno a me t'aggiri?"
                },
                {
                    "verse": "E non hai di me piet\u00e0?"
                },
                {
                    "verse": "Per che mai mi stringi al seno"
                },
                {
                    "verse": "Cos\u00ec pallido, e tremante?"
                },
                {
                    "verse": "Ah! crudele in questo istante"
                },
                {
                    "verse": "Tu divieni il mio martir."
                },
                {
                    "verse": "Se una dura ingiusta legge"
                },
                {
                    "verse": "Non frenasse in me l'ardore,"
                },
                {
                    "verse": "Rivedrei quel tuo pallore"
                },
                {
                    "verse": "Nell'eccesso del gioir."
                },
                {
                    "verse": "Ah! perdona: il mio destino"
                },
                {
                    "verse": "S\u00ec crudel con te mi rende,"
                },
                {
                    "verse": "Che se Amore il cor m'accende"
                },
                {
                    "verse": "Pur gi\u00e0 mai si piegher\u00e0."
                },
                {
                    "verse": "A quel laccio avvinta sono,"
                },
                {
                    "verse": "A quel barbaro dovere"
                },
                {
                    "verse": "Che mi toglie ogni piacere"
                },
                {
                    "verse": "De la cara libert\u00e0."
