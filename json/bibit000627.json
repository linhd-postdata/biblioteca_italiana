        "text": [
            [
                {
                    "verse": "La vaga Primavera"
                },
                {
                    "verse": "Ecco che a noi sen viene;"
                },
                {
                    "verse": "E sparge le serene"
                },
                {
                    "verse": "Aure di molli odori."
                },
                {
                    "verse": "L'erbe novelle e i fiori"
                },
                {
                    "verse": "Ornano il colle e il prato."
                },
                {
                    "verse": "Torna a veder l'amato"
                },
                {
                    "verse": "Nido la rondinella."
                },
                {
                    "verse": "E torna la sorella"
                },
                {
                    "verse": "Di lei a i pianti gravi:"
                },
                {
                    "verse": "E tornano a i soavi"
                },
                {
                    "verse": "Baci le tortorelle."
                },
                {
                    "verse": "Escon le pecorelle"
                },
                {
                    "verse": "Del lor soggiorno odioso;"
                },
                {
                    "verse": "E cercan l'odoroso"
                },
                {
                    "verse": "Timo di balza in balza."
                },
                {
                    "verse": "La pastorella scalza"
                },
                {
                    "verse": "Ne vien con esse a paro;"
                },
                {
                    "verse": "Ne vien cantando il caro"
                },
                {
                    "verse": "Nome del suo pastore."
                },
                {
                    "verse": "Ed ei, seguendo Amore,"
                },
                {
                    "verse": "Volge ove il canto sente;"
                },
                {
                    "verse": "E coglie la innocente"
                },
                {
                    "verse": "Ninfa sul fresco rio."
                },
                {
                    "verse": "Oggi del suo des\u00eco"
                },
                {
                    "verse": "Amore infiamma il mondo:"
                },
                {
                    "verse": "Amore il suo giocondo"
                },
                {
                    "verse": "Senso a le cose inspira."
                },
                {
                    "verse": "Sola il dolor non mira"
                },
                {
                    "verse": "Clori del suo fedele:"
                },
                {
                    "verse": "E sol quella crudele"
                },
                {
                    "verse": "Anima non sospira."
                }
            ]
        ]
    },
    {
        "author": "Giuseppe Parini",
        "work": "Canzonette",
        "title": "II.",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000627",
        "text": [
            [
                {
                    "verse": "Volano i giorni rapidi"
                },
                {
                    "verse": "Del caro viver mio:"
                },
                {
                    "verse": "E giunta in sul pend\u00eco"
                },
                {
                    "verse": "Precipita l'et\u00e0."
                },
                {
                    "verse": "Le belle oim\u00e8! che al fingere"
                },
                {
                    "verse": "Han lingua cos\u00ec presta"
                },
                {
                    "verse": "Sol mi ripeton questa"
                },
                {
                    "verse": "Ingrata verit\u00e0."
                },
                {
                    "verse": "Con quelle occhiate mutole"
                },
                {
                    "verse": "Con quel contegno avaro"
                },
                {
                    "verse": "Mi dicono assai chiaro:"
                },
                {
                    "verse": "Noi non siam pi\u00f9 per te."
                },
                {
                    "verse": "E fuggono e folleggiano"
                },
                {
                    "verse": "Tra giovent\u00f9 vivace;"
                },
                {
                    "verse": "E rendonvi loquace"
                },
                {
                    "verse": "L'occhio la mano e il pi\u00e8."
                },
                {
                    "verse": "Che far? Degg'io di lagrime"
                },
                {
                    "verse": "Bagnar per questo il ciglio?"
                },
                {
                    "verse": "Ah no; miglior consiglio"
                },
                {
                    "verse": "\u00c8 di godere ancor."
                },
                {
                    "verse": "Se gi\u00e0 di mirti teneri"
                },
                {
                    "verse": "Colsi mia parte in Gnido,"
                },
                {
                    "verse": "Lasciamo che a quel lido"
                },
                {
                    "verse": "Vada con altri Amor."
                },
                {
                    "verse": "Volgan le spalle candide"
                },
                {
                    "verse": "Volgano a me le belle:"
                },
                {
                    "verse": "Ogni piacer con elle"
                },
                {
                    "verse": "Non se ne parte al fin."
                },
                {
                    "verse": "A Bacco, all'Amicizia"
                },
                {
                    "verse": "Sacro i venturi giorni."
                },
                {
                    "verse": "Cadano i mirti; e s'orni"
                },
                {
                    "verse": "D'ellera il misto crin."
                },
                {
                    "verse": "Che fai su questa cetera,"
                },
                {
                    "verse": "Corda che amor sonasti?"
                },
                {
                    "verse": "Male al tenor contrasti"
                },
                {
                    "verse": "Del novo mio piacer."
                },
                {
                    "verse": "Or di cantar dilettami"
                },
                {
                    "verse": "Tra' miei giocondi amici,"
                },
                {
                    "verse": "Augurj a lor felici"
                },
                {
                    "verse": "Versando dal bicchier."
                },
                {
                    "verse": "Fugge la instabil Venere"
                },
                {
                    "verse": "Con la stagion de' fiori:"
                },
                {
                    "verse": "Ma tu Li\u00e8o ristori"
                },
                {
                    "verse": "Quando il dicembre usc\u00ec."
                },
                {
                    "verse": "Amor con l'et\u00e0 fervida"
                },
                {
                    "verse": "Convien che si dilegue;"
                },
                {
                    "verse": "Ma l'Amist\u00e0 ne segue"
                },
                {
                    "verse": "Fino all'estremo d\u00ec."
                },
                {
                    "verse": "Le belle ch'or s'involano"
                },
                {
                    "verse": "Schife da noi lontano"
                },
                {
                    "verse": "Verranci allor pian piano"
                },
                {
                    "verse": "Lor brindisi ad offrir."
                },
                {
                    "verse": "E noi compagni amabili"
                },
                {
                    "verse": "Che far con esse allora?"
                },
                {
                    "verse": "Seco un bicchiere ancora"
                },
                {
                    "verse": "Bevere e poi morir."
                }
            ]
        ]
    },
    {
