[
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "I",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "\u00abO madre patria, (deh) dimmi ov'or ti truovi!\u00bb"
                },
                {
                    "verse": "\u00abBrieve rispondo: infra crudeli artigli;"
                },
                {
                    "verse": "e nati in me son questi e son mia figli,"
                },
                {
                    "verse": "n\u00e9 veggio ch'a piat\u00e0 nessun si muovi."
                }
            ],
            [
                {
                    "verse": "Om\u00e8, che pi\u00f9 non \u00e8 alcun che pruovi"
                },
                {
                    "verse": "in su levarmi, o cura di me pigli!"
                },
                {
                    "verse": "Morte m'ha tolto Cosmo e' suoi consigli,"
                },
                {
                    "verse": "e pi\u00f9 non \u00e8 chi con amor mi covi."
                }
            ],
            [
                {
                    "verse": "Cosmo sol cerco e' vecchi cives mei,"
                },
                {
                    "verse": "delle cui morti ancor mi dolgo e ploro,"
                },
                {
                    "verse": "perch\u00e9 lor opre e lor m'orn\u00e2r la chioma"
                }
            ],
            [
                {
                    "verse": "di mia grandezza; e veggio gli anni rei"
                },
                {
                    "verse": "in me rivolver, cagion di costoro,"
                },
                {
                    "verse": "ch'a quel mi menan qual i' veggio Roma."
                }
            ],
            [
                {
                    "verse": "Gravar sento la soma"
                },
                {
                    "verse": "d'ogni crudel mio male, e chi sussidio"
                },
                {
                    "verse": "dar mi pu\u00f2 mi riprieme (odia) ed ha 'n fastidio\u00bb."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "II",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "De[l] basso tempo, al quale i' penso ancora,"
                },
                {
                    "verse": "quanto mi stabilisce ben Fortuna"
                },
                {
                    "verse": "in darmi per compagna sol quell'una"
                },
                {
                    "verse": "ch'al mondo sola fu, e ch'or la plora!"
                }
            ],
            [
                {
                    "verse": "\u00c8 'l secol nostro tristo, e poi che fora"
                },
                {
                    "verse": "di vita si part\u00ec, quanti pur una"
                },
                {
                    "verse": "sola mia mente amar pensier raguna,"
                },
                {
                    "verse": "ch'a ci\u00f2 pensando vivo e moro ognora!"
                }
            ],
            [
                {
                    "verse": "O fermo cogitar, che tanto amaro"
                },
                {
                    "verse": "drieto a dolci pensier mi rechi poi,"
                },
                {
                    "verse": "rotta mia fantasia ov'io la veggio!"
                }
            ],
            [
                {
                    "verse": "O sommo mio riposo, o lume chiaro,"
                },
                {
                    "verse": "col qual gran tempo i' vissi, non t'annoi"
                },
                {
                    "verse": "oma' chiamarmi al dis\u00efato seggio!"
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "III",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Amor, che dentro al cor l'afflitta mente"
                },
                {
                    "verse": "combatte e sforza all'usato cammino,"
                },
                {
                    "verse": "onde, s'i' partir posso, qual destino"
                },
                {
                    "verse": "\u00e8 che mi tenga in questo foco ardente?"
                }
            ],
            [
                {
                    "verse": "S'i' son cagion di ci\u00f2, dunque sovente"
                },
                {
                    "verse": "degna cosa \u00e8 che nel martiro, infino"
                },
                {
                    "verse": "ch'i' gli occhi chiuda, stia, e 'l capo chino"
                },
                {
                    "verse": "in terra posi fra la morta gente."
                }
            ],
            [
                {
                    "verse": "Ma quando e' fia, chi biasmar mi vuole,"
                },
                {
                    "verse": "se 'l ben ch'i' ho da lei s'acquista e viene"
                },
                {
                    "verse": "che poi la Morte mi fa viver sempre?"
                }
            ],
            [
                {
                    "verse": "O divin guardo, onde mia alma tole"
                },
                {
                    "verse": "principio, mezzo e fin d'ogni suo bene,"
                },
                {
                    "verse": "fa' che 'n tal foco mi riscaldi e tempre!"
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "IV",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Che fai, che pensi, animo mio stanco?"
                },
                {
                    "verse": "aver mai posa in questo mondo o pace?"
                },
                {
                    "verse": "Se questo credi, oh, qual pensier fallace"
                },
                {
                    "verse": "ti lega e [an]noda in quel ch'ognor vien manco!"
                }
            ],
            [
                {
                    "verse": "I' ma' non vidi sotto 'l cielo unquanco"
                },
                {
                    "verse": "gi\u00e0 pi\u00f9 cosa mortal, salv' una face"
                },
                {
                    "verse": "da etternar doversi; e pur rapace"
                },
                {
                    "verse": "Morte gli aperse l'uno e l'altro fianco."
                }
            ],
            [
                {
                    "verse": "Se tu 'l conosci e volar vedi il tempo,"
                },
                {
                    "verse": "la barca tua tarlata e pien di fori"
                },
                {
                    "verse": "e per et\u00e0 increspato e bianchi i peli,"
                }
            ],
            [
                {
                    "verse": "om\u00e8, che fai? Non aspettar pi\u00f9 tempo;"
                },
                {
                    "verse": "riconosci il Fattore e' primi amori,"
                },
                {
                    "verse": "acci\u00f2 che pi\u00f9 con lei ritorni a' cieli."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "V",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "I' veggio nella mente mia inserte"
                },
                {
                    "verse": "l'efige del mio ben, s\u00ec come equale"
                },
                {
                    "verse": "gi\u00e0 mai la vedessi; e pur le mortale"
                },
                {
                    "verse": "membra sue belle in terra son coperte."
                }
            ],
            [
                {
                    "verse": "I' la riveggio, ed ella con [i]sperte"
                },
                {
                    "verse": "parole sue liete: \u00abVedi queste ale"
                },
                {
                    "verse": "da su salire alla vita immortale?"
                },
                {
                    "verse": "Gi\u00f9 le sperai e su le truovo certe;"
                }
            ],
            [
                {
                    "verse": "perch\u00e9 ben f\u00f9r volte mi' opre a Dio,"
                },
                {
                    "verse": "mentre ch'i' vissi gi\u00f9 con teco insieme,"
                },
                {
                    "verse": "or le fr\u00fcisco, com'\u00e8 suo volere."
                }
            ],
            [
                {
                    "verse": "Solo lasciarti e 'l tuo lagrimar pio"
                },
                {
                    "verse": "molesti assai mi f\u00f9r, ma pur la speme"
                },
                {
                    "verse": "su m'inclinava al mio Fattor vedere\u00bb."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "VI",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "\u00abO fresche erbette, gentil fronde e fiori"
                },
                {
                    "verse": "p\u00efante tanto degne, vaghe e belle,"
                },
                {
                    "verse": "pini e abeti, lauri e mortelle,"
                },
                {
                    "verse": "che s\u00e8te scoloriti e senza odori,"
                }
            ],
            [
                {
                    "verse": "o chiare fonti, li vostri licori"
                },
                {
                    "verse": "turbidi veggio, e di voi chiare stelle"
                },
                {
                    "verse": "obbumbrar raggi, gli uomini e donzelle"
                },
                {
                    "verse": "di lagrime sol piene e di dolori;"
                }
            ],
            [
                {
                    "verse": "se vostra pena a me lice 'l sentire"
                },
                {
                    "verse": "e la cagion, deh, non me lo celate,"
                },
                {
                    "verse": "ch'i' con voi pianga e di dolor mi pasca!\u00bb"
                }
            ],
            [
                {
                    "verse": "\u00abTua donna \u00e8 morta e noi ci fa languire,"
                },
                {
                    "verse": "per\u00f2 tu sempre e noi staren velate,"
                },
                {
                    "verse": "fin ch'al gran d\u00ec pi\u00f9 bella ella rinasca\u00bb."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "VII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Tacito in fra me, pensando come,"
                },
                {
                    "verse": "nella stagion che 'l sol la terra scalda"
                },
                {
                    "verse": "e ogni duro ghiaccio strugge e salda,"
                },
                {
                    "verse": "gelide divenisson le mie chiome,"
                }
            ],
            [
                {
                    "verse": "nel crudo verno, quando l'erbe dome"
                },
                {
                    "verse": "sono dal ghiaccio e le pioventi acque in falda,"
                },
                {
                    "verse": "poi come chiar cristallo in s\u00e9 le salda,"
                },
                {
                    "verse": "in foco trasformarsi (mie) membra e nome."
                }
            ],
            [
                {
                    "verse": "N\u00e9 doppo questo so quel ch'io rimanga,"
                },
                {
                    "verse": "ombra o fummo, o spirto morto o vivo,"
                },
                {
                    "verse": "ma pur, quel ch'io mi sia, pena sento,"
                }
            ],
            [
                {
                    "verse": "sono e sar\u00f2; Morte spicca o franga"
                },
                {
                    "verse": "quel ch'io resti in tutto; faccil per dio,"
                },
                {
                    "verse": "ch\u00e9 dolce \u00e8 Morte pi\u00f9 che mal tormento."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "VIII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Piangete voi omai quanto conviense;"
                },
                {
                    "verse": "di lagrime farete fonti e rivi,"
                },
                {
                    "verse": "spirti gentil, poi che gli occhi divi"
                },
                {
                    "verse": "Morte crudel da voi gli tolse e spense!"
                }
            ],
            [
                {
                    "verse": "E tu, fratel, con lagrime a ci\u00f2 dense,"
                },
                {
                    "verse": "cagion non hai di pianger pi\u00f9 que' vivi"
                },
                {
                    "verse": "lumi, de' quali or gli occhi tuo son privi,"
                },
                {
                    "verse": "ch'eran tuo sol, tuo speme e tua difensa."
                }
            ],
            [
                {
                    "verse": "Io per me piango e confortar vorr\u00eati"
                },
                {
                    "verse": "con dirti ch'io la vidi in tanta pace"
                },
                {
                    "verse": "nell'urna entrar che ben parea dormire."
                }
            ],
            [
                {
                    "verse": "E con s\u00ec dolci risi in atti quieti"
                },
                {
                    "verse": "parea dicessi a tutti: \u00abState in pace,"
                },
                {
                    "verse": "ch'al ciel, dove aspettata son, vo' gire\u00bb."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "IX",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Non posso pi\u00f9 tenere in tal tormento"
                },
                {
                    "verse": "l'amara vita mia,"
                },
                {
                    "verse": "po' che Fortuna ria"
                },
                {
                    "verse": "combatte el cor con s\u00ec contrario vento."
                }
            ],
            [
                {
                    "verse": "Quando la luce del perduto sguardo"
                },
                {
                    "verse": "mi si rassembra un poco,"
                },
                {
                    "verse": "sogno il dolente gioco"
                },
                {
                    "verse": "e parmi co' sospir vivere in pace."
                }
            ],
            [
                {
                    "verse": "Questo non dura, ond'io sfavillo e ardo,"
                },
                {
                    "verse": "chiamando Morte a fioco,"
                },
                {
                    "verse": "ch\u00e9 sa sortirmi il loco"
                },
                {
                    "verse": "ov'ogni speme mi riman fallace."
                }
            ],
            [
                {
                    "verse": "E quel pensar di lei, che s\u00ec mi piace"
                },
                {
                    "verse": "e fammi adolcir tanto,"
                },
                {
                    "verse": "tosto si volta in pianto:"
                },
                {
                    "verse": "diaccio nel foco e 'l viver non m'\u00e8 spento."
                }
            ]
        ]
    },
    {
        "author": "Carlo de' Medici",
        "work": "Poesie",
        "title": "X",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001025",
        "text": [
            [
                {
                    "verse": "Pensando agli anni, mesi e giorni, ch'io"
                },
                {
                    "verse": "vivuto sono e sciolto da quel raggio"
                },
                {
                    "verse": "che lieto mi tenea e 'l viver saggio,"
                },
                {
                    "verse": "involvendo mie cor con suo desio,"
                }
            ],
            [
                {
                    "verse": "l'ora ch'i' 'l persi turb\u00f2 il viver mio."
                },
                {
                    "verse": "In tutto vidi e, per alcun v\u00efaggio"
                },
                {
                    "verse": "ch'i' facci o cerchi, truovo ancor messaggio"
                },
                {
                    "verse": "che pace rechi a tal dolor s\u00ec rio;"
                }
            ],
            [
                {
                    "verse": "e per fuggir talvolta el mio martire"
                },
                {
                    "verse": "e posa dare all'affannata mente,"
                },
                {
                    "verse": "e verdi colli e templi vo cercando."
                }
            ],
            [
                {
                    "verse": "Ma dov'io vado o stia, questo servire"
                },
                {
                    "verse": "da me non parte, ma veggio sovente"
                },
                {
                    "verse": "essa formata mi si vien mostrando."
                }
            ]
        ]
    }
]