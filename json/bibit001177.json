[
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "I",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Io son nel fondo della magna altezza,"
                },
                {
                    "verse": "fra tanta broda e fra s\u00ec grasse micche,"
                },
                {
                    "verse": "che, se sol agli arnion l'odor s'apicche,"
                },
                {
                    "verse": "lasciar dovrei l'antica mia magrezza."
                }
            ],
            [
                {
                    "verse": "E mangio pan ch'ogni vil can lo sprezza;"
                },
                {
                    "verse": "el vin par distillato per limbicche"
                },
                {
                    "verse": "d'un infreddato zugo d'asin, s\u00ec che"
                },
                {
                    "verse": "la vista sola ogni stomaco spezza."
                }
            ],
            [
                {
                    "verse": "E seggo a mensa a tavola ritonda,"
                },
                {
                    "verse": "guernita d'un grembiul da dipintore;"
                },
                {
                    "verse": "suvi catini ov'io non truovo fonda."
                }
            ],
            [
                {
                    "verse": "E son in una stufa, onde un odore"
                },
                {
                    "verse": "surge, che quel delle Stinche seconda,"
                },
                {
                    "verse": "e direi me' s'i' dicessi piggiore."
                }
            ],
            [
                {
                    "verse": "Pure sperando onore,"
                },
                {
                    "verse": "paz\u00efente comporto ogn'aspro intoppo;"
                },
                {
                    "verse": "non mi dir poi cost\u00ec ch'io spendo troppo."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "II",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Va', lascia l'uccellare a quei c'han l'arte,"
                },
                {
                    "verse": "e ad altro mestiero il pensier pone,"
                },
                {
                    "verse": "e la tuo falsa e fiacca oppin\u00efone"
                },
                {
                    "verse": "per mio amor, deh, mettila in disparte!"
                }
            ],
            [
                {
                    "verse": "E s'tu pretendi che Mercurio o Marte"
                },
                {
                    "verse": "di te augumentar abbin cagione,"
                },
                {
                    "verse": "la lingua pronta, caro compagnone,"
                },
                {
                    "verse": "col magnanimo core in ogni parte"
                }
            ],
            [
                {
                    "verse": "fa' ch'abbi, e non voler esser ferrigno,"
                },
                {
                    "verse": "n\u00e9 per ragion volerci tu far chiari"
                },
                {
                    "verse": "la luna poter porre in uno scrigno;"
                }
            ],
            [
                {
                    "verse": "che gl'intelletti tuoi freddi ed avari,"
                },
                {
                    "verse": "tuo triangol, tuo quadri e tuo frittelle"
                },
                {
                    "verse": "farebbon vagillar divi e mortali."
                }
            ],
            [
                {
                    "verse": "Tu vuo' volar sanz'ali"
                },
                {
                    "verse": "e perso hai lo sparvier, ch'andar lasciasti:"
                },
                {
                    "verse": "i geti almanco ti fusson rimasti."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "IV",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Prima fie del parlar priva ogni lingua"
                },
                {
                    "verse": "e li spiriti uman del mondo cassi"
                },
                {
                    "verse": "e le stelle e' pianeti infimi e bassi"
                },
                {
                    "verse": "che del mie cor l'effigia tua s'estingua."
                }
            ],
            [
                {
                    "verse": "Che fie giammai, bench\u00e9 'n vista dipinga"
                },
                {
                    "verse": "gran crudelt\u00e0 che di seguirt' allassi,"
                },
                {
                    "verse": "ma pi\u00f9 fervente seguir\u00f2 i miei passi,"
                },
                {
                    "verse": "perch\u00e9 grande speranza mi losinga,"
                }
            ],
            [
                {
                    "verse": "dicendomi pur: \u00abSegui arditamente,"
                },
                {
                    "verse": "ch'Amor v\u00f2l che suo legge fien amate,"
                },
                {
                    "verse": "n\u00e9 a chi \u00e8 amato el non amar consente\u00bb."
                }
            ],
            [
                {
                    "verse": "Ond'io, sappiendo che mai donne amate"
                },
                {
                    "verse": "f\u00f9r qual se' tu da me s\u00ec lealmente,"
                },
                {
                    "verse": "spero ch'a te le non fien perdonate."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "V",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Cacciando per l'usata selva ov'io"
                },
                {
                    "verse": "solie d'una cervetta trovar l'orme,"
                },
                {
                    "verse": "sendo gi\u00e0 presso all'ora ch'ogn'om dorme,"
                },
                {
                    "verse": "udi' parlar con modo dolze e pio."
                }
            ],
            [
                {
                    "verse": "\u00abNon mi seguir, ch'i' t'ho posto in obblio"
                },
                {
                    "verse": "\u2014 disse \u2014 lassami star fra le mie torme,"
                },
                {
                    "verse": "perch\u00e9 Fortuna m'ha fatto conforme"
                },
                {
                    "verse": "solo a un can, che d'un ginepro usc\u00eco\u00bb."
                }
            ],
            [
                {
                    "verse": "Ond'io, ch'a seguitarla ero disposto,"
                },
                {
                    "verse": "udendo tal parlar, fu' sbigottito,"
                },
                {
                    "verse": "come \u00e8 colui cui manca 'l presuposto;"
                }
            ],
            [
                {
                    "verse": "e m'arestai e presi per partito"
                },
                {
                    "verse": "tornare addietro e s\u00ec lungi discosto"
                },
                {
                    "verse": "ch'io non turbassi un can cos\u00ec gradito."
                }
            ],
            [
                {
                    "verse": "Cos\u00ec ha aconsentito"
                },
                {
                    "verse": "la preda mia ad altri la Fortuna;"
                },
                {
                    "verse": "il perch\u00e9 non ne seguo pi\u00f9 nessuna."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "VI",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Pensier, ch'a consigliar te stesso avesti"
                },
                {
                    "verse": "troppi pungenti sproni al debil morso,"
                },
                {
                    "verse": "non conoscendo el disperato corso,"
                },
                {
                    "verse": "pel quale oggi correndo non t'arresti,"
                }
            ],
            [
                {
                    "verse": "rimembrar ti dovea che tu beesti,"
                },
                {
                    "verse": "per sorta di fortuna, l'amar sorso;"
                },
                {
                    "verse": "ben prendesti la scorza sanza 'l torso,"
                },
                {
                    "verse": "quando in sorta del mar tu ti mettesti;"
                }
            ],
            [
                {
                    "verse": "perch\u00e9, sappiendo ben che in ogni caso"
                },
                {
                    "verse": "sempre t'era Fortuna stata aversa"
                },
                {
                    "verse": "e da ogni piacer t'avea raso,"
                }
            ],
            [
                {
                    "verse": "tu non dovevi mai creder che persa"
                },
                {
                    "verse": "o tolta avessi gi\u00f9 l'ira dal naso,"
                },
                {
                    "verse": "po' che s\u00ec larga ver te l'avea immersa."
                }
            ],
            [
                {
                    "verse": "Pur, da poi che suo sferza"
                },
                {
                    "verse": "t'ha dato e d\u00e0 s\u00ec aspre discipline,"
                },
                {
                    "verse": "ch'elle ti sien sempiterne dottrine!"
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "VII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Da poi ch'a te rinata \u00e8 nuova voglia"
                },
                {
                    "verse": "d'abandonarmi e disprezzare amore"
                },
                {
                    "verse": "per ubbidire al frate confessore,"
                },
                {
                    "verse": "contento son, bench\u00e9 con greve doglia."
                }
            ],
            [
                {
                    "verse": "Ma ben ti prego, amor, che tu raccoglia"
                },
                {
                    "verse": "tanta perseveranza nel tuo core"
                },
                {
                    "verse": "ch'al buon proposto non manchi vigore:"
                },
                {
                    "verse": "non far com'a ciascun vento la foglia."
                }
            ],
            [
                {
                    "verse": "E io, s\u00ec, come pria, a' piacer tuoi"
                },
                {
                    "verse": "sempre sar\u00f2, per quanto i' vaglia o possa;"
                },
                {
                    "verse": "come al tuo servo comandar mi puoi."
                }
            ],
            [
                {
                    "verse": "Ma non voler da me far cotal mossa,"
                },
                {
                    "verse": "per arestarti con qualch'altro poi,"
                },
                {
                    "verse": "ch\u00e9 d'ogni lealt\u00e0 saresti scossa."
                }
            ],
            [
                {
                    "verse": "Perch\u00e9 fino alla fossa"
                },
                {
                    "verse": "leal saratti ognor la fede mia,"
                },
                {
                    "verse": "cos\u00ec ti prego che la tua mi sia."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "VIII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Amor, da poi ch'i' fu' fuor di tuo torma"
                },
                {
                    "verse": "ove giama' sar\u00e0 un s\u00ec fedele,"
                },
                {
                    "verse": "e ch'io mi missi al dimin delle vele,"
                },
                {
                    "verse": "non ebbi ora tranquilla, ovunque i' dorma;"
                }
            ],
            [
                {
                    "verse": "n\u00e9 spero aver, finch'io sto sotto l'orma"
                },
                {
                    "verse": "di questa iniqua setta, aspra e crudele,"
                },
                {
                    "verse": "nimica d'ogn'onor, gente infedele,"
                },
                {
                    "verse": "dal tuo regno gentil retrosa e sforma."
                }
            ],
            [
                {
                    "verse": "N\u00e9 'n tal noioso stato alcun diporto"
                },
                {
                    "verse": "trovo che la mie vita non aspreggi,"
                },
                {
                    "verse": "se non pensando a futuro conforto;"
                }
            ],
            [
                {
                    "verse": "ch\u00e9 al mie ritorno ancora infra tuo gregge"
                },
                {
                    "verse": "spero trovar colei che m'ha gi\u00e0 morto"
                },
                {
                    "verse": "e servar pi\u00f9 che mai tuo sante legge."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "IX",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Quantunque al vostro elevato, alto ingegno"
                },
                {
                    "verse": "l'infime rime mie non sien conforme,"
                },
                {
                    "verse": "non vo' per\u00f2 dal mio proposto t\u00f4rme:"
                },
                {
                    "verse": "supplico ben che non vi sieno a sdegno."
                }
            ],
            [
                {
                    "verse": "Vorre' saper se destinato segno"
                },
                {
                    "verse": "o pianeta del ciel pu\u00f2 sottoporme"
                },
                {
                    "verse": "alle 'nsidie d'Amore, inique e inorme,"
                },
                {
                    "verse": "o se si pu\u00f2, chi vol, fuggir suo regno."
                }
            ],
            [
                {
                    "verse": "Perch'\u00e8 novellamente atteso al varco,"
                },
                {
                    "verse": "fra li balcon d'una vicina e' miei,"
                },
                {
                    "verse": "la rete ov'i' provai gi\u00e0 greve incarco;"
                }
            ],
            [
                {
                    "verse": "n\u00e9 di suo forze unquanco temerei,"
                },
                {
                    "verse": "per quantunque ver me spiegassi l'arco,"
                },
                {
                    "verse": "se non fusse el bel viso di costei."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "X",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "I' provai gi\u00e0 quanti sospir s'asconde"
                },
                {
                    "verse": "sotto l'amare insegne di Cupido,"
                },
                {
                    "verse": "alle qual fui un tempo tanto fido"
                },
                {
                    "verse": "servo, ch'ogn'altro mi ven\u00ece seconde."
                }
            ],
            [
                {
                    "verse": "N\u00e9 giammai solcheggiando le sals'onde,"
                },
                {
                    "verse": "o stand' in terra ferma al proprio nido,"
                },
                {
                    "verse": "vissi, se non in amar pianto e strido,"
                },
                {
                    "verse": "tal ch'i' fu' per voltar lo spirto altronde."
                }
            ],
            [
                {
                    "verse": "Cos\u00ec sotto la suo bugiarda insegna,"
                },
                {
                    "verse": "descritta dolce, i' prova' sempre amaro:"
                },
                {
                    "verse": "foll'\u00e8 colui ch'a tal giuoco s'impegna."
                }
            ],
            [
                {
                    "verse": "Per\u00f2 con bel parlar soave e raro"
                },
                {
                    "verse": "andrai, sonetto mie, finch\u00e9 tu vegna"
                },
                {
                    "verse": "l\u00e0, dove tu abbi agio, amico caro,"
                }
            ],
            [
                {
                    "verse": "a dire, e dira' chiaro:"
                },
                {
                    "verse": "\u00abTi fa l'amico tuo che, s'tu t'inveschi,"
                },
                {
                    "verse": "gran fatto fie ch'a salvamento n'eschi\u00bb."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "XI",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "La peste ha posto in questa valle villa;"
                },
                {
                    "verse": "diletto a dillo a te c'ha fatto effetto,"
                },
                {
                    "verse": "ch'Amor, com'ero degno d'ogni obietto,"
                },
                {
                    "verse": "m'ha dato in dote s\u00ec bella Sibilla."
                }
            ],
            [
                {
                    "verse": "Di sopra s'apre e dalle stelle stilla"
                },
                {
                    "verse": "la virt\u00f9 ch'avert\u00ec d'un patto el petto,"
                },
                {
                    "verse": "ch'i' ardo: or di', Simon, se men concetto"
                },
                {
                    "verse": "\u00e8 da far qui di for, che 'n cella ancilla."
                }
            ],
            [
                {
                    "verse": "Cos\u00ec a caso amor amar m'ha preso,"
                },
                {
                    "verse": "e fami fumo il caldo che 'l d\u00ec sento,"
                },
                {
                    "verse": "tanto intinto mi veggio ov'oggi \u00e8 'l peso."
                }
            ],
            [
                {
                    "verse": "Mi fa peggio che 'n poggio ire ora al vento;"
                },
                {
                    "verse": "per\u00f2 pur par ch'i' debbia. Dubbio atteso,"
                },
                {
                    "verse": "ch'alfin el fan di vita v\u00f2to e spento."
                }
            ]
        ]
    },
    {
        "author": "Ottavante Barducci",
        "work": "Poesie",
        "title": "XII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001177",
        "text": [
            [
                {
                    "verse": "Simon, quanto pi\u00f9 penso al mondan vivere,"
                },
                {
                    "verse": "fragile e breve e pi\u00f9 ch'un vetro tenero,"
                },
                {
                    "verse": "tanto pi\u00f9 drento adiaccio e 'l viso incenero,"
                },
                {
                    "verse": "perch'io non ho chi da lui mi dilivere."
                }
            ],
            [
                {
                    "verse": "Or ch'i' stimava esser gi\u00e0 fruste e libere"
                },
                {
                    "verse": "tutte mia forze, ed io di nuovo ingenero"
                },
                {
                    "verse": "drento dal cor l'imagin d'una e venero,"
                },
                {
                    "verse": "che forse m'indurr\u00e0 pi\u00f9 spesso a scrivere."
                }
            ],
            [
                {
                    "verse": "Raggi d'un pi\u00f9 bel sol, rubini e perle"
                },
                {
                    "verse": "son le bellezze del celeste viso,"
                },
                {
                    "verse": "tal ch'i' vo per miracolo a vederle."
                }
            ],
            [
                {
                    "verse": "Simone, in questa valle \u00e8 il paradiso;"
                },
                {
                    "verse": "cred'a me: chi potessi possederle"
                },
                {
                    "verse": "non sare' mai d'ogni piacer diviso."
                }
            ],
            [
                {
                    "verse": "E in festa e 'n giuoco e 'n riso."
                },
                {
                    "verse": "in canti e balli e suon d'ogni stormento"
                },
                {
                    "verse": "si vive, s\u00ec che ogni uom ci sta contento."
                }
            ]
        ]
    }
]