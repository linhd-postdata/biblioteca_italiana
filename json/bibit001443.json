[
    {
        "author": "Giosue Carducci",
        "work": "Inno a Satana",
        "title": "A SATANA",
        "structure": "canzone",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001443",
        "text": [
            [
                {
                    "verse": "A te, de l'essere"
                },
                {
                    "verse": "Principio immenso,"
                },
                {
                    "verse": "Materia e spirito,"
                },
                {
                    "verse": "Ragione e senso;"
                }
            ],
            [
                {
                    "verse": "Mentre ne' calici"
                },
                {
                    "verse": "Il vin scintilla"
                },
                {
                    "verse": "S\u00ec come l'anima"
                },
                {
                    "verse": "Ne la pupilla;"
                }
            ],
            [
                {
                    "verse": "Mentre sorridono"
                },
                {
                    "verse": "La terra e il sole"
                },
                {
                    "verse": "E si ricambiano"
                },
                {
                    "verse": "D'amor parole,"
                }
            ],
            [
                {
                    "verse": "E corre un fremito"
                },
                {
                    "verse": "D'imene arcano"
                },
                {
                    "verse": "Da' monti e palpita"
                },
                {
                    "verse": "Fecondo il piano;"
                }
            ],
            [
                {
                    "verse": "A te disfrenasi"
                },
                {
                    "verse": "Il verso ardito,"
                },
                {
                    "verse": "Te invoco, o Satana,"
                },
                {
                    "verse": "Re del convito."
                }
            ],
            [
                {
                    "verse": "Via l'aspersorio,"
                },
                {
                    "verse": "Prete, e il tuo metro!"
                },
                {
                    "verse": "No, prete, Satana"
                },
                {
                    "verse": "Non torna in dietro!"
                }
            ],
            [
                {
                    "verse": "Vedi: la ruggine"
                },
                {
                    "verse": "Rode a Michele"
                },
                {
                    "verse": "Il brando mistico,"
                },
                {
                    "verse": "Ed il fedele"
                }
            ],
            [
                {
                    "verse": "Spennato arcangelo"
                },
                {
                    "verse": "Cade nel vano."
                },
                {
                    "verse": "Ghiacciato \u00e8 il fulmine"
                },
                {
                    "verse": "A Geova in mano."
                }
            ],
            [
                {
                    "verse": "Meteore pallide,"
                },
                {
                    "verse": "Pianeti spenti,"
                },
                {
                    "verse": "Piovono gli angeli"
                },
                {
                    "verse": "Da i firmamenti."
                }
            ],
            [
                {
                    "verse": "Ne la materia"
                },
                {
                    "verse": "Che mai non dorme,"
                },
                {
                    "verse": "Re dei i fenomeni,"
                },
                {
                    "verse": "Re de le forme,"
                }
            ],
            [
                {
                    "verse": "Sol vive Satana."
                },
                {
                    "verse": "Ei tien l'impero"
                },
                {
                    "verse": "Nel lampo tremulo"
                },
                {
                    "verse": "D'un occhio nero,"
                }
            ],
            [
                {
                    "verse": "O ver che languido"
                },
                {
                    "verse": "Sfugga e resista,"
                },
                {
                    "verse": "Od acre ed umido"
                },
                {
                    "verse": "Pr\u00f2vochi, insista."
                }
            ],
            [
                {
                    "verse": "Brilla de' grappoli"
                },
                {
                    "verse": "Nel lieto sangue,"
                },
                {
                    "verse": "Per cui la rapida"
                },
                {
                    "verse": "Gioia non langue,"
                }
            ],
            [
                {
                    "verse": "Che la fuggevole"
                },
                {
                    "verse": "Vita ristora,"
                },
                {
                    "verse": "Che il dolor proroga,"
                },
                {
                    "verse": "Che amor ne incora."
                }
            ],
            [
                {
                    "verse": "Tu spiri, o Satana,"
                },
                {
                    "verse": "Nel verso mio,"
                },
                {
                    "verse": "Se dal sen rompemi"
                },
                {
                    "verse": "Sfidando il dio"
                }
            ],
            [
                {
                    "verse": "De' rei pontefici,"
                },
                {
                    "verse": "De' re cruenti;"
                },
                {
                    "verse": "E come fulmine"
                },
                {
                    "verse": "Scuoti le menti."
                }
            ],
            [
                {
                    "verse": "A te, Agramainio,"
                },
                {
                    "verse": "Adone, Astarte,"
                },
                {
                    "verse": "E marmi vissero"
                },
                {
                    "verse": "E tele e carte,"
                }
            ],
            [
                {
                    "verse": "Quando le ioniche"
                },
                {
                    "verse": "Aure serene"
                },
                {
                    "verse": "Be\u00f2 la Venere"
                },
                {
                    "verse": "Anadiomene."
                }
            ],
            [
                {
                    "verse": "A te del Libano"
                },
                {
                    "verse": "Fremean le piante,"
                },
                {
                    "verse": "De l'alma Cipride"
                },
                {
                    "verse": "Risorto amante:"
                }
            ],
            [
                {
                    "verse": "A te ferveano"
                },
                {
                    "verse": "Le danze e i cori,"
                },
                {
                    "verse": "A te i virginei"
                },
                {
                    "verse": "Candidi amori,"
                }
            ],
            [
                {
                    "verse": "Tra le odorifere"
                },
                {
                    "verse": "Palme d'Idume,"
                },
                {
                    "verse": "Dove biancheggiano"
                },
                {
                    "verse": "Le ciprie spume."
                }
            ],
            [
                {
                    "verse": "Che val se barbaro"
                },
                {
                    "verse": "Il nazareno"
                },
                {
                    "verse": "Furor de l'agapi"
                },
                {
                    "verse": "Dal rito osceno"
                }
            ],
            [
                {
                    "verse": "Con sacra fiaccola"
                },
                {
                    "verse": "I templi t'arse"
                },
                {
                    "verse": "E i segni argolici"
                },
                {
                    "verse": "A terra sparse?"
                }
            ],
            [
                {
                    "verse": "Te accolse profugo"
                },
                {
                    "verse": "Tra gli d\u00e8i lari"
                },
                {
                    "verse": "La plebe memore"
                },
                {
                    "verse": "Ne i casolari."
                }
            ],
            [
                {
                    "verse": "Quindi un femineo"
                },
                {
                    "verse": "Sen palpitante"
                },
                {
                    "verse": "Empiendo, fervido"
                },
                {
                    "verse": "Nume ed amante,"
                }
            ],
            [
                {
                    "verse": "La strega pallida"
                },
                {
                    "verse": "D'eterna cura"
                },
                {
                    "verse": "Volgi a soccorrere"
                },
                {
                    "verse": "L'egra natura."
                }
            ],
            [
                {
                    "verse": "Tu a l'occhio immobile"
                },
                {
                    "verse": "De l'alchimista,"
                },
                {
                    "verse": "Tu de l'indocile"
                },
                {
                    "verse": "Mago a la vista,"
                }
            ],
            [
                {
                    "verse": "Del chiostro torpido"
                },
                {
                    "verse": "Oltre i cancelli,"
                },
                {
                    "verse": "Riveli i fulgidi"
                },
                {
                    "verse": "Cieli novelli."
                }
            ],
            [
                {
                    "verse": "A la Tebaide"
                },
                {
                    "verse": "Te ne le cose"
                },
                {
                    "verse": "Fuggendo, il monaco"
                },
                {
                    "verse": "Triste s'ascose."
                }
            ],
            [
                {
                    "verse": "O dal tuo tramite"
                },
                {
                    "verse": "Alma divisa,"
                },
                {
                    "verse": "Benigno \u00e8 Satana;"
                },
                {
                    "verse": "Ecco Eloisa."
                }
            ],
            [
                {
                    "verse": "In van ti maceri"
                },
                {
                    "verse": "Ne l'aspro sacco:"
                },
                {
                    "verse": "Il verso ei mormora"
                },
                {
                    "verse": "Di Maro e Flacco"
                }
            ],
            [
                {
                    "verse": "Tra la davidica"
                },
                {
                    "verse": "Nenia ed il pianto;"
                },
                {
                    "verse": "E, forme delfiche,"
                },
                {
                    "verse": "A te da canto,"
                }
            ],
            [
                {
                    "verse": "Rosee ne l'orrida"
                },
                {
                    "verse": "Compagnia nera,"
                },
                {
                    "verse": "Mena Licoride,"
                },
                {
                    "verse": "Mena Glicera."
                }
            ],
            [
                {
                    "verse": "Ma d'altre imagini"
                },
                {
                    "verse": "D'et\u00e0 pi\u00f9 bella"
                },
                {
                    "verse": "Talor si popola"
                },
                {
                    "verse": "L'insonne cella."
                }
            ],
            [
                {
                    "verse": "Ei, da le pagine"
                },
                {
                    "verse": "Di Livio, ardenti"
                },
                {
                    "verse": "Tribuni, consoli,"
                },
                {
                    "verse": "Turbe frementi"
                }
            ],
            [
                {
                    "verse": "Sveglia; e fantastico"
                },
                {
                    "verse": "D'italo orgoglio"
                },
                {
                    "verse": "Te spinge, o monaco,"
                },
                {
                    "verse": "Su 'l Campidoglio."
                }
            ],
            [
                {
                    "verse": "E voi, che il rabido"
                },
                {
                    "verse": "Rogo non strusse,"
                },
                {
                    "verse": "Voci fatidiche,"
                },
                {
                    "verse": "Wicleff ed Husse,"
                }
            ],
            [
                {
                    "verse": "A l'aura il vigile"
                },
                {
                    "verse": "Grido mandate:"
                },
                {
                    "verse": "S'innova il secolo,"
                },
                {
                    "verse": "Piena \u00e8 l'etate."
                }
            ],
            [
                {
                    "verse": "E gi\u00e0 gi\u00e0 tremano"
                },
                {
                    "verse": "Mitre e corone:"
                },
                {
                    "verse": "Dal chiostro brontola"
                },
                {
                    "verse": "La ribellione,"
                }
            ],
            [
                {
                    "verse": "E pugna e pr\u00e8dica"
                },
                {
                    "verse": "Sotto la stola"
                },
                {
                    "verse": "Di fra' Girolamo"
                },
                {
                    "verse": "Savonarola."
                }
            ],
            [
                {
                    "verse": "Gitt\u00f2 la tonaca"
                },
                {
                    "verse": "Martin Lutero;"
                },
                {
                    "verse": "Gitta i tuoi vincoli,"
                },
                {
                    "verse": "Uman pensiero,"
                }
            ],
            [
                {
                    "verse": "E splendi e folgora"
                },
                {
                    "verse": "Di fiamme cinto;"
                },
                {
                    "verse": "Materia, inalzati;"
                },
                {
                    "verse": "Satana ha vinto."
                }
            ],
            [
                {
                    "verse": "Un bello e orribile"
                },
                {
                    "verse": "Mostro si sferra,"
                },
                {
                    "verse": "Corre gli oceani,"
                },
                {
                    "verse": "Corre la terra:"
                }
            ],
            [
                {
                    "verse": "Corusco e fumido"
                },
                {
                    "verse": "Come i vulcani,"
                },
                {
                    "verse": "I monti supera,"
                },
                {
                    "verse": "Divora i piani;"
                }
            ],
            [
                {
                    "verse": "Sorvola i baratri;"
                },
                {
                    "verse": "Poi si nasconde"
                },
                {
                    "verse": "Per antri incogniti,"
                },
                {
                    "verse": "Per vie profonde;"
                }
            ],
            [
                {
                    "verse": "Ed esce; e indomito"
                },
                {
                    "verse": "Di lido in lido"
                },
                {
                    "verse": "Come di turbine"
                },
                {
                    "verse": "Manda il suo grido,"
                }
            ],
            [
                {
                    "verse": "Come di turbine"
                },
                {
                    "verse": "L'alito spande:"
                },
                {
                    "verse": "Ei passa, o popoli,"
                },
                {
                    "verse": "Satana il grande."
                }
            ],
            [
                {
                    "verse": "Passa benefico"
                },
                {
                    "verse": "Di loco in loco"
                },
                {
                    "verse": "Su l'infrenabile"
                },
                {
                    "verse": "Carro del foco."
                }
            ],
            [
                {
                    "verse": "Salute, o Satana,"
                },
                {
                    "verse": "O ribellione,"
                },
                {
                    "verse": "O forza vindice"
                },
                {
                    "verse": "De la ragione!"
                }
            ],
            [
                {
                    "verse": "Sacri a te salgano"
                },
                {
                    "verse": "Gl'incensi e i voti!"
                },
                {
                    "verse": "Hai vinto il Geova"
                },
                {
                    "verse": "De i sacerdoti."
                }
            ]
        ]
    }
]