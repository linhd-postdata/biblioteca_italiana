[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000700",
        "author": "Giusto da Volterra",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Non per dormir s'acquist'onore o regno,"
                },
                {
                    "verse": "né per fuggir si viene in alto stato,"
                },
                {
                    "verse": "né per rispondere ognor quand'è chiamato,"
                },
                {
                    "verse": "sì morte per superbia e per isdegno."
                }
            ],
            [
                {
                    "verse": "Non troppo dura chi si vuol far segno,"
                },
                {
                    "verse": "non chi palesa cosa in sé serrato,"
                },
                {
                    "verse": "non chi più spende che non port'a lato,"
                },
                {
                    "verse": "sì chi con tempi accosta suo 'ngegno."
                }
            ],
            [
                {
                    "verse": "Non chi vuol sempre render mal per male,"
                },
                {
                    "verse": "non chi non è umil sofferitore,"
                },
                {
                    "verse": "sì chi conosce sé per quel che vale."
                }
            ],
            [
                {
                    "verse": "Non chi vuol esser d'altrui usurpatore,"
                },
                {
                    "verse": "non chi non vuol vicino a sé uguale,"
                },
                {
                    "verse": "sì chi caccia da sé vizio ed errore."
                }
            ],
            [
                {
                    "verse": "Deh, noti di buon core"
                },
                {
                    "verse": "chi questa rima legge e seco face,"
                },
                {
                    "verse": "mentre che vive averà bene e pace."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000700",
        "author": "Giusto da Volterra",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Temp'è di levar su, alma gentile,"
                },
                {
                    "verse": "più non dormire in ozïose piume,"
                },
                {
                    "verse": "lasciar le tenebre e venire al lume"
                },
                {
                    "verse": "di perfetta virtù: non esser vile!"
                }
            ],
            [
                {
                    "verse": "Armati tutta! Deh, lascia il servile"
                },
                {
                    "verse": "giogo del tuo peccato, e nel volume"
                },
                {
                    "verse": "delle virtù ti scrive; il buon costume"
                },
                {
                    "verse": "voglia sempre servir co' mente umìle!"
                }
            ],
            [
                {
                    "verse": "Deh, pensa quanta gloria onora Cato,"
                },
                {
                    "verse": "Cesare, Iscipïon, Fabrizio ancora,"
                },
                {
                    "verse": "più e più molti, ch'i' non t'ho contato,"
                }
            ],
            [
                {
                    "verse": "che per virtù non si curaron fora"
                },
                {
                    "verse": "di vita uscire e, ciascun meritato,"
                },
                {
                    "verse": "quivi la fama e 'l ciel di su gli onora."
                }
            ]
        ]
    }
]