[
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "In Nativitate Iesu",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Pastores tenerum dicite pupulum"
                },
                {
                    "verse": "Agrestes citharae, rusticae arundines"
                },
                {
                    "verse": "Iesum Davidicum germen amabile"
                },
                {
                    "verse": "Laudent, ac celebrent; agmina coelica,"
                },
                {
                    "verse": "Dum nox nigra silens obtegit aethera"
                },
                {
                    "verse": "Horrens jam tenebris, sidereas domus"
                },
                {
                    "verse": "Linquendo quatiunt summa per aera"
                },
                {
                    "verse": "Alas pennigeras, tectoque paupero"
                },
                {
                    "verse": "Sistunt, quo trepidans aspero frigore"
                },
                {
                    "verse": "Infans Numen jacet: splendido lumine"
                },
                {
                    "verse": "Nox fulgens placidis migrat Agrestibus."
                },
                {
                    "verse": "Frondes arboribus, florido pabulo"
                },
                {
                    "verse": "Nascuntur subito gramina olentia;"
                },
                {
                    "verse": "Iam coelo fugiunt nubila turbida"
                },
                {
                    "verse": "Ac flammis rutilant lucida sidera."
                },
                {
                    "verse": "Haud segnis puerum concine coelicum"
                },
                {
                    "verse": "Pastorum manus, ac umbriferum nemus"
                },
                {
                    "verse": "Cantu nunc resonet, blandoque carmine."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "Infelix Pastor",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Agricola infelix ad collem pergit amoenum"
                },
                {
                    "verse": "Edita ubi sedes surgit, o Virgo tibi."
                }
            ],
            [
                {
                    "verse": "Ne forte ut solet abjecto, demissoque vultu"
                },
                {
                    "verse": "Ipsum arbitreris munera ferre sacra."
                }
            ],
            [
                {
                    "verse": "Pennatam tibi alebam, candentemque columbam"
                },
                {
                    "verse": "Frumentum solitam rapere amica manu."
                }
            ],
            [
                {
                    "verse": "Barbarus at milvus saevis furtim unguibus ausit"
                },
                {
                    "verse": "Tellurem guttis tingere sanguineis."
                }
            ],
            [
                {
                    "verse": "Subsiliens agnus, ludensque in gramine campi"
                },
                {
                    "verse": "Pabulo pascebat pingui, et opimo tibi."
                }
            ],
            [
                {
                    "verse": "Impius ast lupus, acerque antra immania linquit"
                },
                {
                    "verse": "Balantemque imbellem surripit ipse ferox."
                }
            ],
            [
                {
                    "verse": "Melle favum tibi nectareo compleverat ales,"
                },
                {
                    "verse": "Anguis quem foedat toxico diro tumens."
                }
            ],
            [
                {
                    "verse": "Paupero in arvo rosae surgebant purpura pictae,"
                },
                {
                    "verse": "Sacra tibi Virgo pulchra corona comae."
                }
            ],
            [
                {
                    "verse": "Nunc vero haud spinas inter flos ridet amoenus,"
                },
                {
                    "verse": "Ast humili terra languidus ipse jacet"
                }
            ],
            [
                {
                    "verse": "Quid tamen infelix queror, istisque ingemo damnis"
                },
                {
                    "verse": "Cum sors me gravibus vexet ubique malis?"
                }
            ],
            [
                {
                    "verse": "Supplicis ora gerens, lacrimis ac uvida moestis"
                },
                {
                    "verse": "Squallidus, et lugens porrigo vota tibi."
                }
            ],
            [
                {
                    "verse": "Turbida tempestas coelum obtegit atra tenebris,"
                },
                {
                    "verse": "Ardentesque ignes aethera nigra secant."
                }
            ],
            [
                {
                    "verse": "Personat, immugitque tonitru murmurat auster,"
                },
                {
                    "verse": "Agroque fatalis saxea grando ruit."
                }
            ],
            [
                {
                    "verse": "Ignivomum aera sulcat formidabile fulmen,"
                },
                {
                    "verse": "Tectumque exurit funera ubique ferens."
                }
            ],
            [
                {
                    "verse": "Subsidium fer tandem Mater olimpoque pelle"
                },
                {
                    "verse": "Nimbos ac demens protege Virgo gregem."
                }
            ],
            [
                {
                    "verse": "Lanigerumque pecus tibi erit, mollesque columbae,"
                },
                {
                    "verse": "Si auxilium praestes comis, opemque mihi"
                }
            ],
            [
                {
                    "verse": "Pastorisque bidens, et parva ac herbida tellus,"
                },
                {
                    "verse": "Upilioque tibi Virgo dicatus erit."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "Christi Mors",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Cum moriens Iesus rumpens e pectore questus"
                },
                {
                    "verse": "Aerii montis culmina concuteret:"
                }
            ],
            [
                {
                    "verse": "Somno oppressus Adam torpenti, ac crinibus hirtis"
                },
                {
                    "verse": "Funereo tumulo protulit atro caput."
                }
            ],
            [
                {
                    "verse": "Turbida, fronte gravi, detorsit lumina circum"
                },
                {
                    "verse": "Mirans, et gelido saevo timore pavens."
                }
            ],
            [
                {
                    "verse": "Horrens, ac dubius demissa voce requirit"
                },
                {
                    "verse": "Quis trunco fixus pendeat exanimis."
                }
            ],
            [
                {
                    "verse": "Cum nosset malis lugens niveoque capillo"
                },
                {
                    "verse": "Tundenti dextra tristia damna tulit."
                }
            ],
            [
                {
                    "verse": "Versusque uxori lacrymans heu! crimine nostro"
                },
                {
                    "verse": "Numine, ait, genitus ligno cruento perit."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "Caesar at Rubiconem",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Ne quaeso transire undantia littora tentes;"
                },
                {
                    "verse": "Siste: hoc Roma potens imperat ipsa tibi."
                }
            ],
            [
                {
                    "verse": "Non ora haec est Aegipti verum Itala tellus:"
                },
                {
                    "verse": "Per te, Dux, an erit sanguine tincta suo?"
                }
            ],
            [
                {
                    "verse": "Sic caput extollens Rubiconis coerulus amnis"
                },
                {
                    "verse": "Intrepido ajebat, belligeroque viro."
                }
            ],
            [
                {
                    "verse": "At fero trajecit vultu Mavortius heros,"
                },
                {
                    "verse": "Ac tum libertas nobilis ingemuit."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "In Caesaris sepulchrum",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Qui totum aspectu quondam fero terruit orbem"
                },
                {
                    "verse": "Lugubri tumulo pulvis, et ossa jacet."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Carmina varia (1810)",
        "title": "In Mortem Pompeji",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001243",
        "text": [
            [
                {
                    "verse": "Qui Pontum vicit Piratas depulit urbe,"
                },
                {
                    "verse": "Aegyptum advectus proditus ipse perit."
                }
            ]
        ]
    }
