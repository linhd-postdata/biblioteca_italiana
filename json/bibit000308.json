[
    {
        "author": "Francesco del Benino",
        "work": "Poesie",
        "title": "I",
        "structure": "sestina",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000308",
        "text": [
            [
                {
                    "verse": "\u00c9 animal di tanta altera vista"
                },
                {
                    "verse": "e s\u00ec costante a riguardare il sole"
                },
                {
                    "verse": "che muor ne' razzi, e poi ritorna in vita"
                },
                {
                    "verse": "bel come prima; e cos\u00ec si rinnova"
                },
                {
                    "verse": "di tempo in tempo, n\u00e9 si truova in selva"
                },
                {
                    "verse": "di cotanto valore un'altra fera."
                }
            ],
            [
                {
                    "verse": "Similemente questa nobil fera"
                },
                {
                    "verse": "non par per alcun tempo cangiar vista"
                },
                {
                    "verse": "come gli altri animal che stanno in selva,"
                },
                {
                    "verse": "ma tanto \u00e8 bella che avanza il sole"
                },
                {
                    "verse": "al dolce tempo che i fiori rinnova:"
                },
                {
                    "verse": "molti si pascon sol della sua vita."
                }
            ],
            [
                {
                    "verse": "Non sar\u00e0, credo, n\u00e9 fu mai in vita"
                },
                {
                    "verse": "tanto benigna n\u00e9 s\u00ec umil fera"
                },
                {
                    "verse": "per alcun tempo; ed \u00e8 s\u00ec vaga in vista"
                },
                {
                    "verse": "che par che faccia pure invidia al sole"
                },
                {
                    "verse": "e a qualunque animal che pasce in selva."
                }
            ],
            [
                {
                    "verse": "Qual non vorrebbe abitare in selva"
                },
                {
                    "verse": "pascendo l'erba e l\u00ec finir sua vita,"
                },
                {
                    "verse": "se vi raggiasse quel fulvido sole,"
                },
                {
                    "verse": "ch'esce degli occhi d'esta gentil fera,"
                },
                {
                    "verse": "ch\u00e9 m'ha lume gi\u00e0 dato la sua vista"
                },
                {
                    "verse": "quando di stelle lo ciel si rinnova?"
                }
            ],
            [
                {
                    "verse": "Il mio disire ognor s\u00ec si rinnova"
                },
                {
                    "verse": "con dolce fiamme, per che poggio o selva"
                },
                {
                    "verse": "ombra non mi pu\u00f2 dare alla sua vista"
                },
                {
                    "verse": "onde mi piace e vo' seguir tal vita"
                },
                {
                    "verse": "per ciascun calle, ch'essa non \u00e8 fera:"
                },
                {
                    "verse": "il ciel la f\u00e9, or la governa il sole."
                }
            ],
            [
                {
                    "verse": "Il loco suo \u00e8 dove luce il sole,"
                },
                {
                    "verse": "in dolce patria ove si rinnova"
                },
                {
                    "verse": "ogn'animo gentil per cotal fera,"
                },
                {
                    "verse": "che non rapisce altrui n\u00e9 pasce in selva;"
                },
                {
                    "verse": "ma io mi son donato alla sua vita,"
                },
                {
                    "verse": "per esser degno di s\u00ec bella vista;"
                }
            ],
            [
                {
                    "verse": "ch\u00e9 ben si perder\u00e0 del sol la vista,"
                },
                {
                    "verse": "pria ch'io non ami questa nova fera,"
                },
                {
                    "verse": "mentre che 'n selva fia chi porti vita."
                }
            ]
        ]
    },
    {
        "author": "Francesco del Benino",
        "work": "Poesie",
        "title": "RIME DUBBIE",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000308",
        "text": [
            [
                {
                    "verse": "Se fu amante mai con tanta fede,"
                },
                {
                    "verse": "o buon servo a signore,"
                },
                {
                    "verse": "quant'io al tuo splendore,"
                },
                {
                    "verse": "non sia per me mai piat\u00e0 n\u00e9 merzede;"
                },
                {
                    "verse": "anzi sempre ti cresca"
                },
                {
                    "verse": "ver me la crudeltate,"
                },
                {
                    "verse": "fin che del mio morir tu la contenti."
                },
                {
                    "verse": "E se non fu, deh, incresca"
                },
                {
                    "verse": "a tua nobilitate"
                },
                {
                    "verse": "tener la vita mia in tanti tormenti!"
                },
                {
                    "verse": "Meritano in me miei portamenti"
                },
                {
                    "verse": "e la fede e l'amore"
                },
                {
                    "verse": "trovar s\u00ec duro core?"
                },
                {
                    "verse": "S\u00ec, poi che vuoi, ma nessun altro il crede."
                }
            ]
        ]
    }
]