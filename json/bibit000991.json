[
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "I",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Che v\u2019habbia, o d\u2019honestate e di bellezza"
                },
                {
                    "verse": "donna real, tra noi specchio e ricetto,"
                },
                {
                    "verse": "rapace man, del ciel contrario aspetto,"
                },
                {
                    "verse": "tolto quel che pi\u00f9 il volgo ama ed apprezza;"
                }
            ],
            [
                {
                    "verse": "nulla vi tolse gi\u00e0, se la ricchezza "
                },
                {
                    "verse": "torvi non pu\u00f2 del chiaro alto intelletto,"
                },
                {
                    "verse": "n\u00e9 romper pur del saldo e nobil petto"
                },
                {
                    "verse": "lo smalto Amor, dove ogni dardo spezza."
                }
            ],
            [
                {
                    "verse": "Quest\u2019\u00e8 il tesoro, che pregiati e cari"
                },
                {
                    "verse": "ne pu\u00f2 far sempre, e queste son quell\u2019arme"
                },
                {
                    "verse": "che rendon forte l\u2019huom contr\u2019ogni assalto."
                }
            ],
            [
                {
                    "verse": "Gratie rare dal ciel concesse  a rari;"
                },
                {
                    "verse": "che miracol dunqu\u2019\u00e8 s\u2019oggi tant\u2019alto"
                },
                {
                    "verse": "da terra a dir di voi non posso alzarme?"
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "II",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Steril pianta in terren spinoso incolto"
                },
                {
                    "verse": "non pu\u00f2 dar mai, donna real, buon frutto;"
                },
                {
                    "verse": "n\u00e9 secco prato fior, n\u00e9 fonte asciutto"
                },
                {
                    "verse": "bel rio, n\u00e9 luce ombroso bosco e folto."
                }
            ],
            [
                {
                    "verse": "A che dunque da stil rozo, e gi\u00e0 volto"
                },
                {
                    "verse": "a sempiterno lamentoso lutto"
                },
                {
                    "verse": "torvi al nemico oblio cercate in tutto?"
                },
                {
                    "verse": "Chi non ha non pu\u00f2 dar poco n\u00e9 molto."
                }
            ],
            [
                {
                    "verse": "Voi, cui l\u2019acque beate aperte sono"
                },
                {
                    "verse": "mentre il gran Febo a farvi honor s\u2019ingegna,"
                },
                {
                    "verse": "difendetevi pur contra di Lete."
                }
            ],
            [
                {
                    "verse": "Largo il ciel dievvi e l\u2019uno e l\u2019altro dono:"
                },
                {
                    "verse": "chiara historia far sempre, esserne degna."
                },
                {
                    "verse": "Voi di voi stessa sol dunque scrivete."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "III",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Padre felice santo, a\u2019 giorni nostri "
                },
                {
                    "verse": "dato dal ciel per far felice e santo"
                },
                {
                    "verse": "il mondo tutto ch\u2019in errori e \u2019n pianto"
                },
                {
                    "verse": "vide chiusi gran tempo i sommi chiostri,"
                }
            ],
            [
                {
                    "verse": "i tre pi\u00f9 belli e pi\u00f9 lodati inchiostri"
                },
                {
                    "verse": "rivolga Smirna a te, Fiorenza e Manto,"
                },
                {
                    "verse": "e la Chiesa di Dio rivesta il manto"
                },
                {
                    "verse": "che gi\u00e0 spogli\u00f2 quand\u2019hebbe l\u2019oro e gli ostri."
                }
            ],
            [
                {
                    "verse": "Celeste ardor le nobili alme accese "
                },
                {
                    "verse": "del legno errante a darti in man l\u2019impero"
                },
                {
                    "verse": "ch\u2019in van molt\u2019anni il tuo soccorso attese;"
                }
            ],
            [
                {
                    "verse": "e ben l\u2019havr\u00e0, ch\u00e9 se per un nocchiero"
                },
                {
                    "verse": "felicemente in prima il porto prese,"
                },
                {
                    "verse": "hor dove andr\u00e0, che insieme ha Polo e Piero?"
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "IV",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Amor, tu che\u2019l puoi fare, e tu che \u2019l fai, "
                },
                {
                    "verse": "soccorri a chi d\u2019ogni soccorso \u00e8 degno!"
                },
                {
                    "verse": "Quando, spirto gentil, nel tuo bel regno"
                },
                {
                    "verse": "rinchiuso alto dolor tacque giamai?"
                }
            ],
            [
                {
                    "verse": "Rendi pietosi gl\u2019occhi, onde tu fai"
                },
                {
                    "verse": "Plantedio nostro hor neve, hor sasso, hor legno,"
                },
                {
                    "verse": "mentre egli cerca, e mai non giunge al segno,"
                },
                {
                    "verse": "scoprir l\u2019abisso de\u2019 suoi lunghi guai."
                }
            ],
            [
                {
                    "verse": "O, s\u2019a priego d\u2019humil servo si crede,"
                },
                {
                    "verse": "tornalo al primo grado, acci\u00f2 si vanti"
                },
                {
                    "verse": "d\u2019haver trovato in te pietate e fede."
                }
            ],
            [
                {
                    "verse": "S\u00ec vedrem tosto  poi con altrettanti"
                },
                {
                    "verse": "incensi l\u2019ara e \u2019l loco, ove si vede"
                },
                {
                    "verse": "l\u2019imagin tua, spirar rose ed acanti."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "V",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Qual huom di notte in via smarrito e lasso"
                },
                {
                    "verse": "lume lontan da valle ima comprende"
                },
                {
                    "verse": "che \u2019l dubbioso camin certo li rende,"
                },
                {
                    "verse": "e, com pi\u00f9 sprona, e pi\u00f9 rallenta il passo,"
                }
            ],
            [
                {
                    "verse": "tal in questo sentier pien d\u2019ombre e basso"
                },
                {
                    "verse": "scorgo il celeste honor che \u2019n voi risplende,"
                },
                {
                    "verse": "e tutto in un quel ben che ne contende"
                },
                {
                    "verse": "terrena nebbia, e in sen di Dio trapasso."
                }
            ],
            [
                {
                    "verse": "E quanto pi\u00f9 lo stil s\u2019infiamma al corso"
                },
                {
                    "verse": "di vostre lodi, in poca e steril vena"
                },
                {
                    "verse": "tanto pi\u00f9 ogn\u2019hor dal ver si trova lunge;"
                }
            ],
            [
                {
                    "verse": "n\u00e9 spero per mortal vano soccorso"
                },
                {
                    "verse": "parte ombreggiar del bel vostro, ov\u2019a pena"
                },
                {
                    "verse": "d\u2019alto pensier spedito volo aggiunge."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "VI",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Arbor sacra felice e trionphale,"
                },
                {
                    "verse": "ove \u2019l Signor del ciel vivo e verace"
                },
                {
                    "verse": "regn\u00f2 pendendo, e co \u2019l morir di\u00e8 pace"
                },
                {
                    "verse": "a la guerra del mondo aspra e mortale:"
                }
            ],
            [
                {
                    "verse": "Fia mai che a l\u2019ombra tua dolce e vitale"
                },
                {
                    "verse": "l\u2019alma, ch\u2019al mondan visco involta giace,"
                },
                {
                    "verse": "respiri un giorno? E\u2019 ben vana e fallace"
                },
                {
                    "verse": "tutt\u2019altra aita al viver manco e frale."
                }
            ],
            [
                {
                    "verse": "Fia mai ch\u2019io porti al cor le tue radici"
                },
                {
                    "verse": "profonde e ferme, e sia di coglier degno"
                },
                {
                    "verse": "i frutti tuoi ch\u2019a Dio ne fero amici? "
                }
            ],
            [
                {
                    "verse": "Habbisi pur chi vuol del mondo il regno"
                },
                {
                    "verse": "pur ch\u2019io m\u2019inesti in tuoi rami felici,"
                },
                {
                    "verse": "pur ch\u2019io sia croce, e mi trasformi in legno."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "VII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Per l\u2019huom terren la morte entr\u00f2 nel mondo,"
                },
                {
                    "verse": "per l\u2019huom celeste poi venne la vita;"
                },
                {
                    "verse": "lasci\u00f2 Natura il primo orba e ferita,"
                },
                {
                    "verse": "le di\u00e8 poi luce e risan\u00f2 il secondo."
                }
            ],
            [
                {
                    "verse": "L\u2019un n\u2019aperse l\u2019inferno atro e profondo, "
                },
                {
                    "verse": "l\u2019altro la via del ciel chiara e spedita;"
                },
                {
                    "verse": "quelli in miseria bassa ed infinita,"
                },
                {
                    "verse": "questi ne pose in grado alto e giocondo."
                }
            ],
            [
                {
                    "verse": "Ma, se talhor mi volgo a quel che il primo"
                },
                {
                    "verse": "ne rec\u00f2 co \u2019l peccar, trahendo a morte"
                },
                {
                    "verse": "dal ciel per far noi vivi il mio Signore,"
                }
            ],
            [
                {
                    "verse": "quasi a par del guadagno il danno estimo,"
                },
                {
                    "verse": "e grido: o caro don d\u2019adversa sorte,"
                },
                {
                    "verse": "o felice d\u2019Adam primiero errore!"
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "VIII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Se quanto pi\u00f9 dal mar turbato infido "
                },
                {
                    "verse": "del mondo empio nemico il pi\u00e8 trar voglio,"
                },
                {
                    "verse": "tanto pi\u00f9 v\u2019entro, e periglioso scoglio"
                },
                {
                    "verse": "m\u2019\u00e8 sempre innanzi, e s\u2019allontana il lido."
                }
            ],
            [
                {
                    "verse": "Se quanto pi\u00f9 di me stesso mi fido "
                },
                {
                    "verse": "via pi\u00f9 m\u2019inganno, e di ragion mi spoglio,"
                },
                {
                    "verse": "e tanto pi\u00f9 catene e reti accoglio"
                },
                {
                    "verse": "quanto pi\u00f9 libert\u00e0 sospiro e grido,"
                }
            ],
            [
                {
                    "verse": "tu, Signor, salda sede e vero porto,"
                },
                {
                    "verse": "qual di te parte in te stesso mi mira,"
                },
                {
                    "verse": "e non in me, che per me nulla io sono;"
                }
            ],
            [
                {
                    "verse": "rinverdi e avviva il cor, che secco e morto"
                },
                {
                    "verse": "langue, qual fior di ria stagione in ira;"
                },
                {
                    "verse": "gran tempo \u00e8 gi\u00e0 che fost\u00f9 \u2019l mio perdono."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "IX",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Del tuo morir, che fu la vita mia,"
                },
                {
                    "verse": "dogliansi gli altri pur che pi\u00f9 non sanno:"
                },
                {
                    "verse": "la bella morte, il tuo felice affanno"
                },
                {
                    "verse": "nova gioia nel cor mi desta e cria."
                }
            ],
            [
                {
                    "verse": "Tu risaldar la piaga antica e ria, "
                },
                {
                    "verse": "e del fiero angue il venenoso inganno"
                },
                {
                    "verse": "vincer potesti, e ristorar quel danno"
                },
                {
                    "verse": "che ne lasci\u00f2 del ciel chiusa la via."
                }
            ],
            [
                {
                    "verse": "Qual Ocean potea del primo errore "
                },
                {
                    "verse": "lavar la macchia ond\u2019era il mondo infetto"
                },
                {
                    "verse": "fuor che \u2019l divin per noi sparso tuo sangue?"
                }
            ],
            [
                {
                    "verse": "Gode dunque a ragion del tuo dolore,"
                },
                {
                    "verse": "Signor, l\u2019alma gi\u00e0 tua, ma duolsi e langue"
                },
                {
                    "verse": "ch\u2019a morir ti costrinse il suo diffetto."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "X",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Da la strada d\u2019Amor fangosa e torta,"
                },
                {
                    "verse": "ove speme mendace e van desio"
                },
                {
                    "verse": "mantenner cieca in periglioso oblio"
                },
                {
                    "verse": "la vita anzi il suo fin sepolta e morta,"
                }
            ],
            [
                {
                    "verse": "partendo l\u2019alma del suo fallo accorta "
                },
                {
                    "verse": "rivolse il volo a la ragione, a Dio;"
                },
                {
                    "verse": "quando la torse indietro ingordo e rio"
                },
                {
                    "verse": "breve piacer, che lungo affanno apporta."
                }
            ],
            [
                {
                    "verse": "Ed io che anchor piagato ed arso il fianco"
                },
                {
                    "verse": "trahea dal primo assalto, oprar difesa"
                },
                {
                    "verse": "non potei s\u00ec, che non vincesse Amore."
                }
            ],
            [
                {
                    "verse": "Hor veggio ben ch\u2019a la primiera impresa"
                },
                {
                    "verse": "sempre si de\u2019 fugir; ch\u00e9, preso e stanco,"
                },
                {
                    "verse": "ne la seconda poi si perde o more."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "XI",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Vivo alto Re, da l\u2019opre basse e morte"
                },
                {
                    "verse": "richiama l\u2019alma in se stessa sepolta;"
                },
                {
                    "verse": "se fu molto l\u2019error, la gratia \u00e8 molta,"
                },
                {
                    "verse": "tu puoi solo di man ritorla a Morte."
                }
            ],
            [
                {
                    "verse": "Rompi la pietra adamantina e forte,"
                },
                {
                    "verse": "che la circonda e cuopre, acci\u00f2 che sciolta"
                },
                {
                    "verse": "da que\u2019 lacci del mondo, u\u2019 dorme avvolta,"
                },
                {
                    "verse": "si levi al ciel per vie sicure e corte."
                }
            ],
            [
                {
                    "verse": "Gi\u00e0 ti vid\u2019io con molle e scolorita"
                },
                {
                    "verse": "guancia pianger l\u2019amico, e trarlo fuore"
                },
                {
                    "verse": "del chiuso sasso, e ritornarlo in vita."
                }
            ],
            [
                {
                    "verse": "N\u00e9, perch\u00e9 sia l\u2019indignit\u00e0 maggiore"
                },
                {
                    "verse": "mia de la sua, ritarde a darmi aita"
                },
                {
                    "verse": "quel, che mortal ti f\u00e9, celeste amore."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "XII",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "A la mia foscha e tenebrosa mente"
                },
                {
                    "verse": "ch\u00e9 non aggiorni, o vero sol ch\u2019a noi"
                },
                {
                    "verse": "vero giorno apri? E son ben grandi i suoi"
                },
                {
                    "verse": "terreni abissi, onde adombrar si sente."
                }
            ],
            [
                {
                    "verse": "Se dieder lume a le luci orbe e spente "
                },
                {
                    "verse": "di tal che ti fer\u00ec su \u2019l legno i tuoi"
                },
                {
                    "verse": "celesti ombrati raggi, illustrar puoi"
                },
                {
                    "verse": "la notti anchor di chi s\u2019humilia e pente;"
                }
            ],
            [
                {
                    "verse": "e s\u2019ei con un sol colpo il lato manco"
                },
                {
                    "verse": "t\u2019aperse, ed io di sempre nove offese"
                },
                {
                    "verse": "con venenoso stral t\u2019impiago e fiedo."
                }
            ],
            [
                {
                    "verse": "Non men per\u00f2 ver me pronta e cortese"
                },
                {
                    "verse": "sia la virt\u00f9 del pretioso fianco,"
                },
                {
                    "verse": "s\u2019a par di lui, Signor, t\u2019esalto e credo."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "XIII",
        "structure": "strambotto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Il mondo \u00e8 un legno in mar, rotte le vele,"
                },
                {
                    "verse": "un che sempre si muove immobil loco;"
                },
                {
                    "verse": "un\u2019horto, ov\u2019ogni frutto \u00e8 spina, e fele;"
                },
                {
                    "verse": "un\u2019acqua ch\u2019arde, ed un che aggiaccia foco:"
                },
                {
                    "verse": "un filo in aria, un\u2019Echo di querele "
                },
                {
                    "verse": "un dove ognihor si perde infedel gioco;"
                },
                {
                    "verse": "in fin non saprei dir quel ch\u2019egli sia "
                },
                {
                    "verse": "se quel ch\u2019egli \u00e8, non \u00e8, n\u00e9 fu, n\u00e9 fia."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "XIV",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "Ditemi, pensier miei, quando vorrete"
                },
                {
                    "verse": "fermarvi voi, che saldo loco habbiate?"
                },
                {
                    "verse": "Hor alto, hor basso, hor quinci, hor quindi andate"
                },
                {
                    "verse": "vagando pur, che non mai posa havrete."
                }
            ],
            [
                {
                    "verse": "Ah, ciechi, ah temerarii, hor non vedete "
                },
                {
                    "verse": "ch\u2019a voi medesmi oltraggio e torto fate,"
                },
                {
                    "verse": "quasi alme a l\u2019infernal stratio dannate,"
                },
                {
                    "verse": "urne, augel, rota, sassi, e fame e sete?"
                }
            ],
            [
                {
                    "verse": "Ben vi chiamo io, ben vi riprendo e dico:"
                },
                {
                    "verse": "\u00abTornate indietro, e raffrenate il volo\u00bb,"
                },
                {
                    "verse": "ma voi ne gite ognihor pi\u00f9 sordi e pronti."
                }
            ],
            [
                {
                    "verse": "Pur il mio mal co \u2019l vostro mal consolo,"
                },
                {
                    "verse": "ch\u00e9, s\u2019io perdo le voci e m\u2019affatico,"
                },
                {
                    "verse": "voi sete Icari al fin tutti e Phetonti."
                }
            ]
        ]
    },
    {
        "author": "Berardino Rota",
        "work": "Rime extravaganti",
        "title": "XV",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000991",
        "text": [
            [
                {
                    "verse": "O quanto volentier Proteo sarei"
                },
                {
                    "verse": "o Glauco, od altri, che mut\u00f2 gi\u00e0 forma,"
                },
                {
                    "verse": "poi che caso verun non mi trasforma"
                },
                {
                    "verse": "e tu quel desso (o pensier mio) pur sei!"
                }
            ],
            [
                {
