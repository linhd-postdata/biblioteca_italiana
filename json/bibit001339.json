[
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "1",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Io son s\u00ec vaga della mia bellezza,"
                },
                {
                    "verse": "che d'altro amor gi\u00e0 mai"
                },
                {
                    "verse": "non curer\u00f2 n\u00e9 credo aver vaghezza."
                }
            ],
            [
                {
                    "verse": "Io veggio in quella, ognora ch'io mi specchio,"
                },
                {
                    "verse": "quel ben che fa contento lo 'ntelletto:"
                },
                {
                    "verse": "n\u00e9 accidente nuovo o pensier vecchio"
                },
                {
                    "verse": "mi pu\u00f2 privar di s\u00ec caro diletto."
                },
                {
                    "verse": "Quale altro dunque piacevole obgetto"
                },
                {
                    "verse": "potrei veder gi\u00e0 mai"
                },
                {
                    "verse": "che mi mettesse in cuor nuova vaghezza?"
                }
            ],
            [
                {
                    "verse": "Non fugge questo ben qualor disio"
                },
                {
                    "verse": "di rimirarlo in mia consolazione:"
                },
                {
                    "verse": "anzi si fa incontro al piacer mio"
                },
                {
                    "verse": "tanto soave a sentir, che sermone"
                },
                {
                    "verse": "dir nol poria n\u00e9 prendere intenzione"
                },
                {
                    "verse": "d'alcun mortal gi\u00e0 mai,"
                },
                {
                    "verse": "che non ardesse di cotal vaghezza."
                }
            ],
            [
                {
                    "verse": "E io, che ciascuna ora pi\u00f9 m'accendo"
                },
                {
                    "verse": "quanto pi\u00f9 fisi tengo gli occhi in esso,"
                },
                {
                    "verse": "tutta mi dono a lui, tutta mi rendo,"
                },
                {
                    "verse": "gustando gi\u00e0 di ci\u00f2 ch'el m'ha promesso:"
                },
                {
                    "verse": "e maggior gioia spero pi\u00f9 dappresso"
                },
                {
                    "verse": "s\u00ec fatta, che gi\u00e0 mai"
                },
                {
                    "verse": "simil non si sent\u00ec qui da vaghezza."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "2",
        "structure": "canzone",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Qual donna canter\u00e0, s'io non canto io,"
                },
                {
                    "verse": "che son contenta d'ogni mio disio?"
                }
            ],
            [
                {
                    "verse": "Vien dunque, Amor, cagion d'ogni mio bene,"
                },
                {
                    "verse": "d'ogni speranza e d'ogni lieto effetto;"
                },
                {
                    "verse": "cantiamo insieme un poco,"
                },
                {
                    "verse": "non de' sospir n\u00e9 delle amare pene"
                },
                {
                    "verse": "ch'or pi\u00f9 dolce mi fanno il tuo diletto,"
                },
                {
                    "verse": "ma sol del chiaro foco,"
                },
                {
                    "verse": "nel quale ardendo in festa vivo e 'n gioco,"
                },
                {
                    "verse": "te adorando come un mio idio."
                }
            ],
            [
                {
                    "verse": "Tu mi ponesti innanzi agli occhi, Amore,"
                },
                {
                    "verse": "il primo d\u00ec ch'io nel tuo foco entrai,"
                },
                {
                    "verse": "un giovinetto tale,"
                },
                {
                    "verse": "che di bilt\u00e0, d'ardir n\u00e9 di valore"
                },
                {
                    "verse": "non se ne troverebbe un maggior mai,"
                },
                {
                    "verse": "n\u00e9 pure a lui equale:"
                },
                {
                    "verse": "di lui m'accesi tanto, che aguale"
                },
                {
                    "verse": "lieta ne canto teco, signor mio."
                }
            ],
            [
                {
                    "verse": "E quel che 'n questo m'\u00e8 sommo piacere"
                },
                {
                    "verse": "\u00e8 ch'io gli piaccio quanto egli a me piace,"
                },
                {
                    "verse": "Amor, la tua merzede;"
                },
                {
                    "verse": "per che in questo mondo il mio volere"
                },
                {
                    "verse": "posseggo, e spero nell'altro aver pace"
                },
                {
                    "verse": "per quella intera fede"
                },
                {
                    "verse": "che io gli porto. Idio, che questo vede,"
                },
                {
                    "verse": "del regno suo ancor ne sar\u00e0 pio."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "3",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Niuna sconsolata"
                },
                {
                    "verse": "da dolersi ha quant'io,"
                },
                {
                    "verse": "ch'invan sospiro, lassa innamorata."
                }
            ],
            [
                {
                    "verse": "Colui che move il cielo e ogni stella"
                },
                {
                    "verse": "mi fece a suo diletto"
                },
                {
                    "verse": "vaga, leggiadra, graziosa e bella,"
                },
                {
                    "verse": "per dar qua gi\u00f9 a ogni alto intelletto"
                },
                {
                    "verse": "alcun segno di quella"
                },
                {
                    "verse": "bilt\u00e0 che sempre a Lui sta nel cospetto;"
                },
                {
                    "verse": "e il mortal difetto,"
                },
                {
                    "verse": "come mal conosciuta,"
                },
                {
                    "verse": "non mi gradisce, anzi m'ha dispregiata."
                }
            ],
            [
                {
                    "verse": "Gi\u00e0 fu chi m'ebbe cara e volentieri"
                },
                {
                    "verse": "giovinetta mi prese"
                },
                {
                    "verse": "nelle sue braccia e dentro a' suoi pensieri,"
                },
                {
                    "verse": "e de' miei occhi tututto s'accese"
                },
                {
                    "verse": "e 'l tempo, che leggieri"
                },
                {
                    "verse": "sen vola, tutto in vagheggiarmi spese;"
                },
                {
                    "verse": "e io, come cortese,"
                },
                {
                    "verse": "di me il feci degno;"
                },
                {
                    "verse": "ma or ne son, dolente a me!, privata."
                }
            ],
            [
                {
                    "verse": "Femmisi innanzi poi presuntuoso"
                },
                {
                    "verse": "un giovinetto fiero,"
                },
                {
                    "verse": "s\u00e9 nobil reputando e valoroso,"
                },
                {
                    "verse": "e presa tienmi e con falso pensiero"
                },
                {
                    "verse": "divenuto \u00e8 geloso;"
                },
                {
                    "verse": "laond'io, lassa!, quasi mi dispero,"
                },
                {
                    "verse": "cognoscendo per vero,"
                },
                {
                    "verse": "per ben di molti al mondo"
                },
                {
                    "verse": "venuta, da uno essere occupata."
                }
            ],
            [
                {
                    "verse": "Io maledico la mia sventura,"
                },
                {
                    "verse": "quando, per mutar vesta,"
                },
                {
                    "verse": "s\u00ec dissi mai; s\u00ec bella nella oscura"
                },
                {
                    "verse": "mi vidi gi\u00e0 e lieta, dove in questa"
                },
                {
                    "verse": "io meno vita dura,"
                },
                {
                    "verse": "vie men che prima reputata onesta."
                },
                {
                    "verse": "O dolorosa festa,"
                },
                {
                    "verse": "morta foss'io avanti"
                },
                {
                    "verse": "che io t'avessi in tal caso provata!"
                }
            ],
            [
                {
                    "verse": "O caro amante, del qual prima fui"
                },
                {
                    "verse": "pi\u00f9 che altra contenta,"
                },
                {
                    "verse": "che or nel ciel se' davanti a Colui"
                },
                {
                    "verse": "che ne cre\u00f2, deh! pietoso diventa"
                },
                {
                    "verse": "di me, che per altrui"
                },
                {
                    "verse": "te obliar non posso: fa ch'io senta"
                },
                {
                    "verse": "che quella fiamma spenta"
                },
                {
                    "verse": "non sia che per me t'arse,"
                },
                {
                    "verse": "e cost\u00e0 s\u00f9 m'impetra la tornata."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "4",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Lagrimando dimostro"
                },
                {
                    "verse": "quanto si dolga con ragione il core"
                },
                {
                    "verse": "d'esser tradito sotto fede, Amore."
                }
            ],
            [
                {
                    "verse": "Amore, allora che primieramente"
                },
                {
                    "verse": "ponesti in lui colei per cui sospiro"
                },
                {
                    "verse": "senza sperar salute,"
                },
                {
                    "verse": "si piena la mostrasti di virtute,"
                },
                {
                    "verse": "che lieve reputava ogni martiro"
                },
                {
                    "verse": "che per te nella mente,"
                },
                {
                    "verse": "ch'\u00e8 rimasa dolente,"
                },
                {
                    "verse": "fosse venuto; ma il mio errore"
                },
                {
                    "verse": "ora conosco, e non senza dolore."
                }
            ],
            [
                {
                    "verse": "Fatto m'ha conoscente dello 'nganno"
                },
                {
                    "verse": "vedermi abbandonato da colei"
                },
                {
                    "verse": "in cui sola sperava;"
                },
                {
                    "verse": "ch'allora ch'io pi\u00f9 esser mi pensava"
                },
                {
                    "verse": "nella sua grazia e servidore a lei,"
                },
                {
                    "verse": "senza mirare al danno"
                },
                {
                    "verse": "del mio futuro affanno,"
                },
                {
                    "verse": "m'accorsi lei aver l'altrui valore"
                },
                {
                    "verse": "dentro raccolto e me cacciato fore."
                }
            ],
            [
                {
                    "verse": "Com'io conobbi me di fuor cacciato,"
                },
                {
                    "verse": "nacque nel core un pianto doloroso"
                },
                {
                    "verse": "che ancor vi dimora:"
                },
                {
                    "verse": "e spesso maladico il giorno e l'ora"
                },
                {
                    "verse": "che pria m'apparve il suo viso amoroso"
                },
                {
                    "verse": "d'alta biltate ornato"
                },
                {
                    "verse": "e pi\u00f9 che mai infiammato!"
                },
                {
                    "verse": "La fede mia, la speranza e l'ardore"
                },
                {
                    "verse": "va bestemmiando l'anima che more."
                }
            ],
            [
                {
                    "verse": "Quanto 'l mio duol senza conforto sia,"
                },
                {
                    "verse": "signor, tu 'l puoi sentir, tanto ti chiamo"
                },
                {
                    "verse": "con dolorosa voce:"
                },
                {
                    "verse": "e dicoti che tanto e s\u00ec mi cuoce,"
                },
                {
                    "verse": "che per minor martir la morte bramo."
                },
                {
                    "verse": "Venga dunque, e la mia"
                },
                {
                    "verse": "vita crudele e ria"
                },
                {
                    "verse": "termini col suo colpo, e 'l mio furore,"
                },
                {
                    "verse": "ch'ove ch'io vada il sentir\u00f2 minore."
                }
            ],
            [
                {
                    "verse": "Nulla altra via, niuno altro conforto"
                },
                {
                    "verse": "mi resta pi\u00f9 che morte alla mia doglia."
                },
                {
                    "verse": "Dallami dunque omai,"
                },
                {
                    "verse": "pon fine, Amor, con essa alli miei guai,"
                },
                {
                    "verse": "e 'l cor di vita s\u00ec misera spoglia."
                },
                {
                    "verse": "Deh fallo, poi ch'a torto"
                },
                {
                    "verse": "m'\u00e8 gioia tolta e diporto."
                },
                {
                    "verse": "Fa costei lieta, morend'io, signore,"
                },
                {
                    "verse": "come l'hai fatta di nuovo amadore."
                }
            ],
            [
                {
                    "verse": "Ballata mia, se alcun non t'appara"
                },
                {
                    "verse": "io non men curo, per ci\u00f2 che nessuno,"
                },
                {
                    "verse": "com'io, ti pu\u00f2 cantare."
                },
                {
                    "verse": "Una fatica sola ti vo' dare:"
                },
                {
                    "verse": "che tu ritruovi Amore, e a lui solo uno"
                },
                {
                    "verse": "quanto mi sia discara"
                },
                {
                    "verse": "la trista vita amara"
                },
                {
                    "verse": "dimostri appien, pregandol che 'n migliore"
                },
                {
                    "verse": "porto ne ponga per lo suo onore."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "5",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Amor, la vaga luce"
                },
                {
                    "verse": "che move da' begli occhi di costei"
                },
                {
                    "verse": "servo m'ha fatto di te e di lei."
                }
            ],
            [
                {
                    "verse": "Mosse da' suoi begli occhi lo splendore"
                },
                {
                    "verse": "che pria la fiamma tua nel cor m'accese,"
                },
                {
                    "verse": "per li miei trapassando;"
                },
                {
                    "verse": "e quanto fosse grande il tuo valore,"
                },
                {
                    "verse": "il bel viso di lei mi f\u00e9 palese;"
                },
                {
                    "verse": "il quale imaginando,"
                },
                {
                    "verse": "mi senti' gir legando"
                },
                {
                    "verse": "ogni vert\u00f9 e sottoporla a lei,"
                },
                {
                    "verse": "fatta nuova cagion de' sospir miei."
                }
            ],
            [
                {
                    "verse": "Cos\u00ec de' tuoi, adunque, divenuto"
                },
                {
                    "verse": "son, signor caro, e ubidente aspetto"
                },
                {
                    "verse": "dal tuo poter merzede;"
                },
                {
                    "verse": "ma non so ben se 'ntero \u00e8 conosciuto"
                },
                {
                    "verse": "l'alto disio che messo m'hai nel petto"
                },
                {
                    "verse": "n\u00e9 la mia intera fede"
                },
                {
                    "verse": "da costei, che possiede"
                },
                {
                    "verse": "s\u00ec la mia mente, che io non torrei"
                },
                {
                    "verse": "pace fuor che da essa, n\u00e9 vorrei."
                }
            ],
            [
                {
                    "verse": "Per ch'io ti priego, dolce signor mio,"
                },
                {
                    "verse": "che gliel dimostri e faccile sentire"
                },
                {
                    "verse": "alquanto del tuo foco"
                },
                {
                    "verse": "in servigio di me, ch\u00e9 vedi ch'io"
                },
                {
                    "verse": "gi\u00e0 mi consumo amando e nel martire"
                },
                {
                    "verse": "mi sfaccio a poco a poco;"
                },
                {
                    "verse": "e poi, quando fia loco,"
                },
                {
                    "verse": "me raccomanda a lei, come tu dei,"
                },
                {
                    "verse": "che teco a farlo volentier verrei."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "6",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Amor, s'io posso uscir de' tuoi artigli,"
                },
                {
                    "verse": "appena creder posso"
                },
                {
                    "verse": "che alcuno altro uncin mai pi\u00f9 mi pigli."
                }
            ],
            [
                {
                    "verse": "Io entrai giovinetta en la tua guerra,"
                },
                {
                    "verse": "quella credendo somma e dolce pace,"
                },
                {
                    "verse": "e ciascuna mia arma posi in terra,"
                },
                {
                    "verse": "come sicuro chi si fida face:"
                },
                {
                    "verse": "tu, disleal tiranno, aspro e rapace,"
                },
                {
                    "verse": "tosto mi fosti adosso"
                },
                {
                    "verse": "con le tue armi e co' crudel roncigli."
                }
            ],
            [
                {
                    "verse": "Poi, circundata delle tue catene,"
                },
                {
                    "verse": "a quel che nacque per la morte mia,"
                },
                {
                    "verse": "piena d'amare lagrime e di pene"
                },
                {
                    "verse": "presa mi desti, e hammi in sua balia;"
                },
                {
                    "verse": "e \u00e8 s\u00ec cruda la sua signoria,"
                },
                {
                    "verse": "che giammai non l'ha mosso"
                },
                {
                    "verse": "sospir n\u00e9 pianto alcun che m'asottigli."
                }
            ],
            [
                {
                    "verse": "Li prieghi miei tutti glien porta il vento:"
                },
                {
                    "verse": "nullo n'ascolta n\u00e9 ne vuole udire,"
                },
                {
                    "verse": "per che ognora cresce il mio tormento,"
                },
                {
                    "verse": "onde 'l viver m'\u00e8 noia n\u00e9 so morire."
                },
                {
                    "verse": "Deh! dolgati, signor, del mio languire,"
                },
                {
                    "verse": "fa tu quel ch'io non posso:"
                },
                {
                    "verse": "dalmi legato dentro a' tuoi vincigli."
                }
            ],
            [
                {
                    "verse": "Se questo far non vuogli, almeno sciogli"
                },
                {
                    "verse": "i legami annodati da speranza."
                },
                {
                    "verse": "Deh! io ti priego, signor, che tu vogli;"
                },
                {
                    "verse": "ch\u00e9, se tu 'l fai, ancor porto fidanza"
                },
                {
                    "verse": "di tornar bella qual fu mia usanza,"
                },
                {
                    "verse": "e, il dolor rimosso,"
                },
                {
                    "verse": "di bianchi fiori ornarmi e di vermigli."
                }
            ]
        ]
    },
    {
        "author": "Giovanni Boccaccio",
        "work": "Canzoni del Decameron",
        "title": "7",
        "structure": "ballata",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001339",
        "text": [
            [
                {
                    "verse": "Deh lassa la mia vita!"
                },
                {
                    "verse": "Sar\u00e0 giammai ch'io possa ritornar"
                },
                {
                    "verse": "donde mi tolse noiosa partita?"
                }
            ],
            [
                {
                    "verse": "Certo io non so, tanto \u00e8 'l disio focoso,"
                },
                {
                    "verse": "che io porto nel petto,"
                },
                {
                    "verse": "di ritrovarmi ov'io, lassa, gi\u00e0 fui."
                },
                {
                    "verse": "O caro bene, o solo mio riposo,"
                },
                {
                    "verse": "che 'l mio cuor tien distretto,"
                },
                {
                    "verse": "deh dilmi tu, ch\u00e9 'l domandarne altrui"
                },
                {
                    "verse": "non oso, n\u00e9 so cui."
                },
                {
                    "verse": "Deh, signor mio, deh fammelo sperare,"
                },
                {
                    "verse": "si ch'io conforti l'anima smarrita."
                }
            ],
            [
                {
                    "verse": "Io non so ben ridir qual fu 'l piacere"
                },
                {
                    "verse": "che s\u00ec m'ha infiammata,"
                },
                {
                    "verse": "che io non trovo d\u00ec n\u00e9 notte loco."
                },
                {
                    "verse": "Per che l'udire e 'l sentire e 'l vedere"
                },
                {
                    "verse": "con forza non usata"
                },
                {
                    "verse": "ciascun per s\u00e9 accese nuovo foco,"
                },
                {
                    "verse": "nel qual tutta mi coco;"
                },
                {
                    "verse": "n\u00e9 mi pu\u00f2 altri che tu confortare"
                },
                {
                    "verse": "o ritornar la virt\u00f9 sbigottita."
                }
            ],
            [
                {
                    "verse": "Deh dimmi s'esser dee e quando fia"
                },
                {
                    "verse": "ch'io ti trovi giammai"
                },
                {
                    "verse": "dov'io basciai quegli occhi che m'han morta;"
                },
                {
                    "verse": "dimmel, caro mio bene, anima mia,"
                },
                {
                    "verse": "quando tu vi verrai,"
                },
                {
                    "verse": "e col dir'Tosto' alquanto mi conforta."
                },
                {
                    "verse": "Sia la dimora corta"
                },
                {
                    "verse": "d'ora al venire e poi lunga allo stare,"
                },
                {
                    "verse": "ch'io non men curo, s\u00ec m'ha Amor ferita."
                }
            ],
            [
                {
                    "verse": "Se egli avvien che io mai pi\u00f9 ti tenga,"
                },
                {
                    "verse": "non so s'io sar\u00f2 sciocca,"
                },
                {
                    "verse": "com'io or fui a lasciarti partire."
                },
                {
                    "verse": "Io ti terr\u00f2, e che pu\u00f2 s\u00ec n'avenga;"
                },
                {
                    "verse": "e della dolce bocca"
                },
                {
                    "verse": "convien ch'io sodisfaccia al mio disire."
                },
                {
                    "verse": "D'altro non voglio or dire:"
