[
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "1.",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Queste da penna umile impresse note,"
                },
                {
                    "verse": "Son d'inesperta mano opra, e lavoro."
                },
                {
                    "verse": "L\u00e0 d'Elicon' sul monte"
                },
                {
                    "verse": "Ricetto amico a l'Apollineo coro;"
                },
                {
                    "verse": "L\u00e0 d'Ippocrene al fonte,"
                },
                {
                    "verse": "Per cui famoso \u00e8 il Mantovan Cantore;"
                },
                {
                    "verse": "Per cui l'Argivo Vate ha eterno onore;"
                },
                {
                    "verse": "Ed immortale or vive"
                },
                {
                    "verse": "Il fervido Tebano, e il suo Seguace;"
                },
                {
                    "verse": "Ascender volli audace,"
                },
                {
                    "verse": "E le corde toccar di ardita cetra,"
                },
                {
                    "verse": "Sedendo accanto d'Aganippe a l'acque."
                },
                {
                    "verse": "Se il canto mio dispiacque"
                },
                {
                    "verse": "Al Ceto abitator di Pindo ameno,"
                },
                {
                    "verse": "Mi sia benigno il Leggitore almeno."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "4.",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Dal cavo speco orribile,"
                },
                {
                    "verse": "De' venti atra magione,"
                },
                {
                    "verse": "Ove s'annida l'Affrico,"
                },
                {
                    "verse": "Il E il Noto, e l'Aquilone;"
                }
            ],
            [
                {
                    "verse": "Cinta da tale orrifica"
                },
                {
                    "verse": "Turba, alle navi infesta"
                },
                {
                    "verse": "E Con pi\u00e8 furente, e celere"
                },
                {
                    "verse": "Gi\u00e0 sbuca la tempesta."
                }
            ],
            [
                {
                    "verse": "Discende a lei su' l'omero"
                },
                {
                    "verse": "L'irto ceruleo crine,"
                },
                {
                    "verse": "E bieca a mirar ponesi"
                },
                {
                    "verse": "Le ondose acque marine."
                }
            ],
            [
                {
                    "verse": "Vede il tranquillo pelago"
                },
                {
                    "verse": "Moversi gorgogliando,"
                },
                {
                    "verse": "Ed aleggiare il zeffiro,"
                },
                {
                    "verse": "I suoi flutti increspando."
                }
            ],
            [
                {
                    "verse": "Mira le navi placide,"
                },
                {
                    "verse": "Spinte d'amico vento"
                },
                {
                    "verse": "Solcar col rostro ferreo"
                },
                {
                    "verse": "L'infido, ampio elemento."
                }
            ],
            [
                {
                    "verse": "Invida, irata, e torbida"
                },
                {
                    "verse": "Il passo avverso arresta,"
                },
                {
                    "verse": "E il crudo sdegno, fervido"
                },
                {
                    "verse": "Nel petto a lei si desta."
                }
            ],
            [
                {
                    "verse": "Mirar non puote scorrere"
                },
                {
                    "verse": "Tranquillamente l'onde,"
                },
                {
                    "verse": "Ma vuol, che le sconvolgano"
                },
                {
                    "verse": "Procelle furibonde."
                }
            ],
            [
                {
                    "verse": "Gli austri il suo cenno ascoltano"
                },
                {
                    "verse": "Umili, ed ubidienti,"
                },
                {
                    "verse": "E contro de' l'Oceano"
                },
                {
                    "verse": "Si scagliano furenti."
                }
            ],
            [
                {
                    "verse": "Ecco d'intorno oscurasi"
                },
                {
                    "verse": "Ottenebrato il cielo,"
                },
                {
                    "verse": "E lo ricopre un torbido,"
                },
                {
                    "verse": "Atro-funesto velo."
                }
            ],
            [
                {
                    "verse": "Striscia fra dense nuvole"
                },
                {
                    "verse": "Il lampo, e col fulgore"
                },
                {
                    "verse": "Veloce il cielo illumina"
                },
                {
                    "verse": "E inspira alto terrore"
                }
            ],
            [
                {
                    "verse": "Dai sommi poli eterei"
                },
                {
                    "verse": "Il tuono strepitoso"
                },
                {
                    "verse": "Muggisce con orrisono"
                },
                {
                    "verse": "Fragore spaventoso."
                }
            ],
            [
                {
                    "verse": "Per il sulfureo folgore"
                },
                {
                    "verse": "Ardere il ciel gi\u00e0 sembra,"
                },
                {
                    "verse": "E ognuno ha fredde, e gelide"
                },
                {
                    "verse": "Le palpitanti membra."
                }
            ],
            [
                {
                    "verse": "Ma lo sconvolto pelago"
                },
                {
                    "verse": "Alzando i suoi spumanti"
                },
                {
                    "verse": "Flutti nembosi, e torbidi,"
                },
                {
                    "verse": "Assorbe i naviganti."
                }
            ],
            [
                {
                    "verse": "E poppe, e prore vedonsi"
                },
                {
                    "verse": "Infrante galleggiare,"
                },
                {
                    "verse": "E vele, e sarte, e gomene"
                },
                {
                    "verse": "Per l'Oceano errare."
                }
            ],
            [
                {
                    "verse": "I venti alfin s'acchetano,"
                },
                {
                    "verse": "E la tempesta altera"
                },
                {
                    "verse": "Torna mugghiando a chiudersi"
                },
                {
                    "verse": "Nella magion sua nera."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "5.",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Stavan certi Filosofi parlando"
                },
                {
                    "verse": "Assisi intorno in dotta compagn\u00eca,"
                },
                {
                    "verse": "E de' l'alma de' bruti quistionando."
                }
            ],
            [
                {
                    "verse": "Spiegando una sottil filosof\u00eca"
                },
                {
                    "verse": "Nel sostenersi eran' cotanto ardenti,"
                },
                {
                    "verse": "Ch'esser bestia qualcun voluto avr\u00eca."
                }
            ],
            [
                {
                    "verse": "Provavano il parer con argomenti"
                },
                {
                    "verse": "Da intimorire e Socrate, e Platone,"
                },
                {
                    "verse": "Se quivi stati fossero presenti."
                }
            ],
            [
                {
                    "verse": "Ciascuno si credea di aver ragione,"
                },
                {
                    "verse": "Come spesso succede, e a dirla invero,"
                },
                {
                    "verse": "Al mondo non vi fu simil quistione."
                }
            ],
            [
                {
                    "verse": "Chi spirto la credeva inquieto, e fiero;"
                },
                {
                    "verse": "Chi macchina insensata la chiamava;"
                },
                {
                    "verse": "E chi con bizzarrissimo pensiero"
                }
            ],
            [
                {
                    "verse": "Che fossero Demonj contestava,"
                },
                {
                    "verse": "Che stasser de' le bestie al corpo uniti,"
                },
                {
                    "verse": "E in questo suo parer fisso restava."
                }
            ],
            [
                {
                    "verse": "V'erano alcuni poi cotanto arditi,"
                },
                {
                    "verse": "Che credevan, che fossero immortali;"
                },
                {
                    "verse": "Spropositi mai pi\u00f9 nel mondo uditi!"
                }
            ],
            [
                {
                    "verse": "Bella cosa veder per gli animali"
                },
                {
                    "verse": "Applicati, e Filosofi, e Dottori,"
                },
                {
                    "verse": "Come Avvocati a cause criminali."
                }
            ],
            [
                {
                    "verse": "Sembravan tanti nobili Oratori,"
                },
                {
                    "verse": "E si sapean difendere talmente,"
                },
                {
                    "verse": "Che avuti Ciceron n'avr\u00eca timori."
                }
            ],
            [
                {
                    "verse": "Mentre la dotta turma, ed eloquente"
                },
                {
                    "verse": "Pensava al suo parer' onde provarlo;"
                },
                {
                    "verse": "Ne' la camera un cane entra repente."
                }
            ],
            [
                {
                    "verse": "Presto dov'\u00e8 il baston? per discacciarlo"
                },
                {
                    "verse": "Ognun contro gli v\u00e0; ma il cane umile"
                },
                {
                    "verse": "Chiede a loro, che vogliano ascoltarlo."
                }
            ],
            [
                {
                    "verse": "Essi per poco acchetano la bile,"
                },
                {
                    "verse": "E il can, che conoscea coteste inquiete"
                },
                {
                    "verse": "Risse, cos\u00ec parl\u00f2 nel proprio stile."
                }
            ],
            [
                {
                    "verse": "A voi Signori miei gli occhi volgete,"
                },
                {
                    "verse": "Osservate voi stessi indi di noi,"
                },
                {
                    "verse": "se non vi spiace, ragionar potrete."
                }
            ],
            [
                {
                    "verse": "Doveva il saggio can parlare a voi,"
                },
                {
                    "verse": "Che de' difetti altrui sol vi curate,"
                },
                {
                    "verse": "Gli altri soltanto riprendete, e poi"
                }
            ],
            [
                {
                    "verse": "Voi stessi in obblivion sempre lasciate."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "6.",
        "structure": "sonetto",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Quegli, che gi\u00e0 de' Galli ebbe l'impero,"
                },
                {
                    "verse": "Che il Brittanno dom\u00f2 ardir pugnace,"
                },
                {
                    "verse": "Che con l'aspetto spaventoso, e fiero"
                },
                {
                    "verse": "F\u00e8 tremar l'universo; estinto giace."
                }
            ],
            [
                {
                    "verse": "Vittima ci cadde de l'orgoglio altero,"
                },
                {
                    "verse": "De la sfrenata ambizione audace;"
                },
                {
                    "verse": "Cadde, e cadendo f\u00e8 nel mondo intero"
                },
                {
                    "verse": "Arder di guerra la terribil face."
                }
            ],
            [
                {
                    "verse": "Ecco la curia d'atro sangue tinta,"
                },
                {
                    "verse": "Ecco morto il gran Duce, ecco i pugnali;"
                },
                {
                    "verse": "Ma resta ancor la Libertade avvinta:"
                }
            ],
            [
                {
                    "verse": "E di Cocito su la nera foce"
                },
                {
                    "verse": "Freme, presaga de' futuri mali,"
                },
                {
                    "verse": "Di Bruto la sdegnosa ombra, feroce."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "13.",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "O Roma, o nobile citt\u00e0, immortale,"
                },
                {
                    "verse": "Dunque il tuo seno me non accoglie,"
                },
                {
                    "verse": "E l'amor patrio in me non vale?"
                }
            ],
            [
                {
                    "verse": "Or tutto \u00e8 tacito, e un denso velo,"
                },
                {
                    "verse": "Copre le stelle, che incerte splendono,"
                },
                {
                    "verse": "Tutto \u00e8 in silenzio la terra, e il cielo."
                }
            ],
            [
                {
                    "verse": "O Fiume, o Tevere, ah tu mi vieti"
                },
                {
                    "verse": "Tornare ai patrj tetti, amatissimi"
                },
                {
                    "verse": "Giorni a trascorrere contenti, e lieti."
                }
            ],
            [
                {
                    "verse": "Ma un morir nobile pi\u00f9 assai mi giova,"
                },
                {
                    "verse": "Che star fra vincoli d'umil ostaggio,"
                },
                {
                    "verse": "Che morte barbara ognor rinuova."
                }
            ],
            [
                {
                    "verse": "Che pensi, o Clelia? ah non rammenti"
                },
                {
                    "verse": "L'illustre sangue, la patria gloria?"
                },
                {
                    "verse": "E ancor tu palpiti?... e ancor paventi?"
                }
            ],
            [
                {
                    "verse": "Dunque... disprezzisi l'alto periglio;"
                },
                {
                    "verse": "Roma a te vengo, Giove, soccorrimi,"
                },
                {
                    "verse": "Volgi a me provvido pietoso il ciglio."
                }
            ],
            [
                {
                    "verse": "S\u00ec dice intrepida Clelia pensosa;"
                },
                {
                    "verse": "Il cielo mira, a Roma volgesi,"
                },
                {
                    "verse": "E lieve lanciasi sull'acqua ondosa."
                }
            ],
            [
                {
                    "verse": "Clelia, deh fermati, ah mira il dorso"
                },
                {
                    "verse": "Spazioso, e vasto del Sume rapido,"
                },
                {
                    "verse": "Che gonfio mormora nell'ampio corso."
                }
            ],
            [
                {
                    "verse": "Ma non ascoltami, e gi\u00e0 veloce"
                },
                {
                    "verse": "Lieta galleggia l'ostaggio odiabile"
                },
                {
                    "verse": "Mirando in nobile atto feroce."
                }
            ],
            [
                {
                    "verse": "L'onde ammiraronla, stupiron l'acque,"
                },
                {
                    "verse": "E meno altere vidersi scorrere;"
                },
                {
                    "verse": "Il padre Tevere pensoso tacque."
                }
            ],
            [
                {
                    "verse": "Gi\u00e0 la Romulea opposta sponda"
                },
                {
                    "verse": "Festosa afferra, e ad essa lanciasi,"
                },
                {
                    "verse": "E cos\u00ec esprimesi lieta, e gioconda."
                }
            ],
            [
                {
                    "verse": "O Nume, o Apolline, che la cittate"
                },
                {
                    "verse": "Saggio proteggi del forte Romolo,"
                },
                {
                    "verse": "Gi\u00e0 mira Clelia le mura ingrate."
                }
            ],
            [
                {
                    "verse": "Se me scacciarono io pur ritorno"
                },
                {
                    "verse": "N\u00e8 al mio coraggio pon l'onda ostacolo"
                },
                {
                    "verse": "Ma fu dal Tevere fatto pi\u00f9 adorno."
                }
            ],
            [
                {
                    "verse": "Gioisci, o nobile possente Roma,"
                },
                {
                    "verse": "Che la magnanima illustre Clelia"
                },
                {
                    "verse": "Da' lacci ferrei n\u00f2 non fu doma."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "14.",
        "structure": "non-definito",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Dove... che miro, o cieli? odo il suonante"
                },
                {
                    "verse": "Nembo di colpi, il crin Caino afferra"
                },
                {
                    "verse": "Rabbioso al pio Fratello, e con le piante"
                },
                {
                    "verse": "Il preme, e crolla furibondo a terra."
                }
            ],
            [
                {
                    "verse": "Lo sfracella furioso, e l'odio, e l'ira"
                },
                {
                    "verse": "Compagni son ne l'uccision crudele;"
                },
                {
                    "verse": "Spezzato il cranio, e disanguato spira,"
                },
                {
                    "verse": "Lacero, e d'atra polve intriso Abele."
                }
            ],
            [
                {
                    "verse": "De' l'empia morte inonorata giace"
                },
                {
                    "verse": "La vittima primiera, ecco gi\u00e0 tinto"
                },
                {
                    "verse": "Di sangue il suolo l'inamabil face"
                },
                {
                    "verse": "Erge Discordia, e mira il corpo estinto."
                }
            ],
            [
                {
                    "verse": "Tripudia, e mostra il ferro suo stillante"
                },
                {
                    "verse": "L'atroce invidia, ed il crudel furore"
                },
                {
                    "verse": "Legge, e dover calpesta scintillante"
                },
                {
                    "verse": "Da le accese pupille odio, e livore."
                }
            ],
            [
                {
                    "verse": "Ma tuona il cielo, e lampeggiando scuote"
                },
                {
                    "verse": "La folgore tremenda, e per gli ameni,"
                },
                {
                    "verse": "Aerei campi su le ignite ruote,"
                },
                {
                    "verse": "Scorre sdegnato il Nume infra i baleni."
                }
            ],
            [
                {
                    "verse": "Tremano i mostri, e spaventati, e mesti"
                },
                {
                    "verse": "Fuggon' veloci, ma gi\u00e0 s'apre il suolo,"
                },
                {
                    "verse": "Piomban' frementi ai regni atro-funesti,"
                },
                {
                    "verse": "A la magion d'interminabil duolo."
                }
            ],
            [
                {
                    "verse": "Da le spelonche oscure, e cavernose"
                },
                {
                    "verse": "Mugge la terra, e si sconvolgon l'acque,"
                },
                {
                    "verse": "S'alzano al cielo l'onde alte, e spumose:"
                },
                {
                    "verse": "Tanto il delitto al sommo Dio dispiacque!"
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Composizioni per il saggio (1810)",
        "title": "15.",
        "structure": "canzone",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001105",
        "text": [
            [
                {
                    "verse": "Vinto Saulle? il trionfante, il forte,"
                },
                {
                    "verse": "Il vincitor di mille schiere, e mille,"
                },
                {
                    "verse": "Cui vide il campo ostile"
                },
                {
                    "verse": "Grondante in faccia di onorate stille?"
                },
                {
                    "verse": "Di cui sperimentar l'alta possanza"
                },
                {
                    "verse": "Il forte Naas, e gli Ammoniti alteri?"
                },
                {
                    "verse": "Lui che possenti, e fieri"
                },
                {
                    "verse": "Popoli assoggett\u00f2, che ognor si vide"
                },
                {
                    "verse": "Di replicati allori ornato, e cinto;"
                },
                {
                    "verse": "Sconfitto ci cade inonorato, e vinto?"
                }
            ],
            [
                {
                    "verse": "Dunque fia ver, che d'Israello io miri"
                },
                {
                    "verse": "Scosso l'impero con pupille immote,"
                },
                {
                    "verse": "Ed il nemico altero"
                },
                {
                    "verse": "Su le veloci andar vittrici ruote?"
                },
                {
                    "verse": "Dunque fia ver, che inutilmente appenda"
                },
                {
                    "verse": "Al fianco il brando inoperoso, e vile?"
                },
                {
                    "verse": "N\u00f2 se il valore ostile"
                },
                {
                    "verse": "Superar non potesti, a tanti mali"
                },
                {
                    "verse": "Tu sottrar mi dovrai... giorno funesto!"
                },
                {
                    "verse": "N\u00f2, non vedr\u00f2 de le tue ore il resto."
                }
            ],
            [
                {
                    "verse": "Come talor su' le Garganie selve"
                },
                {
                    "verse": "Antico faggio, ovver quercia frondosa"
                },
                {
                    "verse": "Da l'Aquilon percosso"
                },
                {
                    "verse": "Il forte tronco, e l'alta cima annosa"
                },
                {
                    "verse": "Cade atterrata, e la cervice altera"
                },
                {
                    "verse": "Abbassa, e scuote la superba chioma,"
                },
                {
                    "verse": "Da l'Austro ancor non doma:"
                },
                {
                    "verse": "Cos\u00ec trafitto dal funesto ferro"
                },
                {
                    "verse": "Cade Saulle, e sul terren si aggira,"
                },
                {
                    "verse": "Spirando ancor dagli occhi orgoglio, ed ira."
                }
            ],
            [
                {
                    "verse": "Morte... sdegno... furore... ombre fatali..."
                },
                {
                    "verse": "L'insolito pallor... gli orrendi spetri..."
                },
                {
                    "verse": "L'immagini funeste..."
                },
                {
                    "verse": "Larve... pensieri spaventosi, e tetri;"
                },
                {
                    "verse": "Tutto d'innanzi inaspettata scena"
                },
                {
                    "verse": "Gli mostra... oim\u00e8 qual improviso lume"
                },
                {
                    "verse": "Gli balena a la mente? ah il Nume, il Nume..."
                },
                {
                    "verse": "Il cielo oim\u00e8 sprezzai"
                },
                {
                    "verse": "Del Dio vendicator giusto, \u00e8 lo sdegno"
                },
                {
                    "verse": "Tutto perdetti, e la corona, e il regno!"
                }
            ],
            [
                {
                    "verse": "S\u00ec dice, e bieco intorno il guardo volge,"
                },
                {
                    "verse": "E de le ferree spade infra il romore"
                },
                {
                    "verse": "Mira un guerriero: ah vieni,"
                },
                {
                    "verse": "Vieni, gli dice, dal trafitto cuore"
                },
                {
                    "verse": "L'alma non si disgiunge, ah tu m'uccidi."
                },
                {
