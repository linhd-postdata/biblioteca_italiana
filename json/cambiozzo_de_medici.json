[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000269",
        "author": "Cambiozzo de' Medici",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Poi che superbia ti vince e (di)sbalestra"
                },
                {
                    "verse": "contro a chi dietro (ti) vien sempre pregando,"
                },
                {
                    "verse": "udir non vuoi, ma fuggi e, disdegnando,"
                },
                {
                    "verse": "tu sì ten vai con la cera alpestra."
                }
            ],
            [
                {
                    "verse": "Non fu mai fiera sì indomita e silvestra,"
                },
                {
                    "verse": "ogni selvaggio uccel si vien conciando,"
                },
                {
                    "verse": "e tu del tuo error t'avedrai quando"
                },
                {
                    "verse": "a Lucifer sedrai dalla man destra."
                }
            ],
            [
                {
                    "verse": "Non creder tu che giovinile etate"
                },
                {
                    "verse": "sempre mai basti, ma, come vedi, vola:"
                },
                {
                    "verse": "pià forte va ch'uccel battendo l'ale."
                }
            ],
            [
                {
                    "verse": "Superbia non si prezza, ma umiltate;"
                },
                {
                    "verse": "deh, fa' che 'ntenda ciascuna parola,"
                },
                {
                    "verse": "ché 'l pentersi al da sezzo nulla vale."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000269",
        "author": "Cambiozzo de' Medici",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Di nuova rima mi convien far versi."
                },
                {
                    "verse": "Poi che speranza da me s'è partita"
                },
                {
                    "verse": "e 'l dolore pur cresce e sì m'invita,"
                },
                {
                    "verse": "temo di non andar fra i somersi."
                }
            ],
            [
                {
                    "verse": "Omè, Fortuna, perché ti riversi"
                },
                {
                    "verse": "sopra di me, che la vita ho smarrita"
                },
                {
                    "verse": "e la mia fede è suta mal gradita"
                },
                {
                    "verse": "e tutti i ben ch'i' ho fatti si son persi?"
                }
            ],
            [
                {
                    "verse": "Invoco Venere alla vita angosciosa"
                },
                {
                    "verse": "e voi benigne stelle, che 'l ciel rotate,"
                },
                {
                    "verse": "tosto piatù, con tutto sia di terra;"
                }
            ],
            [
                {
                    "verse": "e l'alma aflitta, che giù non ha posa,"
                },
                {
                    "verse": "adunque priega tua umilitate"
                },
                {
                    "verse": "che pace segua della mortal guerra."
                }
            ]
        ]
    }
]