[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "I' fuggi' l'ombra che rimuove il sole"
                },
                {
                    "verse": "per amor d'una vaga e cruda stella,"
                },
                {
                    "verse": "che 'n fin qua giù m'accende di suo foco;"
                },
                {
                    "verse": "e sospirando giunsi in mezzo un bosco"
                },
                {
                    "verse": "d'un laüreto, c'ha le fronde d'oro,"
                },
                {
                    "verse": "tal che' be' rami mi scampâr da morte."
                }
            ],
            [
                {
                    "verse": "In questi rami mai non poté morte,"
                },
                {
                    "verse": "tanto benigno gli coperse il sole,"
                },
                {
                    "verse": "quand'a Fetonte scorse il carro d'oro."
                },
                {
                    "verse": "Po' corsi al lume della bella stella,"
                },
                {
                    "verse": "che pauroso mi condusse al bosco,"
                },
                {
                    "verse": "ch'Apollo accese d'un amaro foco."
                }
            ],
            [
                {
                    "verse": "Prometeo salse al ciel e furò 'l foco,"
                },
                {
                    "verse": "che fu cagion della suo cruda morte,"
                },
                {
                    "verse": "ed altri amando fur conversi in bosco;"
                },
                {
                    "verse": "ed io traggo la morte di quel sole,"
                },
                {
                    "verse": "che Giove fé cadere in pioggia d'oro,"
                },
                {
                    "verse": "per tôr di terra una benigna stella."
                }
            ],
            [
                {
                    "verse": "O sacrata Ciprigna! o santa stella!"
                },
                {
                    "verse": "concedi al messo tuo le forze e 'l foco"
                },
                {
                    "verse": "e l'arco e la faretra e lo stral d'oro,"
                },
                {
                    "verse": "e ferisca colei che mi dà morte,"
                },
                {
                    "verse": "acciò che con la luce del suo sole"
                },
                {
                    "verse": "esca col giorno dell'ombroso bosco."
                }
            ],
            [
                {
                    "verse": "Satiri, ninfe, faüni del bosco,"
                },
                {
                    "verse": "voi vi godete con la vostra stella"
                },
                {
                    "verse": "sotto i be' rami alla spera del sole;"
                },
                {
                    "verse": "e la fenice, ardendo io mezzo 'l foco,"
                },
                {
                    "verse": "si fa più bella, ed io trovo la morte"
                },
                {
                    "verse": "specchiando nel bel viso e cape' d'oro."
                }
            ],
            [
                {
                    "verse": "Vien l'aürora: el ciel rosseggia in oro"
                },
                {
                    "verse": "e la bianca pruina cuopre il bosco."
                },
                {
                    "verse": "Allor mi sveglio e cerco della morte,"
                },
                {
                    "verse": "che vien pe' raggi della forte stella."
                },
                {
                    "verse": "Po', così lasso, e' mi consuma il foco,"
                },
                {
                    "verse": "ché le faville sua si fanno un sole."
                },
                {
                    "verse": "Scuopre sole le chiome e' raggi d'oro"
                },
                {
                    "verse": "sopra del bosco un amoroso foco,"
                },
                {
                    "verse": "che mi dà morte al seren della stella."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Qual fé Pigmalïon, mosso el gran Giove,"
                },
                {
                    "verse": "l'un per l'immago, e l'altro in pioggia d'oro"
                },
                {
                    "verse": "cadde nel dolce albergo al bel tesoro,"
                },
                {
                    "verse": "lasciando 'l cielo ed altra luce altrove,"
                }
            ],
            [
                {
                    "verse": "e quel che 'l terzo ciel nutrica e move"
                },
                {
                    "verse": "m'assembra; addunque dal celeste coro"
                },
                {
                    "verse": "veggo Ciprigna al giovenil lavoro"
                },
                {
                    "verse": "dal mar, che, quand'è 'l sol, il dì rinnove."
                }
            ],
            [
                {
                    "verse": "E te, spirto gentile, amor t'ha preso"
                },
                {
                    "verse": "a quella che 'n virtute ogn'altra eccede,"
                },
                {
                    "verse": "onor del mondo e della nostra etate."
                }
            ],
            [
                {
                    "verse": "E come in fiamma sta carbone acceso"
                },
                {
                    "verse": "e ch'è la luce suo chiara si vede,"
                },
                {
                    "verse": "così in bellezza sta sua onestate."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "In ricca gonna una sacrata dea"
                },
                {
                    "verse": "cinta d'un velo, e d'or le chiome avvolte,"
                },
                {
                    "verse": "le seste aperte, e le punte al ciel volte,"
                },
                {
                    "verse": "e serafiche fiamme in man tenea,"
                }
            ],
            [
                {
                    "verse": "con l'ale stese, e d'Argo gli occhi avea,"
                },
                {
                    "verse": "per le varie cagion diverse e molte,"
                },
                {
                    "verse": "le virtù tutte intorno a lei accolte,"
                },
                {
                    "verse": "ché giusto è venerare la sua iddea."
                }
            ],
            [
                {
                    "verse": "Fra Solone e lo scetro appar costei"
                },
                {
                    "verse": "con l'uber piene e, nelle fiamme ardente,"
                },
                {
                    "verse": "preme 'l dolce licor con vista lieta."
                }
            ],
            [
                {
                    "verse": "Quest'è la vera madre degl'iddei,"
                },
                {
                    "verse": "Discrezion santa, e, se ben miri attente,"
                },
                {
                    "verse": "addorna l'uom sì come il ciel pianeta."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "La nebbia, l'alterigia e' pazzi inganni"
                },
                {
                    "verse": "t'han fatto divenir di magra secca,"
                },
                {
                    "verse": "povera, trista, e da' tuo figli mecca,"
                },
                {
                    "verse": "fatta covil d'alocchi e barbagianni."
                }
            ],
            [
                {
                    "verse": "Alban, Veienti, Numantin con gli anni"
                },
                {
                    "verse": "verrai, perché fortuna ti rimbecca,"
                },
                {
                    "verse": "pigli la gruccia omai la gamba stecca,"
                },
                {
                    "verse": "e fa' di tutti un carro a san Giovanni."
                }
            ],
            [
                {
                    "verse": "Penelope nell'aspettare Ulisse"
                },
                {
                    "verse": "fé e disfé tante volte il lavoro"
                },
                {
                    "verse": "che gli apparve nel sol più d'uno eclisse;"
                }
            ],
            [
                {
                    "verse": "Gianson, per acquistare il vèl dell'oro,"
                },
                {
                    "verse": "fece sì che Medea nel cor gli scrisse"
                },
                {
                    "verse": "el modo da domare il brado toro."
                }
            ],
            [
                {
                    "verse": "Quel lauro ch'io adoro,"
                },
                {
                    "verse": "per l'eccelsa virtù che 'l ciel gli ha dato,"
                },
                {
                    "verse": "el grifone e gli olimpi ha sogiogato."
                }
            ],
            [
                {
                    "verse": "Una Musa ho dallato,"
                },
                {
                    "verse": "che scrive versi come fé Carmenta:"
                },
                {
                    "verse": "i' gli do al vento, perch'Apollo il senta."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "V",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Col sacro Appollo in su' più alti acumi,"
                },
                {
                    "verse": "dov'è il bel fonte d'Elicona aprico,"
                },
                {
                    "verse": "ha tratto versi uom delle Muse amico"
                },
                {
                    "verse": "da gran poeti e da compor volumi."
                }
            ],
            [
                {
                    "verse": "I' sono Icaro e tu Dedal me 'mpiumi,"
                },
                {
                    "verse": "ami Callïopè, converso in Pico;"
                },
                {
                    "verse": "oh cicada cantante al sol, nimico"
                },
                {
                    "verse": "di tanti chiari laureati numi!"
                }
            ],
            [
                {
                    "verse": "Giove ringrazio, ché nelle sant'acque"
                },
                {
                    "verse": "di Penneo fusti mia lustrante scorte,"
                },
                {
                    "verse": "com'Ercol di Teseo, se Stige invita."
                }
            ],
            [
                {
                    "verse": "Tuo splendor, tuo virtù già mai non tacque,"
                },
                {
                    "verse": "ma di nostro idïoma è vero essempre,"
                },
                {
                    "verse": "alma che con gl'iddii è sempre unita."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "VI",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "S'avessi l'arme con ch'Appollo vinse"
                },
                {
                    "verse": "Marsia, che con la lira avea tant'arte,"
                },
                {
                    "verse": "i' sare' 'l primo in campo, e non di Marte,"
                },
                {
                    "verse": "ma di chi poetando opera strinse."
                }
            ],
            [
                {
                    "verse": "Ma piccol vento mia barchetta pinse,"
                },
                {
                    "verse": "quantunque abbia di seta e d'or le sarte,"
                },
                {
                    "verse": "a cantar del bel viso, o porre in carte"
                },
                {
                    "verse": "suo nobiltà, che Giove in l'alma cinse."
                }
            ],
            [
                {
                    "verse": "E se 'l vivo valor, ch'amor contende,"
                },
                {
                    "verse": "ne porse il sol, che ne dimostra l'opra,"
                },
                {
                    "verse": "in giudicio a ragion cantando lodo;"
                }
            ],
            [
                {
                    "verse": "ma se spirto gentil chiaro comprende,"
                },
                {
                    "verse": "potrà veder dove' sant'occhi scopra"
                },
                {
                    "verse": "virtù, bellezza ed onestà 'n un nodo."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "VII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quel laur degno, e 'l mirto che m'ingombra"
                },
                {
                    "verse": "d'amor, e tiemmi in dolce stato altero,"
                },
                {
                    "verse": "l'uno è Pompeo, l'altro Caton severo,"
                },
                {
                    "verse": "o Scipio, che' Roman trasse d'ogn'ombra."
                }
            ],
            [
                {
                    "verse": "Il sommo Giove su dal ciel disgombra"
                },
                {
                    "verse": "le più chiare virtù dell'alto impero,"
                },
                {
                    "verse": "per ornar questo sangue, ond'io più spero:"
                },
                {
                    "verse": "felice patria che ritien tal ombra!"
                }
            ],
            [
                {
                    "verse": "Or venga Apollo, ogni Musa, ogni dea"
                },
                {
                    "verse": "a cantar questi, ch'ogni ben ne 'nduce,"
                },
                {
                    "verse": "ch'a dirne mancheria ogni poeta."
                }
            ],
            [
                {
                    "verse": "Lieta Fiorenza, e tu felice, Alfea,"
                },
                {
                    "verse": "che ritenete in voi le sante luce,"
                },
                {
                    "verse": "qual fan la vita mia sempr'esser lieta!"
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "VIII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Quella felicità, che ti trasporta"
                },
                {
                    "verse": "nell'amor di qualunque Cesar chiama"
                },
                {
                    "verse": "porse alla tela tua la santa trama,"
                },
                {
                    "verse": "ch'avolta al subbio fin qui ne conforta."
                }
            ],
            [
                {
                    "verse": "L'alma ne piangerà, com'un ch'è accorta"
                },
                {
                    "verse": "d'esser lontan dal volto che tant'ama;"
                },
                {
                    "verse": "or di fortuna, or del ciel si richiama,"
                },
                {
                    "verse": "fin che speranza sì le fa la scorta."
                }
            ],
            [
                {
                    "verse": "L'uom mandato da Dio in un bel lauro"
                },
                {
                    "verse": "fatto ha la fama sua sì gloriosa,"
                },
                {
                    "verse": "che passa e monti Olimpi, e Caspi e 'l Tauro."
                }
            ],
            [
                {
                    "verse": "L'alta e nobil virtù già mai ascosa"
                },
                {
                    "verse": "non poté star, perché sì gran tesauro,"
                },
                {
                    "verse": "infin che non si mostra, mai non posa."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "IX",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Lauro gentil, quell'angelica luce,"
                },
                {
                    "verse": "che s'addornò del sole e delle stelle,"
                },
                {
                    "verse": "ha tinto in fiamme l'orate quadrelle,"
                },
                {
                    "verse": "per farvi sol beato ov'è più luce."
                }
            ],
            [
                {
                    "verse": "Quest'è 'l trionfo ver di vostra luce,"
                },
                {
                    "verse": "onor, fama dell'alte opere belle;"
                },
                {
                    "verse": "maravigli' ha chi n'ode, o cerne in quelle"
                },
                {
                    "verse": "cose inaudite, uom della patria luce."
                }
            ],
            [
                {
                    "verse": "Dario, Lucio, Pompeo o Allessandro"
                },
                {
                    "verse": "ebbon mai spoglie porporine o d'ostro,"
                },
                {
                    "verse": "ch'al millesimo fussin del bel lauro?"
                }
            ],
            [
                {
                    "verse": "Le Muse e tutti e cigni di Menandro"
                },
                {
                    "verse": "non potrebbon cantar dell'ordin vostro,"
                },
                {
                    "verse": "che per virtute eccede ogni tesauro."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "X",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Vittorïoso, eccelso, inclito e franco"
                },
                {
                    "verse": "per virtù d'arme e sol felice, in parte"
                },
                {
                    "verse": "vola la fama ch'io rigistro in carte,"
                },
                {
                    "verse": "qual di gentil signor, ch'i' vedessi anco"
                }
            ],
            [
                {
                    "verse": "nel teatro alla pugna vil né stanco,"
                },
                {
                    "verse": "ma un Cesar, un Fabio, Iscipio o Marte,"
                },
                {
                    "verse": "tal ch'ottenesti il dì sì degna parte"
                },
                {
                    "verse": "ch'a ridirne il poeta verrie manco."
                }
            ],
            [
                {
                    "verse": "Latin, Greci, Troian, Persi e Caldei,"
                },
                {
                    "verse": "nati a gran fatti, ne farieno storia"
                },
                {
                    "verse": "dell'alta impresa, onde beato sei."
                }
            ],
            [
                {
                    "verse": "Tant'onor hai aggiunto alla tuo gloria,"
                },
                {
                    "verse": "che per men trionforno uomini e dei,"
                },
                {
                    "verse": "tal che 'n perpetuo di te fia memoria."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000346",
        "author": "Bernardo Altoviti",
        "collection": "Poesie",
        "title": "XI",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Gli occhi lipposi ed isdentata e nera,"
                },
                {
                    "verse": "secca, mal fatta, storta, sorda e lenta,"
                },
                {
                    "verse": "che ricompar quando lo 'nfermo tenta,"
                },
                {
                    "verse": "livida, smorta e mostrüosa fera,"
                }
            ],
            [
                {
                    "verse": "Aleto, Tesifòn, Morte e Megera,"
                },
                {
                    "verse": "malvagia e di mal far sol si contenta,"
                },
                {
                    "verse": "della grazia del ciel nimica e spenta,"
                },
                {
                    "verse": "Ingratitudo. ch'oggi el mondo impera."
                }
            ],
            [
                {
                    "verse": "E perché tutti ci mandi nel cagno,"
                },
                {
                    "verse": "Cimon si rilegò nella catena,"
                },
                {
                    "verse": "che Santippo ne pianse e Pompeo Magno."
                }
            ],
            [
                {
                    "verse": "E se la medicina non ti mena,"
                },
                {
                    "verse": "sì ti può dir: « Buon pro ti faccia il bagno,"
                },
                {
                    "verse": "o tu fai co' becchini all'altelena »."
                }
            ],
            [
                {
                    "verse": "Se tu mostri carena,"
                },
                {
                    "verse": "farai calafatar l'occhio di prua"
                },
                {
                    "verse": "d'un collo di cicogna, o d'una grua."
                }
            ]
        ]
    }
]