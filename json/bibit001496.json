[
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "All'Illustrissimo Signor Don Sebastiano Sanchini",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Precettore che da Dio"
                },
                {
                    "verse": "Siate sempre benedetto"
                },
                {
                    "verse": "Ecco celere v'inv\u00eco"
                },
                {
                    "verse": "Pien d'immagini un sonetto"
                }
            ],
            [
                {
                    "verse": "A me par ch'egli sia bello"
                },
                {
                    "verse": "E che possa far onore"
                },
                {
                    "verse": "Alla cetra ed al pennello"
                },
                {
                    "verse": "Del Poeta e del pittore"
                }
            ],
            [
                {
                    "verse": "Precettor giocoso e gajo,"
                },
                {
                    "verse": "Immortal Poeta invitto"
                },
                {
                    "verse": "Che va sempre con il sajo"
                },
                {
                    "verse": "Il sonetto ecco trascritto"
                }
            ],
            [
                {
                    "verse": "Buona notte, e buona sera"
                },
                {
                    "verse": "State a far la vostra nanna,"
                },
                {
                    "verse": "E sopisca l'ombra nera"
                },
                {
                    "verse": "Ogni cura che vi affanna."
                }
            ],
            [
                {
                    "verse": "Indovini di chi sono questi versi."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "All'lllustrissimo Padrone Colendissimo Il Signor Don Sebastiano Sanchini",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Certun scrive, ed a chi scrive?"
                },
                {
                    "verse": "Scrive al suo gran Precettore"
                },
                {
                    "verse": "Che ne' versi suoi gi\u00e0 vive"
                },
                {
                    "verse": "E vivr\u00e0 seppur non muore"
                }
            ],
            [
                {
                    "verse": "Mio Signore pi\u00f9 non tollera"
                },
                {
                    "verse": "La mia musa d'indugiare,"
                },
                {
                    "verse": "Che la vostra fiera collera"
                },
                {
                    "verse": "Non vuol'ella provocare"
                }
            ],
            [
                {
                    "verse": "Ecco dunque quel sonetto,"
                },
                {
                    "verse": "Che da me desiderate,"
                },
                {
                    "verse": "Se sia bello, mi rimetto"
                },
                {
                    "verse": "A voi dotto amico Vate."
                }
            ],
            [
                {
                    "verse": "Il sonetto ors\u00f9 leggete"
                },
                {
                    "verse": "Precettor di Pindo gioja,"
                },
                {
                    "verse": "Che da ognun quando volete"
                },
                {
                    "verse": "Discacciate via la noja."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Paolina Leopardi. I",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "O mia cara Paolina,"
                },
                {
                    "verse": "La Grammatica Latina"
                },
                {
                    "verse": "Molto incomodo a ognun da,"
                },
                {
                    "verse": "E assai dura vi parr\u00e0."
                },
                {
                    "verse": "Io non so cosa mi dire,"
                },
                {
                    "verse": "Ma vi voglio un po sentire"
                },
                {
                    "verse": "A far questi Latinelli,"
                },
                {
                    "verse": "Che son tutti buoni e belli."
                },
                {
                    "verse": "Io son certo, che farete"
                },
                {
                    "verse": "Tanti sbagli, e non saprete"
                },
                {
                    "verse": "Niente dire, e niente fare,"
                },
                {
                    "verse": "Che qualcun; gite a filare,"
                },
                {
                    "verse": "Vi dir\u00e0; ma state attenta,"
                },
                {
                    "verse": "E sarete poi contenta."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Paolina Leopardi. II",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Mi permettono, che inchini"
                },
                {
                    "verse": "La mia nuova Bandettini?"
                },
                {
                    "verse": "Delle scienze l'Eroina"
                },
                {
                    "verse": "L'immortal grande Paolina?"
                },
                {
                    "verse": "Per onore, per decoro"
                },
                {
                    "verse": "Gli si cingano di alloro"
                },
                {
                    "verse": "Quelle tempia fortunate,"
                },
                {
                    "verse": "D'ogn'ingegno caricate."
                },
                {
                    "verse": "Ed in testa glielo metta"
                },
                {
                    "verse": "Quella lingua benedetta,"
                },
                {
                    "verse": "Quella lingua avventurata,"
                },
                {
                    "verse": "Che da questa ora \u00e8 studiata,"
                },
                {
                    "verse": "Che gli apporta tant'onore,"
                },
                {
                    "verse": "Che gli d\u00e0 tanto splendore."
                },
                {
                    "verse": "Io per me sempre far\u00f2"
                },
                {
                    "verse": "Il suo elogio, e ognor sar\u00f2"
                },
                {
                    "verse": "Il suo grande ammiratore,"
                },
                {
                    "verse": "Ed ancor gran seccatore."
                },
                {
                    "verse": "Or baciandogli la mano"
                },
                {
                    "verse": "Io men v\u00f2 da lei lontano."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora C. P. L. MADRIGALE. IV",
        "structure": "madrigale",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "S\u00ec riverisca ognuno"
                },
                {
                    "verse": "L'esemplar delle dame"
                },
                {
                    "verse": "Ammiri ognun la forte alta Eroina"
                },
                {
                    "verse": "Della diletta sua lingua Latina,"
                },
                {
                    "verse": "Il gran Tullio l'onori,"
                },
                {
                    "verse": "E gli offra i verdi allori,"
                },
                {
                    "verse": "E Corilla, e la dotta Bandettini"
                },
                {
                    "verse": "Gli tributino ossequj e umili inchini."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Contessa Paolina Leopardi, Dotta Gramatica, e Letterata. III",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Giorno tanto desiato,"
                },
                {
                    "verse": "Da me sempre sospirato"
                },
                {
                    "verse": "Giungi alfine, io gi\u00e0 ti vedo,"
                },
                {
                    "verse": "Nella seggiola gi\u00e0 siedo."
                },
                {
                    "verse": "Dunque debbo esaminare"
                },
                {
                    "verse": "Delle donne l'esemplare,"
                },
                {
                    "verse": "L'immortal Dama erudita"
                },
                {
                    "verse": "Fin da Tullio riverita?"
                },
                {
                    "verse": "Non son degno, o Precettore,"
                },
                {
                    "verse": "D'aver tale, e tanto onore,"
                },
                {
                    "verse": "Ma lei vuole; incominciamo"
                },
                {
                    "verse": "Dunque amica e ci assidiamo."
                },
                {
                    "verse": "Sta a sentire Cicerone"
                },
                {
                    "verse": "Sta il Porretti in un cantone,"
                },
                {
                    "verse": "Ed il dotto, e buon Donato"
                },
                {
                    "verse": "Sta a sedere al vostro lato"
                },
                {
                    "verse": "Dunque attenta rispondete"
                },
                {
                    "verse": "E contenta ne sarete."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Contessa Paolina L. Erudita Traduttrice di Marco T. C. III",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Torno in campo a riverire"
                },
                {
                    "verse": "Con novello, e grande ardire"
                },
                {
                    "verse": "L'erudita Signorina"
                },
                {
                    "verse": "Dei dottori alta Regina."
                },
                {
                    "verse": "Ella in luogo decoroso"
                },
                {
                    "verse": "Abbia nobile riposo"
                },
                {
                    "verse": "Ne' l'Arcadia alma, e felice,"
                },
                {
                    "verse": "E s\u00ec chiami Doralice,"
                },
                {
                    "verse": "Cicerone l'incoroni"
                },
                {
                    "verse": "E la scienza ad essa doni,"
                },
                {
                    "verse": "Fedro accanto sempre stia"
                },
                {
                    "verse": "Alla sua gran Signor\u00eca,"
                },
                {
                    "verse": "E il Porretti faccia corte"
                },
                {
                    "verse": "Alla donna amica, e forte,"
                },
                {
                    "verse": "Ancor io l'onorer\u00f2"
                },
                {
                    "verse": "Ed ognor l'ammirer\u00f2."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Contessa Paolina Leopardi. IV",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Mentre jeri errando g\u00eca"
                },
                {
                    "verse": "Per l'amica Arcadia mia"
                },
                {
                    "verse": "Fra quei boschi all'improviso"
                },
                {
                    "verse": "Sorse un alto evviva, un riso,"
                },
                {
                    "verse": "E di cetre un grato suono"
                },
                {
                    "verse": "Si sent\u00eca tra quel frastuono,"
                },
                {
                    "verse": "Ed udii fra quel concento"
                },
                {
                    "verse": "Tali versi a grande stento."
                },
                {
                    "verse": "Salve, o donna, amica, e forte,"
                },
                {
                    "verse": "Che temer non dei la morte,"
                },
                {
                    "verse": "Poich\u00e8 sempre il nome tuo"
                },
                {
                    "verse": "Far\u00e0 fronte al ferro suo;"
                },
                {
                    "verse": "Quell'allor che t'incorona"
                },
                {
                    "verse": "Giustamente a te si dona,"
                },
                {
                    "verse": "E vedrai... Qu\u00ec un rumor\u00eco"
                },
                {
                    "verse": "Interruppe il vate, ed io"
                },
                {
                    "verse": "Pi\u00f9 non volli ivi restare,"
                },
                {
                    "verse": "Ma men volli un poco andare"
                },
                {
                    "verse": "Ammirando fra me stesso"
                },
                {
                    "verse": "Questa donna onor del sesso."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Prefazione. V",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Lacrimosa, irta, ed afflitta"
                },
                {
                    "verse": "Abbattuta, e scarmigliata"
                },
                {
                    "verse": "L'alma, e il cuor dal duol trafitta"
                },
                {
                    "verse": "Dall'affanno maltrattata;"
                }
            ],
            [
                {
                    "verse": "Mi presento al vostro piede,"
                },
                {
                    "verse": "Di giustizia innanzi al letto"
                },
                {
                    "verse": "E vi faccio piena fede"
                },
                {
                    "verse": "Che il mio studio fu perfetto."
                }
            ],
            [
                {
                    "verse": "Deh perdon mi concedete"
                },
                {
                    "verse": "S'io commetto qualche sbaglio"
                },
                {
                    "verse": "Che sar\u00e0 come vedrete"
                },
                {
                    "verse": "D'un Somaro uguale al raglio."
                }
            ],
            [
                {
                    "verse": "Spero un poco di pietade"
                },
                {
                    "verse": "Per la mia piccola scienza"
                },
                {
                    "verse": "Dalla vostra umanitade"
                },
                {
                    "verse": "Dalla vostra gran clemenza."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Contessa Paolina Leopardi. V",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Fuvvi un d\u00ec che si potea"
                },
                {
                    "verse": "Dirvi quel che si volea"
                },
                {
                    "verse": "Si potea scherzare un poco"
                },
                {
                    "verse": "Senza farvi andare in fuoco"
                },
                {
                    "verse": "Sentivate questo, e quello"
                },
                {
                    "verse": "Senza prendere un cappello,"
                },
                {
                    "verse": "Senza andar tosto in curina"
                },
                {
                    "verse": "Come il vin nella cantina."
                },
                {
                    "verse": "Noi perci\u00f2 nel quarto esame"
                },
                {
                    "verse": "Con le fette di salame"
                },
                {
                    "verse": "Vi facemmo una corona"
                },
                {
                    "verse": "Da portarsi in elicona,"
                },
                {
                    "verse": "E mostrarsi a quei Poeti"
                },
                {
                    "verse": "Che sen van contenti e lieti"
                },
                {
                    "verse": "Di uno straccio sol d'alloro"
                },
                {
                    "verse": "Comperato a peso d'oro."
                },
                {
                    "verse": "Or per\u00f2 non \u00e8 cos\u00ec"
                },
                {
                    "verse": "S'io volessi in oggid\u00ec"
                },
                {
                    "verse": "Dirvi un p\u00f2 quattro facezie"
                },
                {
                    "verse": "Schiccherar tre o quattro inezie"
                },
                {
                    "verse": "Prendereste voi di botto"
                },
                {
                    "verse": "Un orribile fagotto"
                },
                {
                    "verse": "Pien di polve di cannoni"
                },
                {
                    "verse": "Da sparar per i calzoni."
                },
                {
                    "verse": "Io per\u00f2 prendo il partito"
                },
                {
                    "verse": "Umiliato, ed avvilito"
                },
                {
                    "verse": "Di donare al vostro merto"
                },
                {
                    "verse": "Di melloni, e fichi un serto"
                },
                {
                    "verse": "Da mostrarsi all'Educande"
                },
                {
                    "verse": "Di quel luogo che gi\u00e0 spande"
                },
                {
                    "verse": "La sua fama in tutto il mondo"
                },
                {
                    "verse": "Quanto \u00e8 lungo, e largo, e tondo."
                },
                {
                    "verse": "Ma non v\u00f2 sprecar pi\u00f9 inchiostro"
                },
                {
                    "verse": "Ora andiamo al sito nostro,"
                },
                {
                    "verse": "E poich\u00e8 gi\u00e0 pronti siamo"
                },
                {
                    "verse": "Attenzione. Incominciamo."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Giacomo Leopardi Al suo Amatissimo Genitore Conte Monaldo Leopardi",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Tornasti alfine a tuoi paterni Lari,"
                },
                {
                    "verse": "O Genitor, da noi tanto bramato,"
                },
                {
                    "verse": "Tornasti a rendere il contento amico"
                },
                {
                    "verse": "Al nostro albergo, ed a' tuoi figli insieme."
                },
                {
                    "verse": "Possiamo alfine su l'amata destra"
                },
                {
                    "verse": "Imprimer baci di contento affetto."
                },
                {
                    "verse": "Al suonante fragor del presto cocchio"
                },
                {
                    "verse": "Lieti esultammo, e a noi balzava in petto"
                },
                {
                    "verse": "L'ansioso cuor pel giubilo improviso"
                },
                {
                    "verse": "E terminata la mestizia, e il duolo,"
                },
                {
                    "verse": "E per goder di tua bramata vista"
                },
                {
                    "verse": "Termino anch'io poich\u00e8 ristretto \u00e8 il tempo,"
                },
                {
                    "verse": "In cui vergar mi \u00e8 dato il breve foglio."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Al signor Conte Monaldo Leopardi",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Mentre tu godi le delizie amene"
                },
                {
                    "verse": "Del campo amico, o Genitor diletto,"
                },
                {
                    "verse": "Con rozza penna a te vergare io voglio"
                },
                {
                    "verse": "Un Eliconia carta, onde a te possa"
                },
                {
                    "verse": "La mia stima svelare, e il mio rispetto."
                },
                {
                    "verse": "Nel fonte d'Ippocren la penna intingo,"
                },
                {
                    "verse": "D'alloro cingo l'Apollinea cetra"
                },
                {
                    "verse": "E di mirto la fronte indi m'assido."
                },
                {
                    "verse": "Ma che mai dico? che pretendo? io dunque"
                },
                {
                    "verse": "Udir far\u00f2 della mia cetra il suono"
                },
                {
                    "verse": "Al dotto Genitor? no che i miei carmi"
                },
                {
                    "verse": "Di te degni non son ma tu potrai,"
                },
                {
                    "verse": "Amato padre, compatirli, e insieme"
                },
                {
                    "verse": "Gradirli ancor se ci\u00f2 sperar mi \u00e8 dato."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Giacomo Leopardi Al suo diletto Genitore Dopo due mesi di studj Filosofici",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Per il sassoso monte, a la cui cima altera"
                },
                {
                    "verse": "Ragion siede spirando austerit\u00e0 severa;"
                }
            ],
            [
                {
                    "verse": "L\u00e0 dove in man recando le sapienti carte"
                },
                {
                    "verse": "I rigidi Filosofi accorser d'ogni parte;"
                }
            ],
            [
                {
                    "verse": "Su' cui sal\u00ec Platone, su' cui Socrate ascese,"
                },
                {
                    "verse": "Ed immortale ognuno la gloria sua gi\u00e0 rese;"
                }
            ],
            [
                {
                    "verse": "Quivi a temprare il barbaro, crudo rigor del fato,"
                },
                {
                    "verse": "Le strade filosofiche a noi calcar fu dato."
                }
            ],
            [
                {
                    "verse": "E qu\u00ec vedemmo ascendere tacito, e pensieroso"
                },
                {
                    "verse": "Lo stuol di scienza cupido, e di saper bramoso."
                }
            ],
            [
                {
                    "verse": "Ma quanti, e quanti in volto, e grave, e maestosa"
                },
                {
                    "verse": "Scacci\u00f2 da se Ragione in aria minacciosa!"
                }
            ],
            [
                {
                    "verse": "Oh quanti di Filosofo, quanti des\u00ecano il nome,"
                },
                {
                    "verse": "E di onorevol laurea cinger vorr\u00ecan le chiome!"
                }
            ],
            [
                {
                    "verse": "Ma quanti ond'esser sembrino d'alto saper profondo"
                },
                {
                    "verse": "Con empie, inique massime corromper sanno il mondo"
                }
            ],
            [
                {
                    "verse": "Fra l'atre, oscure tenebre, fra densa nebbia, e folta"
                },
                {
                    "verse": "Ragion purtroppo resta ottenebrata, e avvolta."
                }
            ],
            [
                {
                    "verse": "Ma pur coteste carte essa verg\u00f2 sgombrando"
                },
                {
                    "verse": "Da se le nere nubi, e il volto rischiarando."
                }
            ],
            [
                {
                    "verse": "Qu\u00ec, Genitor, potrai mirar da l'alto soglio"
                },
                {
                    "verse": "La Verit\u00e0 fiaccare degli empj il fiero orgoglio."
                }
            ],
            [
                {
                    "verse": "Cos\u00ec potesse alfine Filosofia scacciare"
                },
                {
                    "verse": "L'empie seguaci turbe, e i chiari rai vibrare:"
                }
            ],
            [
                {
                    "verse": "Per cui Ragion nel trono sublime un d\u00ec si assida,"
                },
                {
                    "verse": "La Religion si avvivi, giubili il mondo, e rida."
                }
            ],
            [
                {
                    "verse": "Cadan negletti, e vinti gl'iniqui dogmi, e stolti;"
                },
                {
                    "verse": "Il Ciel propizio siami, ed i miei voti ascolti."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "Alla Signora Contessa Virginia Mosca-Leopardi",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Gi\u00e0 salisco sul Parnaso"
                },
                {
                    "verse": "Tutto pien di buon umore;"
                },
                {
                    "verse": "Pria mi soffio un poco il naso"
                },
                {
                    "verse": "Ed asciugomi il sudore:"
                }
            ],
            [
                {
                    "verse": "Poi la cetra appendo al collo,"
                },
                {
                    "verse": "E m'assido in su l'erbetta;"
                },
                {
                    "verse": "Gi\u00e0 mi siede al fianco Apollo"
                },
                {
                    "verse": "Colla musa mia diletta."
                }
            ],
            [
                {
                    "verse": "Sotto un verde alloro ombroso"
                },
                {
                    "verse": "Godo il fresco venticello,"
                },
                {
                    "verse": "E m'ispira il mio riposo"
                },
                {
                    "verse": "Estro grande, estro novello."
                }
            ],
            [
                {
                    "verse": "Odi dunque, ava, il mio canto,"
                },
                {
                    "verse": "Che per te gi\u00e0 sciolgo ardito:"
                },
                {
                    "verse": "Esso avr\u00e0 di gloria il vanto,"
                },
                {
                    "verse": "Se sar\u00e0 da te gradito."
                },
                {
                    "verse": "Venti, tacete;"
                },
                {
                    "verse": "Voi rattenete,"
                },
                {
                    "verse": "O fiumi, il corso;"
                },
                {
                    "verse": "Facciasi il dorso"
                },
                {
                    "verse": "Dell'Oceano"
                },
                {
                    "verse": "Tranquillo e piano,"
                },
                {
                    "verse": "Onde cantare"
                },
                {
                    "verse": "Possa le rare"
                },
                {
                    "verse": "Doti ed i pregi"
                },
                {
                    "verse": "Nobili egregi"
                },
                {
                    "verse": "Dell'ava amica."
                },
                {
                    "verse": "Ogni fatica"
                },
                {
                    "verse": "Si adopri pure;"
                },
                {
                    "verse": "Tutte le cure"
                },
                {
                    "verse": "Per opra tale"
                },
                {
                    "verse": "Non fanno male."
                },
                {
                    "verse": "Ma che dir\u00f2?"
                },
                {
                    "verse": "Che far potr\u00f2?"
                },
                {
                    "verse": "Non \u00e8 bastante"
                },
                {
                    "verse": "Per tali e tante"
                },
                {
                    "verse": "Virtudi amate,"
                },
                {
                    "verse": "Virt\u00f9 pregiate,"
                },
                {
                    "verse": "Tutto l'ingegno"
                },
                {
                    "verse": "Tutto l'impegno."
                }
            ],
            [
                {
                    "verse": "Ma \u00e8 gi\u00e0 stanca la mia musa,"
                },
                {
                    "verse": "Non pi\u00f9 sa suonar la lira,"
                },
                {
                    "verse": "D'Elicon la selva ha chiusa,"
                },
                {
                    "verse": "N\u00e8 pi\u00f9 l'estro ella m'ispira."
                }
            ],
            [
                {
                    "verse": "Umilmente la pregai,"
                },
                {
                    "verse": "Ma non volle essa ascoltarmi:"
                },
                {
                    "verse": "Dunque adesso aggradirai"
                },
                {
                    "verse": "Questi mei poveri carmi."
                }
            ]
        ]
    },
    {
        "author": "Giacomo Leopardi",
        "work": "Scritti vari (1810)",
        "title": "PER IL SIGNOR CONTE LUIGI LEOPARDI STORIOGRAFO DELL'ARCHIGINNASIO DI RECANATI",
        "structure": null,
        "url": "http://www.bibliotecaitaliana.it/testo/bibit001496",
        "text": [
            [
                {
                    "verse": "Muse, Apollo, Grazie a voi"
                },
                {
                    "verse": "Marte Padre degli Eroi"
                },
                {
                    "verse": "Voi Saturno, e voi Giunone"
                },
                {
                    "verse": "Voi Morfeo sommo Poltrone"
                },
                {
                    "verse": "Voi Mercurio ladro insigne"
                },
                {
                    "verse": "Parche, e voi poco benigne"
                },
                {
                    "verse": "E voi Momo fortunato,"
                },
                {
                    "verse": "Che vi siete meritato"
                },
                {
                    "verse": "Di prestare il nome vostro"
                }
            ],
            [
                {
                    "verse": "Al sapiente Dottor nostro,"
                },
                {
                    "verse": "O voi tutti alla celeste"
                },
                {
                    "verse": "Porta omai venir dovreste,"
                },
                {
                    "verse": "O vogliamo dir dovete"
                },
                {
                    "verse": "Ad udir come un ragazzo"
                },
                {
                    "verse": "Vi confonde, o popol pazzo"
                },
                {
                    "verse": "E ad ognun dice che Abramo"
                },
                {
                    "verse": "Padre fu del grande Adamo"
                },
                {
                    "verse": "Che di questi il Nonno Abele"
                },
                {
                    "verse": "F\u00e8 la torre di Babele"
                },
                {
                    "verse": "Che Cain Giacobbe uccise"
                },
                {
                    "verse": "E la morte se ne rise"
                },
                {
                    "verse": "E tante altre belle cose"
                },
                {
                    "verse": "Ad ognun di noi nascose"
                },
                {
                    "verse": "Ma soltanto note ad esso,"
                },
                {
                    "verse": "Che trovolle appresso a un cesso"
                },
                {
                    "verse": "E le scrisse e l'impar\u00f2"
                },
                {
                    "verse": "E narrare ora le vu\u00f2."
                },
                {
                    "verse": "Zitti adunque, e state attenti"
                },
                {
                    "verse": "E sarete ben contenti"
                },
                {
                    "verse": "Cose udite aver che sono"
                },
                {
                    "verse": "D'un s\u00ec gran Storico il dono."
                }
            ]
        ]
    }
]