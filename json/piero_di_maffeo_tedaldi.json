[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001055",
        "author": "Piero di Maffeo Tedaldi",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Tu sai la 'nfermità mia de l'altr'anno"
                },
                {
                    "verse": "quanta mi fu noiosa o malagevole"
                },
                {
                    "verse": "a comportalla; e quanta fu spiacevole"
                },
                {
                    "verse": "color che l'han provato sì lo sanno."
                }
            ],
            [
                {
                    "verse": "Poi pur guari', ed è duo volte un anno,"
                },
                {
                    "verse": "o poco men, ch'i' fui di ciò godevole:"
                },
                {
                    "verse": "or vedi ben com'egli è convenevole"
                },
                {
                    "verse": "ch'io vada ricadendo in tale affanno!"
                }
            ],
            [
                {
                    "verse": "Ed io mi sento pur tornar nel core,"
                },
                {
                    "verse": "poi che tornò colui ch'era partito,"
                },
                {
                    "verse": "non so se dico pazzia o amore;"
                }
            ],
            [
                {
                    "verse": "ed io sto in forse e son sì sbigottito"
                },
                {
                    "verse": "ch'i' fo come colui che cerca onore"
                },
                {
                    "verse": "e fo pazzie com'uom del senno uscito."
                }
            ],
            [
                {
                    "verse": "E son sì indebilito"
                },
                {
                    "verse": "che, s'io non ho da te consiglio o regola,"
                },
                {
                    "verse": "sono impaniato come tordo in pegola."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001055",
        "author": "Piero di Maffeo Tedaldi",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Oggi abbiàn lunedì, come tu sai;"
                },
                {
                    "verse": "domani è martedì, com'è usato"
                },
                {
                    "verse": "mercoledì è l'altro nominato;"
                },
                {
                    "verse": "poi giovedì, el qual non falla mai."
                }
            ],
            [
                {
                    "verse": "L'altro so che cognosci, perché sai"
                },
                {
                    "verse": "che carne non si mangia in nessun lato;"
                },
                {
                    "verse": "sabato è l'altro, i' non l'ho smenticato;"
                },
                {
                    "verse": "l'altro è quel dì che a bottega non vai."
                }
            ],
            [
                {
                    "verse": "Qualunque s'è di questi, mille volte"
                },
                {
                    "verse": "hai detto del fornir del fatto mio,"
                },
                {
                    "verse": "e poi mi di' che hai faccende molte."
                }
            ],
            [
                {
                    "verse": "Tu hai faccende men che non ho io;"
                },
                {
                    "verse": "le tue promesse tutte vane e stolte"
                },
                {
                    "verse": "le truovo, con sustanza men ch'un fio."
                }
            ],
            [
                {
                    "verse": "Dimmi stù credi ch'io"
                },
                {
                    "verse": "ne sia servito innanzi al die iudicio;"
                },
                {
                    "verse": "quando che non, rinunzio al beneficio."
                }
            ]
        ]
    }
]