[
    {
        "author": "Jacopo da Bibbiena",
        "work": "Poesie",
        "title": "I",
        "structure": "versi",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000309",
        "text": [
            [
                {
                    "verse": "Gir\u00f2 sempre piangendo con tormento,"
                },
                {
                    "verse": "per far chi n'\u00e8 cagione esser contento,"
                },
                {
                    "verse": "el volto umile e basso, el cor con lutti,"
                },
                {
                    "verse": "lontan per luoghi alpestri, aspri e silvaggi,"
                },
                {
                    "verse": "gli occhi dolenti, mai di pianto asciutti;"
                },
                {
                    "verse": "e, come uom disperato, i vostri raggi"
                },
                {
                    "verse": "fuggir\u00f2 sempre, ben che 'l cor ne caggi"
                },
                {
                    "verse": "in morte; ma che dich'io? Gi\u00e0 la sento."
                }
            ]
        ]
    }
]