        "author": "Lorenzo Damiani",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Porco putente pessimo poltrone,"
                },
                {
                    "verse": "invido iniquo ingrato iscognoscente,"
                },
                {
                    "verse": "e brutto e tristo e vile e da nïente,"
                },
                {
                    "verse": "rustico rio rapace rubaldone,"
                }
            ],
            [
                {
                    "verse": "o serpe oculta odiosa, o condizione"
                },
                {
                    "verse": "di mal dimon dannato, dira mente,"
                },
                {
                    "verse": "inclino in male invidia, incontenente,"
                },
                {
                    "verse": "perir presto postù, perch'è ragione!"
                }
            ],
            [
                {
                    "verse": "Astuto a 'nganni, ardito allo sforzare,"
                },
                {
                    "verse": "roba rubando ruddemente regni,"
                },
                {
                    "verse": "degno di mala dannazion, di morte,"
                }
            ],
            [
                {
                    "verse": "iusto iudicio è 'n punire; indugiare"
                },
                {
                    "verse": "nulla non voglio nel darti; né sdegni"
                },
                {
                    "verse": "oscuro obrobio o forche o mala sorte."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000480",
        "author": "Lorenzo Damiani",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Grata Ciprigna pel bel pome d'oro"
                },
                {
                    "verse": "esser volendo, non favria Elèna"
                },
                {
                    "verse": "rapir, né Tisbe Pirramo a tal pena"
                },
                {
                    "verse": "are' condotto e 'nsanguinato el moro;"
                }
            ],
            [
                {
                    "verse": "resterebbe Giove in ciel mutato toro,"
                },
                {
                    "verse": "d'Europa carco quando via la mena,"
                },
                {
                    "verse": "o non avrebbe mai ingannato Almena;"
                },
                {
                    "verse": "a Danne forma non sarie d'alloro,"
                }
            ],
            [
                {
                    "verse": "gentil da Brigi se stata allor fusse,"
                },
                {
                    "verse": "l'altre tutte avanzando in ogni laude,"
                },
                {
                    "verse": "in virtù singulare onesta e bella."
                }
            ],
            [
                {
                    "verse": "Addunque, più ch'altr'uom lieto, ti gaude;"
                },
                {
                    "verse": "tu, che meritato hai tu solo avella,"
                },
                {
                    "verse": "a Dio da' grazie, ché a tal ti condusse."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000480",
        "author": "Lorenzo Damiani",
        "collection": "Poesie",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Vera — Amicizia — glorïoso — bene,"
                },
                {
                    "verse": "Amicizia — etternal — dono — d'amore;"
                },
                {
                    "verse": "glorïoso — dono, — perpetuo — onore,"
                },
                {
                    "verse": "bene — d'amore — onore — contiene."
                },
                {
                    "verse": "I cieli — el mondo — carità — sostiene,"
                }
            ],
            [
                {
                    "verse": "el mondo — universal — regge — fervore,"
                },
                {
                    "verse": "carità — regge — ciaschedun — buon cuore,"
                },
                {
                    "verse": "sostien — fervore, — buon cuore — e sovviene."
                }
            ],
            [
                {
                    "verse": "Ciascun che ben — desideri — d'avere,"
                },
                {
                    "verse": "desideri — Amicizia — per sostegno,"
                },
                {
                    "verse": "e d'aver — per sostegno — caritate;"
                }
            ],
            [
                {
                    "verse": "ell'è solo quel ben — che ci dà — regno,"
                },
                {
                    "verse": "che ci dà — viver lieto — e possedere"
                }
            ],
            [
                {
                    "verse": "regno — e posseder — felicitate."
                },
                {
                    "verse": "Per me' potere esprimer mio concetto,"
                },
                {
                    "verse": "in doppio modo di rimare ho detto."
                }
            ]
        ]
    }
]