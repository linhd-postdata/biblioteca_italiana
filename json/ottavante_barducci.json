        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Io son nel fondo della magna altezza,"
                },
                {
                    "verse": "fra tanta broda e fra sì grasse micche,"
                },
                {
                    "verse": "che, se sol agli arnion l'odor s'apicche,"
                },
                {
                    "verse": "lasciar dovrei l'antica mia magrezza."
                }
            ],
            [
                {
                    "verse": "E mangio pan ch'ogni vil can lo sprezza;"
                },
                {
                    "verse": "el vin par distillato per limbicche"
                },
                {
                    "verse": "d'un infreddato zugo d'asin, sì che"
                },
                {
                    "verse": "la vista sola ogni stomaco spezza."
                }
            ],
            [
                {
                    "verse": "E seggo a mensa a tavola ritonda,"
                },
                {
                    "verse": "guernita d'un grembiul da dipintore;"
                },
                {
                    "verse": "suvi catini ov'io non truovo fonda."
                }
            ],
            [
                {
                    "verse": "E son in una stufa, onde un odore"
                },
                {
                    "verse": "surge, che quel delle Stinche seconda,"
                },
                {
                    "verse": "e direi me' s'i' dicessi piggiore."
                }
            ],
            [
                {
                    "verse": "Pure sperando onore,"
                },
                {
                    "verse": "pazïente comporto ogn'aspro intoppo;"
                },
                {
                    "verse": "non mi dir poi costì ch'io spendo troppo."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001177",
