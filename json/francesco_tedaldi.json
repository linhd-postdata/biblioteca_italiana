[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "I",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "El mal, che da Fortuna si distilla"
                },
                {
                    "verse": "per oggetto conforme di sue rote,"
                },
                {
                    "verse": "quando più presto l'arco li percuote,"
                },
                {
                    "verse": "di subito fa rompere o aprilla."
                }
            ],
            [
                {
                    "verse": "E se superfluo amor non la distilla,"
                },
                {
                    "verse": "tutte ragion allor li son remote,"
                },
                {
                    "verse": "esse non son però del tutto note"
                },
                {
                    "verse": "perché gran foco fa poca favilla."
                }
            ],
            [
                {
                    "verse": "Né ci dobbian però forte amirare,"
                },
                {
                    "verse": "veggendo che nel ciel già non consiste"
                },
                {
                    "verse": "la forza, donde fa l'opera degna."
                }
            ],
            [
                {
                    "verse": "Ma quel che più ne sa può operare"
                },
                {
                    "verse": "per le ragion che son di virtù miste,"
                },
                {
                    "verse": "la qual fa vita d'ogni laude pregna."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "II",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Abondant'è virtù ne l'alto cielo,"
                },
                {
                    "verse": "che volge all'intelletto lo splendore,"
                },
                {
                    "verse": "di che s'accende nel supremo amore,"
                },
                {
                    "verse": "quando la terza rota ha rotto il velo."
                }
            ],
            [
                {
                    "verse": "Allor, sublime d'amoroso zelo,"
                },
                {
                    "verse": "ripresa la sua forza e 'l suo vigore,"
                },
                {
                    "verse": "abraccia la virtù parte del core"
                },
                {
                    "verse": "che di dolor non ne trarrebbe pelo."
                }
            ],
            [
                {
                    "verse": "Però l'anima nostra sconsolata"
                },
                {
                    "verse": "non per colpa del ciel che la governa,"
                },
                {
                    "verse": "si lamenta del corpo che la regge."
                }
            ],
            [
                {
                    "verse": "E s'egli avien che sia raconsolata"
                },
                {
                    "verse": "per la virtù della suo luce etterna,"
                },
                {
                    "verse": "non debbe esser sommessa a questa legge."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "III",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Per comun corso da natura dato,"
                },
                {
                    "verse": "quella ch'al mio dolor era conforto"
                },
                {
                    "verse": "ed a mia aversità tranquillo porto"
                },
                {
                    "verse": "in cielo è gita e di qua m'ha lasciato."
                }
            ],
            [
                {
                    "verse": "Morte in un punto ogni ben m'ha levato,"
                },
                {
                    "verse": "né so se io son dritto o se son torto,"
                },
                {
                    "verse": "né s'io voglio esser vivo o esser morto,"
                },
                {
                    "verse": "perché sanz'essa sono in basso stato."
                }
            ],
            [
                {
                    "verse": "Ma solo un punto è quel che mi notrica,"
                },
                {
                    "verse": "se 'l qual non fusse, certo io crederei"
                },
                {
                    "verse": "alla Fortuna come a mio maggiore,"
                }
            ],
            [
                {
                    "verse": "ch'ella, parlando meco, par che dica:"
                },
                {
                    "verse": "«Vivimi allegro e non temer di lei,"
                },
                {
                    "verse": "ché per te prego sempre il mio Signore»."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Già non ha molto tempo ch'io solea"
                },
                {
                    "verse": "viver allegro e gir sempre cantando;"
                },
                {
                    "verse": "or doloroso e sempre lagrimando,"
                },
                {
                    "verse": "poi c'ho perduta la mia bella dea."
                }
            ],
            [
                {
                    "verse": "O lass'a me, che per nulla credea,"
                },
                {
                    "verse": "sì bella cosa sempre riguardando,"
                },
                {
                    "verse": "che ora adorna il ciel lassù istando,"
                },
                {
                    "verse": "mal mi venisse, e per certo 'l tenea!"
                }
            ],
            [
                {
                    "verse": "Ma, se chi tutto può lassù la vuole,"
                },
                {
                    "verse": "ben è ragion perch'è degna del loco"
                },
                {
                    "verse": "via più che noi qua giù d'averla in terra."
                }
            ],
            [
                {
                    "verse": "Già di suo morte a me punto non duole,"
                },
                {
                    "verse": "ma ben m'incresce stare in questo foco,"
                },
                {
                    "verse": "fin ch'ella non mi trae di questa guerra."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "IV",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Madonna m'è venuta a vicitare"
                },
                {
                    "verse": "di mezzanotte al letto ov'io dormia,"
                },
                {
                    "verse": "tre belle donne seco in compagnia,"
                },
                {
                    "verse": "le quali in vita solian con lei stare."
                }
            ],
            [
                {
                    "verse": "Gli abiti lor non saprei divisare,"
                },
                {
                    "verse": "ma el color l'una bianco vestia"
                },
                {
                    "verse": "e l'altra verde e la terza paria"
                },
                {
                    "verse": "rossa, come carbon si suol soffiare."
                }
            ],
            [
                {
                    "verse": "Intorno al letto mio le quattro belle"
                },
                {
                    "verse": "donne vi fecion tanto di splendore"
                },
                {
                    "verse": "qual la casa del sol descrive Ovidio."
                }
            ],
            [
                {
                    "verse": "Io amirato di cotal novelle,"
                },
                {
                    "verse": "madonna mi parlò piena d'amore:"
                },
                {
                    "verse": "«Io presto ti trarrò di gran fastidio»."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "VI",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "A Pisa giunsi come arai inteso"
                },
                {
                    "verse": "sano e salvo, la grazia di Dio,"
                },
                {
                    "verse": "e ti vo' dir, carissimo Anton mio,"
                },
                {
                    "verse": "che vita tengo e come son fornito."
                }
            ],
            [
                {
                    "verse": "Marino assai del fritto e del bolito"
                },
                {
                    "verse": "già mai non manca dove mi truov'io;"
                },
                {
                    "verse": "tinche e lucci troppo non li spio,"
                },
                {
                    "verse": "ch'a voi li lasso per far in mortito."
                }
            ],
            [
                {
                    "verse": "E tutti questi nobili mercanti"
                },
                {
                    "verse": "per farmi festa ciascun mi convita"
                },
                {
                    "verse": "e par che d'onorarmi ognun si vanti."
                }
            ],
            [
                {
                    "verse": "Lamprede e cheppie cotte a la pulita,"
                },
                {
                    "verse": "muggin, pagelli, orate mangio tante"
                },
                {
                    "verse": "che stuccheresti a fiutarmi le dita."
                }
            ],
            [
                {
                    "verse": "E per far miglior vita,"
                },
                {
                    "verse": "e' non s'accosti a me Melanconia;"
                },
                {
                    "verse": "razzese beo a pasto e malvagia."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "VII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Sommi nutrito ne la magna Francia"
                },
                {
                    "verse": "a copia di vin buoni e a capponi,"
                },
                {
                    "verse": "fagian, conigli, pernice e pagoni"
                },
                {
                    "verse": "e pesci buon da tener ben la pancia."
                }
            ],
            [
                {
                    "verse": "Or son tornato a carne secca rancia,"
                },
                {
                    "verse": "a vecchi infermi e a magri castroni;"
                },
                {
                    "verse": "vin tristo beo e spesso di cerconi:"
                },
                {
                    "verse": "quando ho del buon, mi par aver la mancia."
                }
            ],
            [
                {
                    "verse": "Pur a le volte un po' di torticina"
                },
                {
                    "verse": "di carne fredda, di formaggio e d'ova,"
                },
                {
                    "verse": "di quella che ci avanza la mattina."
                }
            ],
            [
                {
                    "verse": "In casa nostra spezie non si truova,"
                },
                {
                    "verse": "però non ragionar di gelatina,"
                },
                {
                    "verse": "ché troppo ci parrebbe cosa nuova."
                }
            ],
            [
                {
                    "verse": "E questo sa chi 'l pruova,"
                },
                {
                    "verse": "che giuoco egli è spiacevole e malvagio"
                },
                {
                    "verse": "tornar a questa vita da quell'agio."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "VIII",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Antonio, spirto di suprema fama,"
                },
                {
                    "verse": "a cui tante virtù gli van dintorno"
                },
                {
                    "verse": "ch'ogni dotto om ne serebbe adorno,"
                },
                {
                    "verse": "secondo che per tutti si diclama,"
                }
            ],
            [
                {
                    "verse": "per la vertù, che germina tuo rama,"
                },
                {
                    "verse": "seremi car di vederti ogni giorno"
                },
                {
                    "verse": "per udir la dottrina del tuo corno,"
                },
                {
                    "verse": "ché altro el mio enteletto più non brama."
                }
            ],
            [
                {
                    "verse": "Ma, poi che or non si può far presente,"
                },
                {
                    "verse": "prego mi scriva quel che più ecceda,"
                },
                {
                    "verse": "o l'uon costante o quel ch'è continente,"
                }
            ],
            [
                {
                    "verse": "e la cagion, a ciò che chiaro el veda;"
                },
                {
                    "verse": "e dimmi grato poi, se sei intendente,"
                },
                {
                    "verse": "e d'ira e di lussuria la più feda."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit000462",
        "author": "Francesco Tedaldi",
        "collection": "Poesie",
        "title": "IX",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "O vir preclaro e di sublime ingegno,"
                },
                {
                    "verse": "Francesco mi mandò un tuo sonetto,"
                },
                {
                    "verse": "di rime e di vocaboli perfetto,"
                },
                {
                    "verse": "adritto a Bastian nostro, d'onor degno."
                }
            ],
            [
                {
                    "verse": "Tu non hai tratto drittamente al segno;"
                },
                {
                    "verse": "volendo immaginar con l'enteletto,"
                },
                {
                    "verse": "debbesi lasciar far cotal efetto"
                },
                {
                    "verse": "a quei che stanno nel celeste regno."
                }
            ],
            [
                {
                    "verse": "Se non sapevi el nome de l'amico,"
                },
                {
                    "verse": "non ti portava nulla, ma dovevi"
                },
                {
                    "verse": "responder al contento de la tema."
                }
            ],
            [
                {
                    "verse": "Sì che da nuovo ti prego e replìco"
                },
                {
                    "verse": "che mi responda come far potevi,"
                },
                {
                    "verse": "ché drieto al senso la ragione scema."
                }
            ]
        ]
    }
]