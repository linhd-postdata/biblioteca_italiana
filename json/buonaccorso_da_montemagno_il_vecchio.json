[
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "1",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Amor con le sue man compuose te,"
                },
                {
                    "verse": "che di sua gentilezza ornata t'ha,"
                },
                {
                    "verse": "con altera bellezza, che ti dà"
                },
                {
                    "verse": "d'amoroso piacere e di merzé,"
                }
            ],
            [
                {
                    "verse": "mostrando in te quanto valore ha in sé,"
                },
                {
                    "verse": "perché ne' tuo begli occhi sempre sta:"
                },
                {
                    "verse": "el qual di gran dolcezza prender fa"
                },
                {
                    "verse": "chi mira quella luce dov'ell' è."
                }
            ],
            [
                {
                    "verse": "In bella giovinezza porti tu"
                },
                {
                    "verse": "adorna leggiadria come appar qui,"
                },
                {
                    "verse": "poi che tu se' ornata da lui sì"
                }
            ],
            [
                {
                    "verse": "et hai perfettamente ogni virtù."
                },
                {
                    "verse": "Così in sulla cima d'amor vo,"
                },
                {
                    "verse": "po' che del tuo amor fasciato so'."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "2",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Dappoi ch'i' persi i fiori e le vïole"
                },
                {
                    "verse": "e 'l bel paese e le vezzose piume"
                },
                {
                    "verse": "e 'l viso adorno pien d'ogni costume,"
                },
                {
                    "verse": "in pianto sto come fortuna vuole,"
                }
            ],
            [
                {
                    "verse": "se già non cangia stil, com'ella suole,"
                },
                {
                    "verse": "per consolar il cor, che si consume"
                },
                {
                    "verse": "in urla, stride et in rabbiose schiume:"
                },
                {
                    "verse": "ché così fa Amor, chi ben lo cole."
                }
            ],
            [
                {
                    "verse": "Ma priego il cielo e dipoi ogni stella,"
                },
                {
                    "verse": "destino invoco, fato o chi far pote"
                },
                {
                    "verse": "o quel che l'arco porta e la faretra,"
                }
            ],
            [
                {
                    "verse": "che mi conduca in servitù di quella"
                },
                {
                    "verse": "che sempre ride con pulite gote,"
                },
                {
                    "verse": "sicché del core ogni dolore ispetra."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "3",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Erano e mia pensier ristretti al core"
                },
                {
                    "verse": "dinanzi a quel che nostre colpe vede,"
                },
                {
                    "verse": "per chieder con disio dolce merzede"
                },
                {
                    "verse": "d'ogni antico mortal commesso errore,"
                }
            ],
            [
                {
                    "verse": "quando colei che 'n compagnia d'Amore"
                },
                {
                    "verse": "sola scolpita in mezzo el cor mi siede"
                },
                {
                    "verse": "apparve agli occhi miei: che, per lor fede,"
                },
                {
                    "verse": "degna mi parve di celeste onore."
                }
            ],
            [
                {
                    "verse": "Qui rinsonava allora uno umil pianto,"
                },
                {
                    "verse": "qui la salute de' beati regni,"
                },
                {
                    "verse": "qui rilucea mia mattutina stella."
                }
            ],
            [
                {
                    "verse": "A lei mi volsi, e se 'l Maestro santo"
                },
                {
                    "verse": "sì leggiadra la fece, or non si sdegni"
                },
                {
                    "verse": "ch'io rimirassi allor cosa sì bella."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "4",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Fuggite, sospir lenti, al tristo core,"
                },
                {
                    "verse": "ch'amando spera e che morir si vede,"
                },
                {
                    "verse": "privo di que' begli occhi, onde merzede"
                },
                {
                    "verse": "non spero più, ché nol consente Amore."
                }
            ],
            [
                {
                    "verse": "E voi, spirti gentil, che in questo errore"
                },
                {
                    "verse": "avete sperïenza usata e fede,"
                },
                {
                    "verse": "piangete meco il mal che mi concede"
                },
                {
                    "verse": "l'avversa mia fortuna a tutte l'ore:"
                }
            ],
            [
                {
                    "verse": "poich'i' son fuor del più dolce disio"
                },
                {
                    "verse": "ch'al mondo ma' disiassi uom terreno"
                },
                {
                    "verse": "per allentar sue pene e suo martìri,"
                }
            ],
            [
                {
                    "verse": "e veggomi in un punto venir meno"
                },
                {
                    "verse": "pien d'ira e sdegno e condurmi al morire"
                },
                {
                    "verse": "e finir la mia vita in un baleno."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "5",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Inclita Maestà felice e santa,"
                },
                {
                    "verse": "ch'è di tua gloria e di tua gran virtute?"
                },
                {
                    "verse": "O disiata sol nostra salute,"
                },
                {
                    "verse": "o sacro Carlo, che sì bella pianta,"
                }
            ],
            [
                {
                    "verse": "fama del tuo bel nome eternal, lassi?"
                },
                {
                    "verse": "Dapoi che 'l cielo in te nostra salute"
                },
                {
                    "verse": "riserbato ha dopo a miseria tanta,"
                },
                {
                    "verse": "circunda omai con gli onorati passi"
                }
            ],
            [
                {
                    "verse": "Italia nostra peregrina intorno,"
                },
                {
                    "verse": "che sol te veder brama."
                },
                {
                    "verse": "Ah, Signor mio, che glorïosa fama"
                },
                {
                    "verse": "ti serba un sacro e benedetto giorno,"
                }
            ],
            [
                {
                    "verse": "se 'l vero el dir poetico distingue,"
                },
                {
                    "verse": "che del tuo nome adorno"
                },
                {
                    "verse": "cantino ancor mille famose lingue!"
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "6",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Lasso, dappoi che per amor tanto arsi,"
                },
                {
                    "verse": "invan gli anni, le notte e' giorni spesi!"
                },
                {
                    "verse": "Invano el ben servir, e quanto, intesi!"
                },
                {
                    "verse": "Invan quante parole e preghi sparsi!"
                }
            ],
            [
                {
                    "verse": "Invano i passi e tutti i sospir farsi"
                },
                {
                    "verse": "veggio! Per uscir me dai lacci tesi,"
                },
                {
                    "verse": "invan sempre chiamai! Invan contesi"
                },
                {
                    "verse": "que' due begli occhi a me sempre sì scarsi!"
                }
            ],
            [
                {
                    "verse": "Invan le rime! Invano ogni mio verso!"
                },
                {
                    "verse": "Invano ogni fatica! Invan si spera!"
                },
                {
                    "verse": "Questo so ben, ma lamentar non giova:"
                }
            ],
            [
                {
                    "verse": "ond'io bramo per morte esser summerso,"
                },
                {
                    "verse": "perché ogni mio ben manca, il mal rinnuova."
                },
                {
                    "verse": "E così va chi serve anima altiera!"
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "7",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Lume, che 'n questo tenebroso orrore"
                },
                {
                    "verse": "fosti scorta al mio corso e fido polo"
                },
                {
                    "verse": "de le tempeste mie, se nudo e solo"
                },
                {
                    "verse": "mi hai qui lasciato, io pur tempro il dolore,"
                }
            ],
            [
                {
                    "verse": "io pur freno il desio di mandar fore"
                },
                {
                    "verse": "quest'alma afflitta, e me stesso consolo"
                },
                {
                    "verse": "che con l'ali d'onor t'alzasti a volo"
                },
                {
                    "verse": "ove non giunse mai penna o valore."
                }
            ],
            [
                {
                    "verse": "Or vedi la miseria de' mortali"
                },
                {
                    "verse": "e ti ridi del mondo e di sue fole"
                },
                {
                    "verse": "e ti pasce di gloria etterna e vera."
                }
            ],
            [
                {
                    "verse": "Ma, mentre miri il Sol che face il sole,"
                },
                {
                    "verse": "piacciati di viarmi in tanti mali"
                },
                {
                    "verse": "e di mostrarmi la tua forma intiera."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "8",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "Non vide unque mai 'l sol, che tutto vede,"
                },
                {
                    "verse": "donna tanto leggiadra e tanto onesta,"
                },
                {
                    "verse": "bella, savia, gentil, né sì modesta"
                },
                {
                    "verse": "quant'è costei d'ogni virtute erede."
                }
            ],
            [
                {
                    "verse": "E se ci fusse chi il mio dir non crede,"
                },
                {
                    "verse": "miri sotto l'ammanto ch'ell'ha 'n testa:"
                },
                {
                    "verse": "vedrà quanto di gloria il Ciel le presta"
                },
                {
                    "verse": "e com' in lei risiede onore e fede:"
                }
            ],
            [
                {
                    "verse": "ch'a 'ntonar le sue laude non è degno"
                },
                {
                    "verse": "spirito uman, perché tant'è suprema"
                },
                {
                    "verse": "che rompe e spezza ogni fiorito ingegno."
                }
            ],
            [
                {
                    "verse": "Giràn li sguardi d'esta Diadema,"
                },
                {
                    "verse": "lo modesto parlare e 'l suo cor degno"
                },
                {
                    "verse": "a tormi l'alma: onde 'l mio cor ne trema."
                }
            ]
        ]
    },
    {
        "url": "https://github.com/linhd-postdata/biblioteca_italiana/blob/master/xml/bibit001423",
        "author": "Buonaccorso da Montemagno il Vecchio",
        "collection": "Rime",
        "title": "9",
        "manually_checked": false,
        "text": [
            [
                {
                    "verse": "S'i' consento al desio che mi molesta,"
                },
                {
                    "verse": "veggo vergogna e duol seguirne insieme:"
                },
                {
                    "verse": "ché ben è folle il nocchier che non teme"
                },
                {
                    "verse": "di salvo porto mettersi in tempesta."
                }
            ],
            [
                {
                    "verse": "Libero uccel gioendo alla foresta,"
                },
                {
                    "verse": "chiuso poi in gabbia, lamentando geme."
                },
                {
                    "verse": "Lasso, io il so ben! Ma me tal forza preme"
                },
                {
                    "verse": "ch'a più saggio di me tolto ha potesta."
                }
            ],
            [
                {
                    "verse": "Or come puossi quel che all'alma piace"
                },
                {
                    "verse": "e vuol, far che disvoglia e che dispiaccia?"
                },
                {
                    "verse": "Quest'è impossibil: dica altri che vuole."
                }
            ],
            [
                {
                    "verse": "Segua adunque che vuol, ch'i' mi dò pace;"
                },
                {
                    "verse": "e son contento pur ch'Amor mi faccia"
                },
                {
                    "verse": "arder da' raggi d'un sì vivo sole."
                }
            ]
        ]
    }
]