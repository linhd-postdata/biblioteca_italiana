[
    {
        "author": "Antonio Calzaiuolo",
        "work": "Poesie",
        "title": "I",
        "structure": "poesia",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000166",
        "text": [
            [
                {
                    "verse": "La volubil fortuna, i cieli e' fati,"
                },
                {
                    "verse": "il ver che molto ha forza e la ragione"
                },
                {
                    "verse": "han fatto far di nuovo un paragone"
                },
                {
                    "verse": "d'un or, ch'\u00e8 ventiquattro o pi\u00f9 carati."
                }
            ],
            [
                {
                    "verse": "Adunque, ingegni divini, almi beati,"
                },
                {
                    "verse": "diventate nel dire ognun precone,"
                },
                {
                    "verse": "porgendo laulde e gran reputazione"
                },
                {
                    "verse": "al nostro Mar\u00efotto Davanzati,"
                }
            ],
            [
                {
                    "verse": "c'ha a\u00f9to appetto un nuovo barberesco"
                },
                {
                    "verse": "veloce molto, e da lui venne il gaggio,"
                },
                {
                    "verse": "nol credendo trovar nel corso in punto;"
                }
            ],
            [
                {
                    "verse": "ma \u00e8 ereda di messer Francesco:"
                },
                {
                    "verse": "gli di\u00e8 le mosse e del terren vantaggio,"
                },
                {
                    "verse": "poi lasci\u00f2 al prato, ed egli al palio \u00e8 giunto."
                }
            ]
        ]
    },
    {
        "author": "Antonio Calzaiuolo",
        "work": "Poesie",
        "title": "II",
        "structure": "poesia",
        "url": "http://www.bibliotecaitaliana.it/testo/bibit000166",
        "text": [
            [
                {
                    "verse": "Muovasi un tigre o qualche caldo leo,"
                },
                {
                    "verse": "che strazi ogni tuo membro e sbrani el core,"
                },
                {
                    "verse": "e Giove fabbricar faccia un furore,"
                },
                {
                    "verse": "che fulmini el tuo corpo iniquo e reo."
                }
            ],
            [
                {
                    "verse": "Sentir post\u00f9 di morsi di Tideo,"
                },
                {
                    "verse": "da poi che se' pi\u00f9 falso traditore"
                },
                {
                    "verse": "che non fu Gano o 'l troiano Anten\u00f2re,"
                },
                {
                    "verse": "o Giuda Antipateo o Tolomeo."
                }
            ],
            [
                {
                    "verse": "Diventare post\u00f9 pi\u00f9 oscuro e magro"
                },
                {
                    "verse": "che 'l popol ch'assedi\u00f2 Vespes\u00efano;"
                },
                {
                    "verse": "faccino i can di te qual d'Ateonne."
                }
            ],
            [
                {
                    "verse": "Arder post\u00f9 com'arse Meleagro,"
                },
                {
                    "verse": "muggir com fece il bu' cicil\u00efano"
                },
                {
                    "verse": "e poi senta del remo di Caronne."
                }
            ],
            [
                {
                    "verse": "O volto di Gorgonne,"
                },
                {
                    "verse": "ruinar post\u00f9 fin nella Giudecca,"
                },
                {
                    "verse": "da poi che 'n te piet\u00e0 \u00e8 rasa e secca!"
                }
            ]
        ]
    }
]